    <validationRules>
        <fullName>VR_FS_MAR2019_EndDate</fullName>
        <active>true</active>
        <description>“End Date” cannot be more than 18 months after “Start Date”</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),
DATE(YEAR(DATEVALUE (End_Date__c)), MONTH((DATEVALUE(End_Date__c))), DAY((DATEVALUE(End_Date__c)))) &gt;
DATE (  /*YEAR*/  YEAR( DATEVALUE (Start_Date__c )) + 1 + FLOOR((MONTH( DATEVALUE (Start_Date__c )) + 6 - 1)/12),  /*MONTH*/  CASE(MOD(MONTH( DATEVALUE (Start_Date__c )) + 6, 12 ), 0, 12, MOD(MONTH( DATEVALUE (Start_Date__c ))+ 6, 12 )),  /*DAY*/  MIN(DAY( DATEVALUE (Start_Date__c )),  CASE(MOD(MONTH( DATEVALUE (Start_Date__c )) + 6,12), 9, 30, 4, 30, 6, 30, 11, 30, 2,  /* return max days for February dependent on if end date is leap year */  IF(MOD(YEAR( DATEVALUE (Start_Date__c )) + FLOOR((MONTH( DATEVALUE (Start_Date__c )) + 6)/12), 400) = 0 || (MOD(YEAR( DATEVALUE (Start_Date__c )) + FLOOR((MONTH( DATEVALUE (Start_Date__c )) + 6)/12), 4) = 0 &amp;&amp; MOD(YEAR( DATEVALUE (Start_Date__c )) + FLOOR((MONTH( DATEVALUE (Start_Date__c )) + 6)/12), 100) &lt;&gt; 0  ), 29,28), 31))  )
	)</errorConditionFormula>
        <errorMessage>“End Date” cannot be more than 18 months after “Start Date”.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Q216_startDateBeforeEndDate</fullName>
        <active>true</active>
        <description>This checks that the start date is always before the end date.</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), Start_Date__c&gt;End_Date__c)</errorConditionFormula>
        <errorMessage>End date cannot be a date before the start date.</errorMessage>
    </validationRules>
