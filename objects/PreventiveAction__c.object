    <validationRules>
        <fullName>PRA_VR01_DuplicatedPreventiveActionRequi</fullName>
        <active>true</active>
        <description>When the status of a Preventive Action moves to Duplicated, referencing the Duplicated Preventive Action is mandatory</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp; 
ISPICKVAL( Status__c , &quot;Duplicate&quot;)  &amp;&amp;  
(LEN( DuplicatedPreventiveAction__c ) = 0)</errorConditionFormula>
        <errorDisplayField>DuplicatedPreventiveAction__c</errorDisplayField>
        <errorMessage>&quot;Duplicated Preventive Action&quot; is mandatory when the Status is set to &quot;Duplicate&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRA_VR02_OwnerFieldMandatory</fullName>
        <active>true</active>
        <description>Owner field is mandatory for Preventive Action</description>
        <errorConditionFormula>AND
(
	NOT($User.BypassVR__c),
	$RecordType.Name !=&apos;Supplier&apos;,
	ISBLANK( Owner__c)
)</errorConditionFormula>
        <errorDisplayField>Owner__c</errorDisplayField>
        <errorMessage>The corresponding Owner for the Preventive Action is not found. Owner must be associated to a Preventive Action</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRA_VR03_Prevent_Supplier_PRA</fullName>
        <active>true</active>
        <description>OCT 2015 Release : Supplier Preventive Action cannot be created on a Schneider Problem.</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),RelatedProblem__r.RecordType.DeveloperName = &apos;Schneider&apos; , RecordType.DeveloperName = &apos;Supplier&apos;)</errorConditionFormula>
        <errorMessage>Supplier Preventive Action cannot be created on a Schneider Problem.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRA_VR_JUN16I2P01_CheckSafeDomain</fullName>
        <active>true</active>
        <description>Check the safe domain in AdditionalContact__c</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
 VLOOKUP($ObjectType.SafeDomains__c.Fields.Name, $ObjectType.SafeDomains__c.Fields.Name,SUBSTITUTE(AdditionalContact__c,left(AdditionalContact__c,FIND(&quot;@&quot;, AdditionalContact__c)), &quot;&quot;))!=SUBSTITUTE(AdditionalContact__c,left(AdditionalContact__c,FIND(&quot;@&quot;, AdditionalContact__c)), &quot;&quot;) )</errorConditionFormula>
        <errorDisplayField>AdditionalContact__c</errorDisplayField>
        <errorMessage>Please enter valid email address within a Schneider safe domain.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRA_VR_OCT15I2P01_DueDateGtStartDate</fullName>
        <active>true</active>
        <description>I2P: OCT15 RELEASE. Due date should not be before Creation Date</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),  IF(ISNEW(),TODAY() &gt; DueDate__c, DATEVALUE( CreatedDate ) &gt; DueDate__c ))</errorConditionFormula>
        <errorDisplayField>DueDate__c</errorDisplayField>
        <errorMessage>Due date should not be before Creation Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRA_VR_OCT15I2P02_PACrifPrblmInPrognPrbl</fullName>
        <active>true</active>
        <description>I2P: OCT 15 Release. Preventive Action can be created only when the related Problem status is In Progress and Problem leader has been Nomiated</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), ISNEW(), NOT( ISPICKVAL( RelatedProblem__r.Status__c , &quot;In Progress&quot;) ),  ISBLANK( RelatedProblem__r.ProblemLeader__c )  )</errorConditionFormula>
        <errorMessage>Preventive Action can only be created when the related Problem Status is In Progress and Schneider Problem Leader is nominated.</errorMessage>
    </validationRules>
