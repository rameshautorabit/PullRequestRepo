    <validationRules>
        <fullName>KWD_VR_JUN16CCC01_AllCategoryForWeb</fullName>
        <active>true</active>
        <description>All Category Case Classification can be assigned only for Keywords, where Channel Type is Web or App</description>
        <errorConditionFormula>AND(
    NOT($User.BypassVR__c),
    CaseCategory__r.Name ==  $Label.CLJUN16CCC01,
    NOT(ISPICKVAL(PoCType__c, &quot;Web&quot;)),
    NOT(ISPICKVAL(PoCType__c, &quot;App&quot;)),NOT(ISPICKVAL(PoCType__c, &quot;Social Media&quot;))
)</errorConditionFormula>
        <errorMessage>Case Category for Type &quot;All Category&quot; can be assigned only for Web or App or Social Media PoC Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>KWD_VR_JUN16CCC02_PoCTypeValidation</fullName>
        <active>true</active>
        <description>PoC Type of Keyword should match with the Channel Type of Point of Contact</description>
        <errorConditionFormula>AND(
NOT($User.BypassVR__c),
TEXT(PoCType__c)  &lt;&gt; TEXT( PointOfContact__r.ChannelType__c)  
)</errorConditionFormula>
        <errorDisplayField>Active__c</errorDisplayField>
        <errorMessage>Keyword&apos;s PoC Type should match with Point of Contact&apos;s Channel Type.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
