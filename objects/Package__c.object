    <validationRules>
        <fullName>PKG_VR01_Q316_TeamMandatryProactiveCases</fullName>
        <active>true</active>
        <description>Customer care team should be populated when active package marked for proactive case creation.</description>
        <errorConditionFormula>AND(
	NOT($User.BypassVR__c),
	AutomaticProactiveCase__c ,
	ISPICKVAL(Status__c,&apos;Active&apos;),
	ISBLANK( ProactiveCaseSupportTeam__c) 
)</errorConditionFormula>
        <errorMessage>Proactive Case Support Team is mandatory for Automatic Proactive Case creation</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PKG_VR02_Q316_CaseOlyForActivePkgs</fullName>
        <active>true</active>
        <description>Proactive cases can be created only for active packages</description>
        <errorConditionFormula>AND(
	NOT($User.BypassVR__c),
        NOT(ISPICKVAL(Status__c, &apos;Active&apos; )),
	AutomaticProactiveCase__c
)</errorConditionFormula>
        <errorMessage>Proactive cases are created for active packages only.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PKG_VR_SEP12CCC01_EndDtLessThanStrtDt</fullName>
        <active>true</active>
        <description>End Date should be greater than Start Date</description>
        <errorConditionFormula>(AND($User.BypassVR__c=False, EndDate__c &lt;  StartDate__c))</errorConditionFormula>
        <errorMessage>End Date should be greater than Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PKG_VR_SEP12CCC02_CantUpdateStatustoExp</fullName>
        <active>true</active>
        <description>Status cannot be Expired as end date is not reached</description>
        <errorConditionFormula>(AND
( $User.BypassVR__c=False,
 ISPICKVAL( Status__c , &apos;Expired&apos;), 
(EndDate__c  &gt;   TODAY() )
 ))</errorConditionFormula>
        <errorMessage>Status cannot be Expired as end date is not reached</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PKG_VR_SEP12CCC03_StrtDtGrtrThanEndDate</fullName>
        <active>true</active>
        <description>Start Date should not be greater than End Date</description>
        <errorConditionFormula>(AND
( ISNEW() ,
$User.BypassVR__c=False, 
StartDate__c  &gt;   EndDate__c ))</errorConditionFormula>
        <errorDisplayField>StartDate__c</errorDisplayField>
        <errorMessage>Start Date cannot be greater than End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PKG_VR_SEP12CCC03_StrtDtGrtrThanToday</fullName>
        <active>false</active>
        <description>Start Date should not be greater than today</description>
        <errorConditionFormula>(AND
( ISNEW() ,
$User.BypassVR__c=False, 
StartDate__c  &gt;  TODAY()))</errorConditionFormula>
        <errorDisplayField>StartDate__c</errorDisplayField>
        <errorMessage>Start Date should not be greater than today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PKG_VR_SEP12CCC04_InactivePKGcantbeadded</fullName>
        <active>false</active>
        <description>Inactive package cannot be added to Contract</description>
        <errorConditionFormula>(AND
( ISNEW() ,
$User.BypassVR__c=False,
ISPICKVAL( Status__c , &apos;Inactive&apos;) ))</errorConditionFormula>
        <errorMessage>Inactive package cannot be added to Contract</errorMessage>
    </validationRules>
