    <validationRules>
        <fullName>OBD_VR01_MandatoryAxis</fullName>
        <active>true</active>
        <description>SR0+ - A validation rule to check whether the &apos;Product&apos; and/or &apos;Country of Destination&apos; fields have a value in.</description>
        <errorConditionFormula>AND ($User.BypassVR__c = False, AND(ISBLANK( Product__c),ISBLANK(CountryofDestination__c)))</errorConditionFormula>
        <errorMessage>Please select a Product Line and/or Country of Destination before saving.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OBD_VR02_PercentageCheck</fullName>
        <active>true</active>
        <description>SR0+ - Validate that all percentages fall within 0 - 100%.</description>
        <errorConditionFormula>AND ($User.BypassVR__c = False, (OR( 
AND(OR( DirectSalePercentageYear__c  &lt; 0.00,DirectSalePercentageYear__c &gt; 1.00), $RecordType.Name = &apos;Yearly&apos;),
AND(OR( DirectSalePercentageQ4__c  &lt; 0.00,DirectSalePercentageQ4__c &gt; 1.00), $RecordType.Name = &apos;Quarterly&apos;),
AND(OR( DirectSalePercentageQ3__c  &lt; 0.00,DirectSalePercentageQ3__c &gt; 1.00), $RecordType.Name = &apos;Quarterly&apos;),
AND(OR( DirectSalePercentageQ2__c  &lt; 0.00,DirectSalePercentageQ2__c &gt; 1.00), $RecordType.Name = &apos;Quarterly&apos;),
AND(OR( DirectSalePercentageQ1__c  &lt; 0.00,DirectSalePercentageQ1__c &gt; 1.00), $RecordType.Name = &apos;Quarterly&apos;),
AND(OR( DirectSalePercentageJan__c  &lt; 0.00,DirectSalePercentageJan__c &gt; 1.00), $RecordType.Name = &apos;Monthly&apos;),
AND(OR( DirectSalePercentageFeb__c  &lt; 0.00,DirectSalePercentageFeb__c &gt; 1.00), $RecordType.Name = &apos;Monthly&apos;),
AND(OR( DirectSalePercentageMar__c  &lt; 0.00,DirectSalePercentageMar__c &gt; 1.00), $RecordType.Name = &apos;Monthly&apos;),
AND(OR( DirectSalePercentageApr__c  &lt; 0.00,DirectSalePercentageApr__c &gt; 1.00), $RecordType.Name = &apos;Monthly&apos;),
AND(OR( DirectSalePercentageMay__c  &lt; 0.00,DirectSalePercentageMay__c &gt; 1.00), $RecordType.Name = &apos;Monthly&apos;),
AND(OR( DirectSalePercentageJun__c  &lt; 0.00,DirectSalePercentageJun__c &gt; 1.00), $RecordType.Name = &apos;Monthly&apos;),
AND(OR( DirectSalePercentageJul__c  &lt; 0.00,DirectSalePercentageJul__c &gt; 1.00), $RecordType.Name = &apos;Monthly&apos;),
AND(OR( DirectSalePercentageAug__c  &lt; 0.00,DirectSalePercentageAug__c &gt; 1.00), $RecordType.Name = &apos;Monthly&apos;),
AND(OR( DirectSalePercentageSep__c  &lt; 0.00,DirectSalePercentageSep__c &gt; 1.00), $RecordType.Name = &apos;Monthly&apos;),
AND(OR( DirectSalePercentageOct__c  &lt; 0.00,DirectSalePercentageOct__c &gt; 1.00), $RecordType.Name = &apos;Monthly&apos;),
AND(OR( DirectSalePercentageNov__c  &lt; 0.00,DirectSalePercentageNov__c &gt; 1.00), $RecordType.Name = &apos;Monthly&apos;),
AND(OR( DirectSalePercentageDec__c  &lt; 0.00,DirectSalePercentageDec__c &gt; 1.00), $RecordType.Name = &apos;Monthly&apos;)
)))</errorConditionFormula>
        <errorMessage>Please ensure that all percentages are between 0 and 100% inclusively.</errorMessage>
    </validationRules>
