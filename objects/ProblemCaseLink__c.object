    <fields>
        <fullName>AccountName__c</fullName>
        <externalId>false</externalId>
        <formula>if(ISBLANK( Case__r.AssetOwner__c),Case__r.Account.Name,if(Case__r.TECH_RelatedAccountStatus__c = &quot;VIP1&quot;,Case__r.AssetOwner__r.Name, 
if(ISPICKVAL(Case__r.Account.Account_VIPFlag__c, &quot;VIP1&quot;),Case__r.Account.Name, 
if(Case__r.TECH_RelatedAccountStatus__c = &quot;VIP4&quot;,Case__r.AssetOwner__r.Name, 
if(ISPICKVAL(Case__r.Account.Account_VIPFlag__c, &quot;VIP4&quot;),Case__r.Account.Name, 
if(Case__r.TECH_RelatedAccountStatus__c = &quot;VIP5&quot;,Case__r.AssetOwner__r.Name,if(ISPICKVAL(Case__r.Account.Account_VIPFlag__c, &quot;VIP5&quot;),Case__r.Account.Name,Case__r.AssetOwner__r.Name)))))))</formula>
        <label>Account Name</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Case__c</fullName>
        <externalId>false</externalId>
        <label>Case</label>
        <referenceTo>Case</referenceTo>
        <relationshipLabel>Related Problems</relationshipLabel>
        <relationshipName>Problem_Case_Links</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>true</writeRequiresMasterRead>
    </fields>
    <fields>
