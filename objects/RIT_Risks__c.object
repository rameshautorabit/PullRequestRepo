    <validationRules>
        <fullName>RSK_VR01_RiskToBeFilled</fullName>
        <active>true</active>
        <description>Quote Approval – Risk ID to be Filled</description>
        <errorConditionFormula>NOT($User.BypassVR__c) 
&amp;&amp; 
(OR(AND(  $RecordType.DeveloperName = &apos;RSK_FinancialInformation&apos;,  FinancialRisks__c=null) , AND( $RecordType.DeveloperName= &apos;RSK_ProjectManagement&apos;, ProjectManagementRisks__c=null),AND( $RecordType.DeveloperName= &apos;RSK_TechnicalManufacturingScope&apos;, TechnicalManufacturingRisks__c=null),AND( $RecordType.DeveloperName= &apos;RSK_SubContractorsOGSuppliers&apos;, SubContractorsAndOGSuppRisks__c=null),AND( $RecordType.DeveloperName= &apos;RSK_CommercialandCompetitiveEnv&apos;, CommercialAndCompetitiveEnvRisks__c=null),AND( $RecordType.DeveloperName = &apos;RSK_ContractualEnvironment&apos;, ContractualEnvironmentalRisks__c=null)))</errorConditionFormula>
        <errorMessage>Risks cannot be null</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RSK_VR02_CheckQuoteStatus</fullName>
        <active>true</active>
        <description>Quote Approval - To ensure that information is not edited after Quote Approval</description>
        <errorConditionFormula>NOT($User.BypassVR__c) 
&amp;&amp; 
(Text(QuoteLink__r.QuoteStatus__c) = &apos;Approved&apos;)</errorConditionFormula>
        <errorMessage>RITE Information cannot be modified now, as the Quote is Approved</errorMessage>
    </validationRules>
