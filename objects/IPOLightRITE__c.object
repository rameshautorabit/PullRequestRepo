    <validationRules>
        <fullName>LRT_VR_APR14SLS02_IndirectDamages</fullName>
        <active>true</active>
        <description>Validates that Mitigation Action Plan should be filled, if &apos;NO&apos; is selected</description>
        <errorConditionFormula>NOT($User.BypassVR__c) 
&amp;&amp; 
(AND(ISPICKVAL(ExclusionOfDamages__c,&apos;No&apos;), ISBLANK(ExclusionOfDamagesPlan__c)))</errorConditionFormula>
        <errorDisplayField>ExclusionOfDamages__c</errorDisplayField>
        <errorMessage>Please fill the Mitigation Action Plan for &apos;1. Is there an exclusion of Consequential or Indirect Damages (Loss of Profit / Data/ Use / Business and Production) from our liabilities?&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LRT_VR_APR14SLS03_DamageLimited</fullName>
        <active>true</active>
        <description>Validates that Mitigation Action Plan should be filled, if &apos;NO&apos; is selected</description>
        <errorConditionFormula>NOT($User.BypassVR__c) 
&amp;&amp; 
(AND(ISPICKVAL(DamageLimited__c,&apos;No&apos;), ISBLANK(DamageLimitedPlan__c)))</errorConditionFormula>
        <errorMessage>Please fill the Mitigation Action Plan of &apos;2. Is SE liability in terms of direct damages limited to a maximum of 100% of Contract?&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LRT_VR_APR14SLS04_LegalEntity</fullName>
        <active>true</active>
        <description>Validates that Mitigation Action Plan should be filled, if &apos;YES&apos; is selected</description>
        <errorConditionFormula>NOT($User.BypassVR__c) 
&amp;&amp; 
(AND(ISPICKVAL(LegalEntity__c,&apos;Yes&apos;), ISBLANK(LegalEntityPlan__c)))</errorConditionFormula>
        <errorMessage>Please fill the Mitigation Action Plan for &apos;3. Is a new legal entity  required as part of the project? (e.g.: Special Purpose Vehicle or JV)&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LRT_VR_APR14SLS05_ParentCompany</fullName>
        <active>true</active>
        <description>Validates that Mitigation Action Plan should be filled, if &apos;NO&apos; is selected</description>
        <errorConditionFormula>NOT($User.BypassVR__c) 
&amp;&amp; 
(AND(ISPICKVAL(ParentCompany__c,&apos;Yes&apos;), ISBLANK(ParentCompanyPlan__c)))</errorConditionFormula>
        <errorMessage>Please fill the Mitigation Action Plan for &apos;4. Is a Parent Company Guarantee required from either Schneider Electric SA or from SEI sas?&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LRT_VR_APR14SLS23_ValidateQuoteStatus</fullName>
        <active>true</active>
        <description>Checks if the Quote is approved</description>
        <errorConditionFormula>NOT($User.BypassVR__c) 
&amp;&amp; 
(Text(QuoteLink__r.QuoteStatus__c) = &apos;Approved&apos;)</errorConditionFormula>
        <errorMessage>Light RITE Information cannot be modified as the Quote is Approved.</errorMessage>
    </validationRules>
