    <fields>
        <fullName>CCOEbitPercent__c</fullName>
        <description>Quote Approval - Formula in order to calculate the Percentage value for CCOEbit against Total Sales Value.</description>
        <externalId>false</externalId>
        <formula>IF(Sales_RP__c !=0, ( CCOEbit__c  /  Sales_RP__c  ) , 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>CCO Commercial Margin %</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
