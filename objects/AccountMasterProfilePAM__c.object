    <validationRules>
        <fullName>AMPPAM_VR_Q316SLS01_ValidateNegValues</fullName>
        <active>true</active>
        <errorConditionFormula>NOT($User.BypassVR__c) 
&amp;&amp; 
( PAM__c &lt; 0 ||  DirectSales__c &lt; 0 ||  IndirectSales__c &lt; 0)</errorConditionFormula>
        <errorMessage>Please enter non-negative number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AMPPAM_VR_Q316SLS02_SalesValuesValidate</fullName>
        <active>true</active>
        <errorConditionFormula>NOT($User.BypassVR__c) 
&amp;&amp; 
PAM__c &lt; (FLOOR(DirectSales__c) + FLOOR(IndirectSales__c) )</errorConditionFormula>
        <errorMessage>BU PAM should be higher or equal to BU Direct Sales + BU Indirect Sales</errorMessage>
    </validationRules>
