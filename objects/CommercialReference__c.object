        <description>Mar 2016 Release</description>
        <externalId>false</externalId>
        <formula>IF(NonStandardDatecode__c != null || NonStandardSerialN__c != null, $Label.CLMAR16I2P07 ,(IF(NOT(ISNULL(StartDatecode__c)), &apos;Plant&apos; &amp; &apos; &apos; &amp; Plantcode__c &amp; &apos; &apos; &amp; $Label.CLMAR16I2P05 &amp; &apos; &apos; &amp; IF(LEN(TEXT(DAY(StartDatecode__c))) == 1 ,&apos;0&apos; &amp; TEXT(DAY(StartDatecode__c)),TEXT(DAY(StartDatecode__c))) &amp; &apos;-&apos; &amp; CASE(MONTH(StartDatecode__c),1, &quot;JAN&quot;,2, &quot;FEB&quot;,3, &quot;MAR&quot;,4, &quot;APR&quot;,5, &quot;MAY&quot;,6, &quot;JUN&quot;,7, &quot;JUL&quot;,8, &quot;AUG&quot;,9, &quot;SEPT&quot;,10, &quot;OCT&quot;,11, &quot;NOV&quot;,12, &quot;DEC&quot;,&quot;None&quot;)
&amp; &apos;-&apos; &amp; TEXT(YEAR(StartDatecode__c)) ,&apos;&apos;) &amp; IF(NOT(ISNULL(EndDatecode__c)), &apos; &apos; &amp; $Label.CLMAR16I2P06 &amp; &apos; &apos; &amp; &apos; &apos; &amp; IF(LEN(TEXT(DAY(EndDatecode__c))) == 1, &apos;0&apos; &amp; TEXT(DAY(EndDatecode__c)),TEXT(DAY(EndDatecode__c))) &amp; &apos;-&apos; &amp; CASE(MONTH(EndDatecode__c),1, &quot;JAN&quot;,2, &quot;FEB&quot;,3, &quot;MAR&quot;,4, &quot;APR&quot;,5, &quot;MAY&quot;,6, &quot;JUN&quot;,7, &quot;JUL&quot;,8, &quot;AUG&quot;,9, &quot;SEPT&quot;,10, &quot;OCT&quot;,11, &quot;NOV&quot;,12, &quot;DEC&quot;,&quot;None&quot;) &amp; &apos;-&apos; &amp; TEXT(YEAR(EndDatecode__c)) ,&apos;&apos;) &amp; IF(InitialSerialN__c != null, &apos; &apos; &amp; $Label.CLMAR16I2P05 &amp; &apos; &apos; &amp; $Label.CLMAR16I2P08 &amp; &apos; &apos; &amp; InitialSerialN__c, &apos;&apos;) &amp; IF(LastSerialN__c != null, &apos; &apos; &amp; $Label.CLMAR16I2P06 &amp; &apos; &apos; &amp; $Label.CLMAR16I2P08 &amp; &apos; &apos; &amp; LastSerialN__c, &apos;&apos;)))</formula>
