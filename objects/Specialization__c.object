    <validationRules>
        <fullName>SPE_VR_OCT13PRM31_CheckUserAccess</fullName>
        <active>true</active>
        <description>To check that only Program Owners/Program Approver/Program Administrator can create/edit global specializations</description>
        <errorConditionFormula>AND($User.BypassVR__c=False, (IF(
AND(
(
NOT
(OR(
$User.ProgramAdministrator__c ,
$User.ProgramApprover__c ,
$User.ProgramOwner__c 
))),
RecordType.Id = &apos;012K00000004dMB&apos; ),
true,
false)))</errorConditionFormula>
        <errorMessage>Only Program Owners / Process Owners can create/edit global specializations.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPE_VR_OCT13PRM32_CheckGlobalSPEStatus</fullName>
        <active>true</active>
        <description>Allow user to create Country Specialization only when Global Specialization is active</description>
        <errorConditionFormula>AND($User.BypassVR__c=False, ISNEW() , (IF(
AND(OR(RecordTypeId = &apos;012K00000004dML&apos; ,RecordTypeId = &apos;012K00000004dMG&apos;),
NOT( 
ISPICKVAL( GlobalSpecialization__r.Status__c  , &apos;Active&apos;))), true,false
)))</errorConditionFormula>
        <errorMessage>Country Specialization cannot be created as the Global Specialization is not Active</errorMessage>
    </validationRules>
