        <externalId>false</externalId>
        <formula>IF ( ISNULL( TECHDeferredStart__c ) , 0
    , IF (ISNULL(TECHDeferredEnd__c)
         , TODAY() - TECHDeferredStart__c 
         , TECHDeferredEnd__c - TECHDeferredStart__c 
         ) +1
     )</formula>
