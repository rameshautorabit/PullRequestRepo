    <validationRules>
        <fullName>IDE_VR01_MandatoryReasonForRejection</fullName>
        <active>true</active>
        <description>When the Status is Rejectedn the Reason for Rejection field becomes mandatory.</description>
        <errorConditionFormula>NOT($User.BypassVR__c)
&amp;&amp; 
( ISPICKVAL( Status , &apos;Rejected&apos;) || ISPICKVAL( Status , &apos;Closed&apos;) )
&amp;&amp; 
ISBLANK( Reason_for_rejection__c)</errorConditionFormula>
        <errorMessage>Please provide reasons that explain the rejection or the closure of this Idea.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IDE_VR02_DoNotChangeCommunity</fullName>
        <active>true</active>
        <description>Prevent user from updating the Community (only bridge Front Office should be selected).
This VR should be deactivated as soon as the communities will be merged.</description>
        <errorConditionFormula>NOT($User.BypassVR__c)
&amp;&amp;  BEGINS(  Community.Name , &quot;XX&quot;)</errorConditionFormula>
        <errorMessage>Please do not select this Community.
You can specify the Process this Idea is related to by selecting the Category instead.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IDE_VR03_Technical_Complexity_required</fullName>
        <active>true</active>
        <description>BFOS-3689, Technical Complexity should be mandatory field prior to moving to “Qualified for Change Board”</description>
        <errorConditionFormula>AND(ISPICKVAL( Status , &apos;Qualified for Change Board&apos;), ISBLANK(TEXT( TechnicalComplexity__c )))</errorConditionFormula>
        <errorMessage>Technical Complexity should be mandatory prior moving the Status to “Qualified for Change Board”</errorMessage>
    </validationRules>
