    <validationRules>
        <fullName>FieloCH__FillMemberOrTeam</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISBLANK(FieloCH__Member__c), ISBLANK(FieloCH__Team__c) )</errorConditionFormula>
        <errorMessage>You must choose either a Member or a Team</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Q416PRM_ChallengeModeCompetition</fullName>
        <active>true</active>
        <errorConditionFormula>IF(NOT($User.BypassVR__c),IF(ISPICKVAL(FieloCH__Challenge2__r.FieloCH__Mode__c,&apos;Competition&apos;), IF(OR(ISBLANK(FieloCH__Member__c),Not(ISBLANK(FieloCH__Team__c))),true,false) ,false),false)</errorConditionFormula>
        <errorMessage>This is a Challenge of mode Competition and requires a Member entry, not a Team.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Q416PRM_GrpAccTeamEnrolledToChallenge</fullName>
        <active>true</active>
        <errorConditionFormula>IF(NOT($User.BypassVR__c),IF(ISPICKVAL(FieloCH__Challenge2__r.FieloCH__Mode__c,&apos;Teams&apos;), 
IF((NOT(FieloCH__Challenge2__r.GroupAccountChallenge__c) &amp;&amp; FieloCH__Team__r.GroupAccountTeam__c) || NOT(ISBLANK(FieloCH__Member__c)) || ISBLANK(FieloCH__Team__c),True,False),False) ,false)</errorConditionFormula>
        <errorMessage>This is a Team Challenge and requires a Team entry, not a Member &amp; Group account teams can be enrolled only to  challenges that are enabled for group accounts.</errorMessage>
    </validationRules>
