    </searchLayouts>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>PLC_VR01_OtherCompetitorMandatory</fullName>
        <active>true</active>
        <description>DECEMBER RELEASE - When Competitor is Selected as Other, User has to provide the Competitor Name, Otherwise Other Name field should be blank.</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), OR(AND(CONTAINS(LOWER(Competitor__r.Name), LOWER($Label.CL00519) ), ISBLANK(OtherName__c)), AND(NOT(CONTAINS(LOWER( Competitor__r.Name ), LOWER($Label.CL00519) )), NOT(ISBLANK(OtherName__c)))))</errorConditionFormula>
        <errorDisplayField>OtherName__c</errorDisplayField>
        <errorMessage>Other Competitor Name should be populated only if the competitor is selected as Others.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
