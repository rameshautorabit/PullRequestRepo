    <fields>
        <fullName>CompetitorMarketShare__c</fullName>
        <description>December Release - To store the market share of competitor for the related product line.</description>
        <externalId>false</externalId>
        <label>Competitor Market Share (%)</label>
        <precision>18</precision>
        <required>true</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Competitor__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>December Release - To associate a competitor to a product line.</description>
        <externalId>false</externalId>
        <label>Competitor</label>
        <referenceTo>Competitor__c</referenceTo>
        <relationshipLabel>Product Line Competitors</relationshipLabel>
        <relationshipName>ProductLineCompetitors</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>OtherName__c</fullName>
        <description>December Release :- Field for Other Product Line Competitor Name.</description>
        <externalId>false</externalId>
        <label>Other Name</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>PLCID__c</fullName>
        <description>December Release - To associate a competitor to a product line</description>
        <externalId>false</externalId>
        <label>PAM and Competition Id</label>
        <referenceTo>PAMandCompetition__c</referenceTo>
        <relationshipLabel>Product Line Competitors</relationshipLabel>
        <relationshipName>ProductLineCompetitors</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>TECH_UniqueCompetitor__c</fullName>
        <caseSensitive>true</caseSensitive>
        <description>DECEMBER RELEASE - Unique field to restrict user from Saving the same Competitor more than 1 time.</description>
        <externalId>true</externalId>
        <label>TECH_Unique Competitor</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <label>Product Line Competitor</label>
    <nameField>
        <displayFormat>PLC -{0000}</displayFormat>
        <label>Product Line Competitor Id</label>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Product Line Competitors</pluralLabel>
    <searchLayouts>
        <lookupDialogsAdditionalFields>Competitor__c</lookupDialogsAdditionalFields>
        <lookupDialogsAdditionalFields>CompetitorMarketShare__c</lookupDialogsAdditionalFields>
        <lookupDialogsAdditionalFields>PLCID__c</lookupDialogsAdditionalFields>
        <lookupPhoneDialogsAdditionalFields>Competitor__c</lookupPhoneDialogsAdditionalFields>
