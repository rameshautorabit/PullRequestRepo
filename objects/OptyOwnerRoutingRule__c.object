    <pluralLabel>OptyOwnerRoutingRules</pluralLabel>
    <searchLayouts/>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>PostalCodeMandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),AND( ISPICKVAL(RoutingRule__c , &apos;Postalcode&apos;) , OR(ZipCodeMax__c = null,ZipCodeMax__c =&apos;&apos;, ZipCodeMin__c =null, ZipCodeMin__c =&apos;&apos;)))</errorConditionFormula>
        <errorMessage>For Routing Rule = Postal Code, Zip Min and Zip Max fields are Mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_ActiveUserCheckOn_Opty_Owner_Routing</fullName>
        <active>true</active>
        <errorConditionFormula>Person__r.IsActive=false</errorConditionFormula>
        <errorDisplayField>Person__c</errorDisplayField>
        <errorMessage>The person cannot be  inactive ,please choose an active user.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_JUL16_Person</fullName>
        <active>true</active>
        <description>For Postal &amp; Geographical routing rules, the Person field cannot be blank</description>
        <errorConditionFormula>AND(NOT( $User.BypassVR__c ), AND( OR( ISPICKVAL( RoutingRule__c , &apos;Geographic&apos;) , ISPICKVAL( RoutingRule__c , &apos;Postalcode&apos;)) ,  OR(Person__c  = null,Person__c  = &apos; &apos;) ))</errorConditionFormula>
        <errorMessage>For Postal &amp; Geographical routing rules, the Person field cannot be blank</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
