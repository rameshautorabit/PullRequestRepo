    <validationRules>
        <fullName>PRR_VR_MAY13PRM01_CheckGlobalReqStatus</fullName>
        <active>true</active>
        <description>Please activate the Parent Level and then try again</description>
        <errorConditionFormula>AND($User.BypassVR__c=False,(IF( 
AND(
 GlobalProgramRequirement__c  != null ,
 GlobalProgramRequirement__r.Active__c = false,
 ISCHANGED(Active__c) ,
 Active__c = true),true,false)))</errorConditionFormula>
        <errorMessage>Please activate the Parent Requirement at Global Level and then try again</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRR_VR_MAY13PRM02_CheckUserAccess</fullName>
        <active>true</active>
        <description>To check that only Program Owners can create/edit global program requirements</description>
        <errorConditionFormula>AND($User.BypassVR__c=False,(IF(
AND(
(
NOT
(OR( 
$User.ProgramOwner__c,
$User.ProgramAdministrator__c 
))),
 PartnerProgram__r.RecordType.Id =  $Label.CLMAY13PRM15),
true,
false)))</errorConditionFormula>
        <errorMessage>Only Program Owners/Program Administrators  can create/edit global program requirement.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRR_VR_MAY13PRM03_CheckMandatoryReq</fullName>
        <active>true</active>
        <description>For PRM May 13 Release
To restrict users to update the Req to inactive when the Req is Manadatory</description>
        <errorConditionFormula>AND($User.BypassVR__c=False,(IF( AND(
Mandatory__c =&apos;Yes&apos;,
Active__c = false,NOT(ISNEW()) ,
OR(
PartnerProgram__r.RecordType.Id = $Label.CLMAY13PRM05,PartnerProgram__r.RecordType.Id = $Label.CLJUN13PRM09),
NOT(OR( $User.ProgramAdministrator__c , $User.ProgramOwner__c ))
),true,false)))</errorConditionFormula>
        <errorMessage>Mandatory Requirement cannot be inactive. Please update the requirement to active</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRR_VR_MAY13PRM03_CheckUserAccess</fullName>
        <active>true</active>
        <description>Requirement can be activated only if the parent level is active.</description>
        <errorConditionFormula>AND($User.BypassVR__c=False,(IF(
AND(
 ProgramLevel__c  != null ,
NOT( ISPICKVAL(  ProgramLevel__r.LevelStatus__c  , &apos;Active&apos;) ),
 Active__c =true),true,false)))</errorConditionFormula>
        <errorMessage>Please activate the related Program Level and then try again</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRR_VR_MAY13PRM04_CantChangeLevelPrgReq</fullName>
        <active>true</active>
        <description>For PRM May 13 Release
Program Level,Partner PRogram and Requirement Catalog cannot be updated</description>
        <errorConditionFormula>AND($User.BypassVR__c=False,(IF(
OR(
 ISCHANGED( ProgramLevel__c ) ,
 ISCHANGED(  RequirementCatalog__c  ) 

),
true,false
)))</errorConditionFormula>
        <errorMessage>Program Level or Requirement Catalog cannot be updated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRR_VR_MAY13PRM05_SameRecordType</fullName>
        <active>true</active>
        <errorConditionFormula>NOT($User.BypassVR__c )  &amp;&amp;
(
(  RecordTypeId == &apos;012K00000008kTb&apos;  &amp;&amp;   RequirementCatalog__r.RecordTypeId &lt;&gt; &apos;012K00000008kTg&apos;)  ||
(  RecordTypeId == &apos;012K00000008kTc&apos;  &amp;&amp;  RequirementCatalog__r.RecordTypeId&lt;&gt; &apos;012K00000008kTh&apos;)  ||
(  RecordTypeId == &apos;012K00000008kTd&apos;  &amp;&amp;  RequirementCatalog__r.RecordTypeId&lt;&gt; &apos;012K00000008kTi&apos;)   
)</errorConditionFormula>
        <errorDisplayField>RequirementCatalog__c</errorDisplayField>
        <errorMessage>Please select  Requirement of same type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRR_VR_OCT14PRM01_CheckGlobalProgramOwnr</fullName>
        <active>true</active>
        <description>To check whether the user is global program owner.</description>
        <errorConditionFormula>AND($User.BypassVR__c=False,(IF( 
AND( 
( 
NOT 
(OR( 
$User.ProgramAdministrator__c , 
$User.ProgramApprover__c , 
AND($User.ProgramOwner__c, 
$User.Id = PartnerProgram__r.OwnerId) 
))), 
PartnerProgram__r.RecordTypeId = $Label.CLMAY13PRM15), 
true, 
false)))</errorConditionFormula>
        <errorMessage>Only Global Program owners can create/edit global program Requirements.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
