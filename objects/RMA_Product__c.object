    <validationRules>
        <fullName>RI_VR01_QuantityLessThanOne</fullName>
        <active>true</active>
        <description>2012R4 - This Validation rule is to restrict the user to enter the quantity as less than one.</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
         Quantity__c &lt; 1)</errorConditionFormula>
        <errorDisplayField>Quantity__c</errorDisplayField>
        <errorMessage>Quantity should not be less than one.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RI_VR_APR14RR01_RequestTypeCheck</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),RMA__r.TECH_RequestType__c =&apos;Repair&apos;, RMA__r.TECH_RequestType__c!=TECH_RequestType__c )</errorConditionFormula>
        <errorMessage>This Return Request only accepts items for repair. Please create a new Return Request for other resolution types.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RI_VR_APR14RR02_BillingMethodNullCheck</fullName>
        <active>true</active>
        <description>Billing Method Null Check for Out of warranty</description>
        <errorConditionFormula>AND( 
NOT($User.BypassVR__c) , 
if( AND(ISPICKVAL(WarrantyStatus__c, &apos;Out of warranty&apos;), ISPICKVAL(BillingMethod__c,&apos;&apos;)),true,false))</errorConditionFormula>
        <errorDisplayField>BillingMethod__c</errorDisplayField>
        <errorMessage>Billing Method is required when the Warranty Status is Out of warranty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RI_VR_APR14RR02_RequestTypeCheckReturn</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),RMA__r.TECH_RequestType__c =&apos;Return&apos;, RMA__r.TECH_RequestType__c!=TECH_RequestType__c )</errorConditionFormula>
        <errorMessage>This Return Request does not accept items for repair. Please create a new Return Request for other resolution types.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RI_VR_APR15I2P01_SympotomCheckforDefect</fullName>
        <active>true</active>
        <description>RI_VR_APR15I2P01_SympotomCheckforDefect</description>
        <errorConditionFormula>IF(
	ISNEW(),
	And(
		NOT($User.BypassVR__c),
		ISPICKVAL(ProductCondition__c, &apos;Defective&apos;), 
		RMA__r.Case__r.CaseSymptom__c=&apos;&apos; , RMA__r.Case__r.CaseSubSymptom__c =&apos;&apos;
	),
	false
	)</errorConditionFormula>
        <errorMessage>The symptom and subsymptom on Case are mandatory in order to create a Return Item with Product Condition = Defective.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RI_VR_DEC12RMA01_DisallowEditShippingAdd</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
	NOT($User.BypassVR__c),
Not( ISNEW() ),
NOT(ISBLANK(PRIORVALUE(ReturnCenter__c ))),
	Other__c=FALSE,
	OR
	(
		ISCHANGED( ShippingStreet__c ),ISCHANGED(ShippingAdditionalAddress__c),ISCHANGED(ShippingCity__c),
		ISCHANGED(ShippingCountry__c),ISCHANGED(ShippingCounty__c),
		ISCHANGED(ShippingPOBox__c),ISCHANGED(ShippingPOBoxZip__c),
		ISCHANGED(ShippingStateProvince__c),ISCHANGED(ShippingZipCode__c),
		ISCHANGED(ShippingLocalCity__c),ISCHANGED(ShippingLocalAdditionalAddress__c),ISCHANGED(ShippingLocalCounty__c),ISCHANGED(ShippingStreetLocalLang__c),
		ISCHANGED(ShippingContactName__c),ISCHANGED(ShippingFax__c),ISCHANGED(ShippingPhone__c)

	)

)</errorConditionFormula>
        <errorMessage>Manual Address Override needs to be selected in order to change the return center address.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RI_VR_DEC12RMA02_DoNotChangeProdCond</fullName>
        <active>true</active>
        <errorConditionFormula>NOT($User.BypassVR__c)&amp;&amp; 
 Text(RMA__r.ProductCondition__c) != Text( ProductCondition__c)</errorConditionFormula>
        <errorMessage>Return Item should have same Product Condition as Return Request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RI_VR_DEC12RMA05_NotAllowUpdtOther</fullName>
        <active>true</active>
        <description>I2P-RR: DEC12RELEASE - If Manual Address Override is checked not allow to uncheck it.</description>
        <errorConditionFormula>AND 
( 
NOT($User.BypassVR__c) , 
NOT( ISNEW() ),
PRIORVALUE(Other__c)=True,
Other__c=False
)</errorConditionFormula>
        <errorMessage>Force Manual Selection cannot be unchecked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RI_VR_DEC12RMA06_NotAllowUpdteCommercial</fullName>
        <active>true</active>
        <description>I2P December 2012
I2P Oct 2014 - Changed the Error message as the label is changed from commercial reference to Product</description>
        <errorConditionFormula>AND
(
NOT($User.BypassVR__c),
Not( ISNEW() ),
 OR(ISCHANGED( Name ) ,ISCHANGED( CommercialReference__c ) ,ISCHANGED( Family__c ))
)</errorConditionFormula>
        <errorMessage>Product cannot be changed once added . Please delete this record and create a new Return Item.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RI_VR_JUN16I2P01_CheckSafeDomain</fullName>
        <active>true</active>
        <description>Check safe domain in ContactEmail__c</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), VLOOKUP($ObjectType.SafeDomains__c.Fields.Name, $ObjectType.SafeDomains__c.Fields.Name,SUBSTITUTE(ContactEmail__c,left(ContactEmail__c,FIND(&quot;@&quot;, ContactEmail__c)), &quot;&quot;))!=SUBSTITUTE(ContactEmail__c,left(ContactEmail__c,FIND(&quot;@&quot;, ContactEmail__c)), &quot;&quot;))</errorConditionFormula>
        <errorDisplayField>ContactEmail__c</errorDisplayField>
        <errorMessage>Please enter valid email address within a Schneider safe domain.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RI_VR_MAY13RR01_RprStatusEqualsAwtngFwd</fullName>
        <active>true</active>
        <description>RI_VR_MAY13RR01_RprStatusEqualsAwtngFwdRtnCntr</description>
        <errorConditionFormula>AND
(
NOT($User.BypassVR__c),
      (ISPICKVAL( CustomerResolution__c, &apos;REP&apos;))
,
 ISNULL( ReceivedbyLocalLogisticsCenter__c),
 ISPICKVAL( LogisticsStatus__c, &apos;Awaiting forward to return Center *&apos;) 
)</errorConditionFormula>
        <errorDisplayField>ReceivedbyLocalLogisticsCenter__c</errorDisplayField>
        <errorMessage>Received by local logistic center date needs to be filled in.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RI_VR_MAY13RR04_ForwarderToRC</fullName>
        <active>true</active>
        <description>I2P RR May 2013 Release</description>
        <errorConditionFormula>AND
(
NOT($User.BypassVR__c),
(ISPICKVAL( CustomerResolution__c, &apos;REP&apos;))
,
 ISNULL( ForwardedToReturnCenter__c),
 ISPICKVAL( LogisticsStatus__c, &apos;In transit to return Center&apos;),
NOT(ISNULL(ReceivedbyLocalLogisticsCenter__c))
)</errorConditionFormula>
        <errorDisplayField>ForwardedToReturnCenter__c</errorDisplayField>
        <errorMessage>Please specify the date on transit to return center</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RI_VR_OCT13RR01_ExpertCenterCheck</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c) , 
NOT( ISNEW() ),PRIORVALUE(Other__c)==False,Other__c==True, (PRIORVALUE(ReturnCenter__c)==ReturnCenter__c))</errorConditionFormula>
        <errorDisplayField>ReturnCenter__c</errorDisplayField>
        <errorMessage>Return Center needs to be changed when Force Manual Selection is selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RI_VR_OCT13RR02_ExpertCenterBlankCheck</fullName>
