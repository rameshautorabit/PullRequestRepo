        <fullName>Category__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ProgramFeature__c != null,ProgramFeature__r.Category__c,
IF(FeatureCatalog__c != null , IF(INCLUDES(FeatureCatalog__r.Category__c, &quot;Content&quot;),&quot;Content;&quot;,NULL)+
IF(INCLUDES(FeatureCatalog__r.Category__c, &quot;Opportunity Registration&quot;),&quot;Opportunity Registration;&quot;,NULL)+
IF(INCLUDES(FeatureCatalog__r.Category__c, &quot;Demo&quot;),&quot;Demo;&quot;,NULL)+
IF(INCLUDES(FeatureCatalog__r.Category__c, &quot;Support&quot;),&quot;Support;&quot;,NULL)+
IF(INCLUDES(FeatureCatalog__r.Category__c, &quot;Loyalty&quot;),&quot;Loyalty;&quot;,NULL)+
IF(INCLUDES(FeatureCatalog__r.Category__c, &quot;Marketing Tools &amp; Materials&quot;),&quot;Marketing Tools &amp; Materials;&quot;,NULL)+
IF(INCLUDES(FeatureCatalog__r.Category__c, &quot;MDF Partner Locator&quot;),&quot;MDF Partner Locator;&quot;,NULL)+
IF(INCLUDES(FeatureCatalog__r.Category__c, &quot;Program Eligibility&quot;),&quot;Program Eligibility;&quot;,NULL)+
IF(INCLUDES(FeatureCatalog__r.Category__c, &quot;Training&quot;),&quot;Training;&quot;,NULL)+
IF(INCLUDES(FeatureCatalog__r.Category__c, &quot;Pricing&quot;),&quot;Pricing;&quot;,NULL)+
IF(INCLUDES(FeatureCatalog__r.Category__c, &quot;Discounts/Rebates&quot;),&quot;Discounts/Rebates;&quot;,NULL)+
IF(INCLUDES(FeatureCatalog__r.Category__c, &quot;Events&quot;),&quot;Events;&quot;,NULL)+
IF(INCLUDES(FeatureCatalog__r.Category__c, &quot;Simple ORF&quot;),&quot;Simple ORF;&quot;,NULL) +
IF(INCLUDES(FeatureCatalog__r.Category__c, &quot;Lead Management&quot;),&quot;Lead Management;&quot;,NULL) +
IF(INCLUDES(FeatureCatalog__r.Category__c, &quot;Passed to Partner Opportunities&quot;),&quot;Passed to Partner Opportunities;&quot;,NULL)+
IF(INCLUDES(FeatureCatalog__r.Category__c, &quot;Service Opportunity&quot;),&quot;Service Opportunity;&quot;,NULL)
,&apos;&apos;)
)</formula>
        <label>Category</label>
        <required>false</required>
