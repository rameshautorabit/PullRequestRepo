    <validationRules>
        <fullName>VCP_VR02_InformationConsistency</fullName>
        <active>true</active>
        <description>SR0 - When contact role or account role are filling first, this VR checks that Contact or Account are entered.</description>
        <errorConditionFormula>NOT($User.BypassVR__c) 
&amp;&amp;(
(
NOT(ISBLANK(TEXT( AccountRole__c ))) 
&amp;&amp;  ISBLANK(Account__c)
 )
||
(
NOT(ISBLANK(TEXT(  ContactRole__c  ))) 
&amp;&amp;  ISBLANK( Contact__c )
)
|| 
(
ISBLANK( Contact__c ) &amp;&amp; ISBLANK(Account__c) 
)
)</errorConditionFormula>
        <errorMessage>Please enter either an Account or a Contact with an associated role.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VCP_VR03_AccountControl</fullName>
        <active>true</active>
        <description>July 2011: Ensures that the Account has not been marked for deletion</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp; ( Account__r.ToBeDeleted__c = TRUE )</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>This Account has been marked for deletion, please choose another one.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VCP_VR_APR14SLS01_AssociatedRole</fullName>
        <active>true</active>
        <errorConditionFormula>NOT($User.BypassVR__c) 
&amp;&amp; 
(( 
ISBLANK(TEXT( AccountRole__c ))
&amp;&amp; NOT(ISBLANK(Account__c)) 
&amp;&amp; ISBLANK( Contact__c ) 
) 
||
( 
ISBLANK(TEXT( ContactRole__c )) 
&amp;&amp; NOT(ISBLANK( Contact__c )) 
)
)</errorConditionFormula>
        <errorMessage>Please enter either an Account or a Contact with an associated role.</errorMessage>
