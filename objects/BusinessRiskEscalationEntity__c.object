        <errorConditionFormula>LEN(Entity__c) +  LEN(SubEntity__c) + LEN(Location__c) + 6 &gt;80  &amp;&amp; NOT($User.BypassVR__c)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Name shouldn&apos;t exceed 80 characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ORG_VR_DEC12RR01_POBoxOrStrtShdBeFilled</fullName>
        <active>true</active>
        <errorConditionFormula>NOT($User.BypassVR__c) 
&amp;&amp; 
ISPICKVAL(Location_Type__c, &apos;Return Center&apos;) 
&amp;&amp; 
ISBLANK(RCPOBox__c) 
&amp;&amp; 
ISBLANK(Street__c)</errorConditionFormula>
        <errorMessage>Return Center requires Street Or POBox.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ORG_VR_DEC12RR02_CityZipCountryReq</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
NOT($User.BypassVR__c) ,
ISPICKVAL(Location_Type__c, &apos;Return Center&apos;) ,
OR
(
ISBLANK(RCCountry__c) ,
ISBLANK(RCZipCode__c),
ISBLANK(RCCity__c)

)

)</errorConditionFormula>
        <errorMessage>Return Center requires Country, Zip Code and City.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ORG_VR_DEC17RR_CheckingOrgStatus</fullName>
        <active>true</active>
        <errorConditionFormula>IF(And(NOT($User.BypassVR__c) ,ISBLANK(ReasonForDeactivation__c)), 
OR(ISPICKVAL(Status__c, &quot;Inactive&quot;),ISPICKVAL(Status__c, &quot;To be deactivated&quot;)),false)</errorConditionFormula>
        <errorDisplayField>ReasonForDeactivation__c</errorDisplayField>
        <errorMessage>Please fill the Reason for Deactivation</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ORG_VR_JUN16RR01_CheckValidDomains</fullName>
        <active>true</active>
        <description>OCT2015 Release : ContactEmail__c should contain valid email id</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
 VLOOKUP($ObjectType.SafeDomains__c.Fields.Name, $ObjectType.SafeDomains__c.Fields.Name,SUBSTITUTE(ContactEmail__c,left(ContactEmail__c,FIND(&quot;@&quot;, ContactEmail__c)), &quot;&quot;))!=SUBSTITUTE(ContactEmail__c,left(ContactEmail__c,FIND(&quot;@&quot;, ContactEmail__c)), &quot;&quot;) )</errorConditionFormula>
        <errorDisplayField>ContactEmail__c</errorDisplayField>
        <errorMessage>Please enter valid email address within a Schneider safe domain.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ORG_VR_NOV16_CheckTheLocationType</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
NOT($User.BypassVR__c) ,
NOT(ISPICKVAL(Location_Type__c, &apos;Customer Care Center&apos;)),VerticalisedCustomerCare__c=True)</errorConditionFormula>
        <errorMessage>You can not select  Location type other than Customer Care when you check  Verticalised Customer Care</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ORG_VR_OCT14RR01_blockthePlantCode</fullName>
        <active>true</active>
        <description>ORG_VR_OCT14RR01_blockthePlantCode</description>
        <errorConditionFormula>And(NOT($User.BypassVR__c),PlantCode__c!=&apos;&apos; ,NOT(ISPICKVAL(Location_Type__c,&apos;Plant&apos;)),
NOT(ISPICKVAL(Location_Type__c,&apos;Distribution Center&apos;)),
NOT(ISPICKVAL(Location_Type__c,&apos;Adaptation Center&apos;)))</errorConditionFormula>
        <errorMessage>You can&apos;t update Plant Code when Location Type is other than Plant or Distribution Center or Adaption Center</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ORG_VR_OCT15I2P01_BusinessUnit</fullName>
        <active>true</active>
        <description>I2P Oct 15 Release</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), NOT(ISBLANK(TEXT(BusinessUnit__c))),OR( NOT(ISBLANK( SubEntity__c )), NOT(ISBLANK( Location__c ))))</errorConditionFormula>
        <errorMessage>Business unit Can be Set only at the Entity Level.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ORG_VR_OCT15RR01_TEXEmailAddress</fullName>
        <active>true</active>
        <description>OCT2015 Release : TEX Email Address should contain valid email id</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
