    <validationRules>
        <fullName>LTR_VR_DEC12CCC01_LineItemReadonly</fullName>
        <active>false</active>
        <description>This Validation rule is to enforce not to edit any value when the Parent literature request status is processed.</description>
        <errorConditionFormula>AND(ISPICKVAL(LiteratureRequest__r.Status__c,&quot;Processed&quot;),OR(
ISCHANGED(CommercialReference__c),
ISCHANGED(LiteratureRequest__c),
ISCHANGED(Quantity__c)))</errorConditionFormula>
        <errorMessage>Line Item details cannot be changed for the processed Literature requests.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LTR_VR_DEC12CCC02_Quantitylessthanone</fullName>
        <active>true</active>
        <description>This Validation rule is to restrict the user to enter the quantity as less than one.</description>
        <errorConditionFormula>AND($User.BypassVR__c = False, 
Quantity__c &lt; 1)</errorConditionFormula>
        <errorMessage>Quantity should not be less than one .</errorMessage>
    </validationRules>
