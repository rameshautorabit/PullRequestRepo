    <validationRules>
        <fullName>VR_R117_GoldenId_Check</fullName>
        <active>true</active>
        <description>This rule will check if the losing and golden has different golden ids</description>
        <errorConditionFormula>AND(NOT( $User.BypassVR__c ),( Losing_Installed_Product__r.GoldenAssetId__c &lt;&gt;  Gaining_Installed_Product__r.GoldenAssetId__c ))</errorConditionFormula>
        <errorMessage>The losing and gaining IP must have the same golden ID</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_R117_InstalledAtAccountSame_Check</fullName>
        <active>false</active>
        <description>This will check if the losing asset and gaining asset belongs to different installed account</description>
        <errorConditionFormula>AND(NOT( $User.BypassVR__c ),(Gaining_Installed_Product__r.SVMXC__Company__c &lt;&gt;  Losing_Installed_Product__r.SVMXC__Company__c))</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>The losing and gaining IPs must have the same Installed At Account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_R117_MasterShip_Check</fullName>
        <active>true</active>
        <description>This validation rule will check if the losing and gaining ip belongs to BFO.</description>
        <errorConditionFormula>AND(NOT( $User.BypassVR__c ),Losing_Installed_Product__r.TECH_SDHPublisherMaster__c  &lt;&gt;  &apos;BFO&apos;, Gaining_Installed_Product__r.TECH_SDHPublisherMaster__c   &lt;&gt; &apos;BFO&apos;)</errorConditionFormula>
        <errorMessage>The losing and gaining IPs must have the same mastership as BFO</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_R117_ProductGroupId_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT( $User.BypassVR__c ), Losing_Installed_Product__r.ProductGroupID__c != null, Gaining_Installed_Product__r.ProductGroupID__c != null, Losing_Installed_Product__r.ProductGroupID__c !=  Gaining_Installed_Product__r.ProductGroupID__c )</errorConditionFormula>
        <errorMessage>The losing and gaining IP must have the same Product Group Id</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_R117_TechnicalAttribute_Check</fullName>
        <active>true</active>
        <description>This validation rule will check if the losing and gaining has different technical attribute.</description>
        <errorConditionFormula>AND(NOT( $User.BypassVR__c ), Losing_Installed_Product__r.TechnicalAttributePart1__c &lt;&gt; null, Gaining_Installed_Product__r.TechnicalAttributePart1__c  &lt;&gt; null, Losing_Installed_Product__r.TechnicalAttributePart1__c &lt;&gt;  Gaining_Installed_Product__r.TechnicalAttributePart1__c  )</errorConditionFormula>
        <errorMessage>Please review both Technical Attribute and choose which one needs to be kept by deleting the bad one.</errorMessage>
    </validationRules>
