    <fields>
        <fullName>IsACrossSellingOpptyLine__c</fullName>
        <externalId>false</externalId>
        <formula>IF 
( OR ( ProductBU__c=&apos;SERVICE&amp;PROJET DOMAIN&apos;, 

AND( 
OR( 
ProductBU__c=&apos;BUILDING MANAGEMENT&apos;, 
ProductBU__c=&apos;ECOBUSINESS&apos;, 
ProductBU__c=&apos;ENM LOW VOLTAGE&apos;, 
ProductBU__c=&apos;ENM HOME DISTRIBUTION&apos;, 
ProductBU__c=&apos;PARTNER&apos;, 
ProductBU__c=&apos;ENM DIGITAL ENERGY&apos;, 
ProductBU__c=&apos;ENM SECURE POWER&apos; 
), 
OR 
( 
ISPICKVAL(Opportunity__r.LeadingBusiness__c,&apos;BI&apos;), 
ISPICKVAL(Opportunity__r.LeadingBusiness__c,&apos;BD&apos;), 
ISPICKVAL(Opportunity__r.LeadingBusiness__c,&apos;PP&apos;), 
ISPICKVAL(Opportunity__r.LeadingBusiness__c,&apos;PR&apos;), 
ISPICKVAL(Opportunity__r.LeadingBusiness__c,&apos;IT&apos;) 
) 
), 

AND 
( 
OR( 
ProductBU__c=&apos;ENM MEDIUM VOLTAGE&apos;, 
ProductBU__c=&apos;Energy&apos; 
), 
ISPICKVAL(Opportunity__r.LeadingBusiness__c,&apos;EN&apos;) 
), 

AND 
( 
ProductBU__c=&apos;CLOUD&apos;, 
OR 
( 
ISPICKVAL(Opportunity__r.LeadingBusiness__c,&apos;GW&apos;), 
ISPICKVAL(Opportunity__r.LeadingBusiness__c,&apos;PS&apos;) 
) 
), 

AND 
( 
ProductBU__c=&apos;IND OEM BIC&apos;, 
OR 
( 
ISPICKVAL(Opportunity__r.LeadingBusiness__c,&apos;ID&apos;), 
ISPICKVAL(Opportunity__r.LeadingBusiness__c,&apos;IA&apos;), 
ISPICKVAL(Opportunity__r.LeadingBusiness__c,&apos;IB&apos;) 
) 
), 

AND 
( 
ProductBU__c=&apos;SOLAR&apos;, 
ISPICKVAL(Opportunity__r.LeadingBusiness__c,&apos;SO&apos;) 
) 
) 
,0,1)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
