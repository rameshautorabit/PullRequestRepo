    <validationRules>
        <fullName>PRL_VR01_CurrencyConsistency</fullName>
        <active>true</active>
        <description>SR0 - Make sure selected currency is the same as opportunity currency.</description>
        <errorConditionFormula>NOT( $User.BypassVR__c ) 
&amp;&amp; 
TEXT(CurrencyIsoCode) &lt;&gt; TEXT( Opportunity__r.CurrencyIsoCode )</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>Currency should be the same as the one selected as opportunity level.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRL_VR02_NonNegativeValue</fullName>
        <active>true</active>
        <description>Display an error message when the user enters a negative value in the Amount and / or Quantity</description>
        <errorConditionFormula>NOT( $User.BypassVR__c ) 
&amp;&amp; 
OR(  Amount__c &lt; 0,  Quantity__c &lt; 0)</errorConditionFormula>
        <errorMessage>Please enter non-negative numbers for Amount and / or Quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRL_VR03_ProbabilityPercentage</fullName>
        <active>true</active>
        <description>Probability percentage cannot be more than 100%</description>
        <errorConditionFormula>(Probability__c &gt; 1) &amp;&amp;  NOT($User.BypassVR__c)</errorConditionFormula>
        <errorDisplayField>Probability__c</errorDisplayField>
        <errorMessage>Probability percent cannot exceed 100%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRL_VR04_InactiveCheck</fullName>
        <active>true</active>
        <description>This rule will only fire on cloned Line.</description>
        <errorConditionFormula>NOT( $User.BypassVR__c ) &amp;&amp; TECH_IsCloned__c &amp;&amp; TECH_Inactive__c &amp;&amp; ISCHANGED( LineStatus__c ) &amp;&amp; NOT(ISPICKVAL(LineStatus__c, &apos;To be Deleted&apos;))</errorConditionFormula>
        <errorMessage>Line contains invalid product or PM0 values. Please use the &quot;Change Product&quot; function.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRL_VR_R1019SLS01_ProductLineCheck</fullName>
        <active>false</active>
        <description>In Sales Stage 5 that contain product lines;  INIBS, ITIBS, IDIBO or PTIBS,  then  must have  Product Family and Family  each of those lines</description>
        <errorConditionFormula>AND(Not($User.BypassVR__c), 
OR(ISPICKVAL(Opportunity__r.StageName ,&apos;5 - Prepare &amp; Bid&apos;),ISPICKVAL(Opportunity__r.StageName ,&apos;6 - Negotiate &amp; Win&apos;),ISPICKVAL(Opportunity__r.StageName ,&apos;7 - Deliver &amp; Validate&apos;)), 
OR(BEGINS (ProductLine__c,&quot;INIBS&quot;),
   BEGINS (ProductLine__c,&quot;ITIBS&quot;), 
   BEGINS (ProductLine__c,&quot;IDIBO&quot;),
   BEGINS (ProductLine__c,&quot;PTIBS&quot;)),
NOT(OR(ISPICKVAL(LineStatus__c,&quot;To be deleted&quot;),ISPICKVAL(LineStatus__c,&quot;Cancelled&quot;),ISPICKVAL(LineStatus__c,&quot;Lost&quot;))),
OR(ISBLANK(ProductFamily__c),
    ISBLANK(Family__c)
))</errorConditionFormula>
        <errorDisplayField>BusbarArrangement__c</errorDisplayField>
        <errorMessage>Please ensure all Service Product Lines (INIBS, ITIBS, IDIBO,PTIBS) contain Product Family AND Family details</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRL_VR_R217SLS02_LinePODateChange</fullName>
        <active>true</active>
        <description>When user tries to change the line PO date to the past, there should be a warning message, saying: &quot;Only Won, Booked, Shipped or Invoiced lines can be saved with a Line PO Date in the past.&quot;</description>
        <errorConditionFormula>AND(
Not($User.BypassVR__c),
ischanged(LinePODate__c),
NOT(
OR(
ISPICKVAL(LineStatus__c,&apos;Won&apos;),
ISPICKVAL(LineStatus__c,&apos;Booked&apos;),
ISPICKVAL(LineStatus__c,&apos;Shipped&apos;),
ISPICKVAL(LineStatus__c,&apos;Invoiced&apos;))),
LinePODate__c&lt;TODAY()
)</errorConditionFormula>
        <errorMessage>Only Won, Booked, Shipped or Invoiced lines can be saved with a Line PO Date in the past</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRL_VR_R217SLS04_LineStatusUpdate</fullName>
        <active>true</active>
        <description>To change the Line status to Won, Booked, Shipped or Invoiced the related opportunity Must be in stage 7</description>
        <errorConditionFormula>AND(
NOT($User.BypassVR__c),
ischanged(LineStatus__c),
OR(
(ISPICKVAL( LineStatus__c ,&apos;Won&apos;)),
(ISPICKVAL( LineStatus__c ,&apos;Booked&apos;)),
(ISPICKVAL( LineStatus__c ,&apos;Shipped&apos;)), 
(ISPICKVAL( LineStatus__c ,&apos;Invoiced&apos;))),
 NOT(ISPICKVAL(Opportunity__r.StageName,&apos;7 - Deliver &amp; Validate&apos; )))</errorConditionFormula>
        <errorMessage>The Opportunity must be in Sales Stage 7 for an Opportunity Line Status to be Won, Booked, Shipped or Invoiced</errorMessage>
    </validationRules>
