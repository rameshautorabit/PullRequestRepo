    <validationRules>
        <fullName>POLVRAPR2015SRV01</fullName>
        <active>true</active>
        <description>Validation Rule to make sure only one value at a time should be given(either &quot;Generic Reference&quot; or &quot;Free Text Reference&quot;.)</description>
        <errorConditionFormula>AND(RecordType.Name = &apos;Non reference spare part&apos;,Not(ISBLANK(Free_text_reference__c)), Not(ISBLANK( Generic_reference__c )), NOT($User.BypassVR__c)&amp;&amp; NOT($Profile.Id = $Label.CLFEB14SRV02))</errorConditionFormula>
        <errorMessage>You must only enter one value at a time, either &quot;Generic Reference&quot; or &quot;Free Text Reference&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>POLVRAPR2015SRV02</fullName>
        <active>true</active>
        <description>Validation Rule to make sure both  value cannot be null at a time (&quot;Generic Reference&quot; and &quot;Free Text Reference&quot;.)</description>
        <errorConditionFormula>AND(RecordType.Name  = &apos;Non reference spare part&apos;,ISBLANK(Free_text_reference__c),ISBLANK( Generic_reference__c ), NOT($User.BypassVR__c)&amp;&amp; NOT($Profile.Id = $Label.CLFEB14SRV02))</errorConditionFormula>
        <errorMessage>Please, fill in either &apos;Generic Reference&quot; or &quot;Free Text Reference&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>POLVRAPR2015SRV03</fullName>
        <active>true</active>
        <description>Free Text Reference field is not supported for Back-Office System &quot;IT ORACLE&quot;.
BFOFS-1934 R&amp;R july|2018 Free Test Ref field is not supported for PO BO sys</description>
        <errorConditionFormula>AND(RecordType.Name = &apos;Non reference spare part&apos;,OR(TEXT( SVMXC__Service_Order__r.BackOfficeSystem__c) = $Label.CLAPR15SRV54, 
TEXT( SVMXC__RMA_Shipment_Order__r.BackOfficeSystem__c) = $Label.CLAPR15SRV54), Not( ISBLANK( Free_text_reference__c )), NOT($User.BypassVR__c)&amp;&amp; NOT($Profile.Id = $Label.CLFEB14SRV02))</errorConditionFormula>
        <errorMessage>Free Text Reference field is not supported for Back-Office System &quot;IT ORACLE&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>POLVRAPR2015SRV04</fullName>
        <active>true</active>
        <description>Validation Rule to make sure both value cannot be null at a time (&quot;Use Refurbished&quot; and &quot;Reason for New&quot;.)</description>
        <errorConditionFormula>AND(RecordType.Name = &apos;Reference spare part&apos;,
OR(TEXT( SVMXC__Service_Order__r.BackOfficeSystem__c) = $Label.CLAPR15SRV54,TEXT( SVMXC__RMA_Shipment_Order__r.BackOfficeSystem__c) = $Label.CLAPR15SRV54),
NOT(searchForRefurbished__c),
ISPICKVAL(Reason_For_New__c,&quot;&quot;),
NOT($User.BypassVR__c),
NOT($Profile.Id = $Label.CLFEB14SRV02))</errorConditionFormula>
        <errorMessage>Please fill in the reason for new on the non refurbished part.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>POLVRAPR2015SRV05</fullName>
        <active>true</active>
        <description>Validation Rule to make sure both value cannot be null at a time (&quot;Use Refurbished&quot; and &quot;Reason for New&quot;.)</description>
        <errorConditionFormula>AND(RecordType.Name = &apos;Reference spare part&apos;,
OR(TEXT( SVMXC__Service_Order__r.BackOfficeSystem__c) == $Label.CLAPR15SRV54, TEXT( SVMXC__RMA_Shipment_Order__r.BackOfficeSystem__c) = $Label.CLAPR15SRV54),
searchForRefurbished__c=True &amp;&amp; NOT(ISPICKVAL( Reason_For_New__c,&quot;&quot;)), NOT($User.BypassVR__c)&amp;&amp; NOT($Profile.Id = $Label.CLFEB14SRV02))</errorConditionFormula>
        <errorMessage>Please don&apos;t fill in the reason for new on the refurbished part.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>POLVRJUN2015SRV02</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), SVMXC__Expected_Quantity2__c &lt; 0)</errorConditionFormula>
        <errorDisplayField>SVMXC__Expected_Quantity2__c</errorDisplayField>
        <errorMessage>Please make sure to enter a valid expected quantity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>POLVROCT2017SRV019</fullName>
        <active>true</active>
        <description>This validation is for limiting the quantity as 1 for Installed products</description>
        <errorConditionFormula>AND( 
NOT($User.BypassVR__c),
NOT($Profile.Name = &quot;SE - Interface – WorkOrderConnector&quot;), 

OR(
	AND(
	ISPICKVAL(SVMXC__Line_Type__c,&apos;RMA&apos;),
	SVMXC__Expected_Quantity2__c !=1 , 
	NOT(ISBLANK(SVMXC__Serial_Number__c)),
	ISCHANGED(SVMXC__Serial_Number__c)
	), 

	AND(
		NOT(ISBLANK(SVMXC__Serial_Number__c)),
		ISCHANGED(SVMXC__Expected_Quantity2__c),	
		OR(
		SVMXC__Expected_Quantity2__c &lt; 0,
		SVMXC__Expected_Quantity2__c &gt; 1
	    )
    )
 )
)</errorConditionFormula>
        <errorMessage>Parts Order line that is linked to an Installed Product must have an Expected Qty which is 1 or zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>POLVROCT2017SRV19</fullName>
        <active>true</active>
        <description>BFOFS-1934.This validation blocks the creation on shipment part order lines of parts orders that are in the released status.</description>
        <errorConditionFormula>if(AND(NOT($User.BypassVR__c),NOT($Profile.Id = $Label.CLFEB14SRV02)), 
if(ISNEW(),if( 
ISPICKVAL(SVMXC__Line_Type__c,&apos;Shipment&apos;),if( 


OR(
AND(ISPICKVAL(SVMXC__RMA_Shipment_Order__r.SVMXC__Order_Status__c,&apos;Released&apos;), 
CONTAINS($Label.CLR317FS04, TEXT(SVMXC__RMA_Shipment_Order__r.BackOfficeSystem__c ))
),

ISPICKVAL(SVMXC__RMA_Shipment_Order__r.SVMXC__Order_Status__c,&apos;Pending Release in BO&apos;),
NOT(ISBLANK(SVMXC__RMA_Shipment_Order__r.TECH_BOPartsOrderRecordId__c))) 

,true,false),false 
),false),false 
)</errorConditionFormula>
        <errorMessage>Shipment lines cannot be added as the Part Order is already Released, synchronized or in the process of being synchronized to the Back Office</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>POLVROCT2017SRV20</fullName>
        <active>true</active>
        <description>Validation Rule to make sure &quot;Generic Reference&quot; or &quot;Free Text Reference&quot; or &quot;Spare Part Name&quot; cannot be null at a time</description>
        <errorConditionFormula>NOT($User.BypassVR__c)&amp;&amp; NOT($Profile.Id = $Label.CLFEB14SRV02) &amp;&amp; 
RecordType.Name = &apos;RMA&apos; &amp;&amp; 
(
( 
IF(ISBLANK(Free_text_reference__c),1,0) 
+ IF(ISBLANK(Generic_reference__c),1,0) 
+ IF(ISBLANK(SVMXC__Product__c),1,0) 
) != 2
)</errorConditionFormula>
        <errorMessage>Please, fill in either &quot;Generic Reference&quot; or &quot;Free Text Reference&quot; or &quot;Spare part Name&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>POLVROCT2017SRV21</fullName>
        <active>true</active>
        <description>This validation blocks the creation of part order lines of parts orders that are in the Closed,Cancelled, Deleted status.</description>
        <errorConditionFormula>NOT($User.BypassVR__c)&amp;&amp; NOT($Profile.Id = $Label.CLFEB14SRV02) &amp;&amp; 
ISNEW() &amp;&amp; 
SVMXC__RMA_Shipment_Order__r.RecordType.Name ==&apos;Return&apos; &amp;&amp; 
OR(
ISPICKVAL(SVMXC__RMA_Shipment_Order__r.SVMXC__Order_Status__c,&apos;Closed&apos;), 
ISPICKVAL(SVMXC__RMA_Shipment_Order__r.SVMXC__Order_Status__c,&apos;Cancelled&apos;),
ISPICKVAL(SVMXC__RMA_Shipment_Order__r.SVMXC__Order_Status__c,&apos;Deleted&apos;),
