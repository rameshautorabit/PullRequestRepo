    <validationRules>
        <fullName>SVMXC__SVMXC_ValidateDateRequired</fullName>
        <active>true</active>
        <description>Required date must be a future date.</description>
        <errorConditionFormula>AND(ISNEW(),  SVMXC__Date_Required__c &lt;  TODAY())</errorConditionFormula>
        <errorDisplayField>SVMXC__Date_Required__c</errorDisplayField>
        <errorMessage>Date required cannot be a past date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SVMXC__SVMXC_ValidateQuantityChange</fullName>
        <active>true</active>
        <description>Quantity Required cannot be changed.</description>
        <errorConditionFormula>AND(!ISNEW(),   OR(SVMXC__Quantity_Shipment_Initiated2__c &gt; 0,  SVMXC__Quantity_Shipped2__c &gt; 0), ISCHANGED( SVMXC__Quantity_Required2__c ))</errorConditionFormula>
        <errorDisplayField>SVMXC__Quantity_Required2__c</errorDisplayField>
        <errorMessage>Quantity Required cannot be changed once shipment activity has been initiated. Create a new Parts request instead.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SVMXC__SVMXC_ValidateQuantityRequired</fullName>
        <active>true</active>
        <description>Required Quantity must be greater than zero.</description>
        <errorConditionFormula>SVMXC__Quantity_Required2__c &lt;= 0</errorConditionFormula>
        <errorDisplayField>SVMXC__Quantity_Required2__c</errorDisplayField>
        <errorMessage>Required Quantity must be greater than zero.</errorMessage>
    </validationRules>
