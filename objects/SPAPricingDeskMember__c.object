    <validationRules>
        <fullName>SPA_VR_SEP12SLS01_Chairman</fullName>
        <active>true</active>
        <description>If Member Type is Chairman then Local Pricing Group should be empty</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c),
AND (ISPICKVAL( MemberType__c , &apos;Chairman&apos;),  NOT( ISBLANK( LocalPricingGroup__c ) )  ) )</errorConditionFormula>
        <errorDisplayField>LocalPricingGroup__c</errorDisplayField>
        <errorMessage>Chairman should not be assigned to a Local Pricing Group</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_SEP12SLS02_LPGMandatory</fullName>
        <active>true</active>
        <description>Local Pricing Group is mandatory for Member Type &quot;Local Pricing Group Lead&quot;</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c),
AND( ISPICKVAL( MemberType__c , &apos;Local Pricing Group Lead&apos; ) , ISBLANK( LocalPricingGroup__c )  ) )</errorConditionFormula>
        <errorDisplayField>LocalPricingGroup__c</errorDisplayField>
        <errorMessage>Local Pricing Group is mandatory for Member Type &quot;Local Pricing Group Lead&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_SEP12SLS03_CPGMandatory</fullName>
        <active>true</active>
        <description>Customer Pricing Group is mandatory for Member Type &quot;Customer Pricing Group Lead&quot;</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c),
AND( ISPICKVAL( MemberType__c , &apos;Customer Pricing Group Lead&apos; ) , ISBLANK( CustomerPricingGroup__c )  ) )</errorConditionFormula>
        <errorDisplayField>CustomerPricingGroup__c</errorDisplayField>
        <errorMessage>Customer Pricing Group is mandatory for Member Type &quot;Customer Pricing Group Lead&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_SEP12SLS04_LPGBlank</fullName>
        <active>true</active>
        <description>Local Pricing Group should be blank for Pricing Desk Member of any type other than Local Pricing Group</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c), 
AND(NOT ISPICKVAL( MemberType__c , &apos;Local Pricing Group Lead&apos; ) , NOT ISBLANK( LocalPricingGroup__c ) ) )</errorConditionFormula>
        <errorDisplayField>LocalPricingGroup__c</errorDisplayField>
        <errorMessage>Local Pricing Group should be entered only for member type &quot;Local Pricing Group Lead&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_SEP12SLS05_CPGBlank</fullName>
        <active>true</active>
        <description>Customer Pricing Group should be blank for Pricing Desk Member of any type other than Customer Pricing Group Lead</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c),
AND(NOT ISPICKVAL( MemberType__c , &apos;Customer Pricing Group Lead&apos; ) , NOT ISBLANK( CustomerPricingGroup__c )  ) )</errorConditionFormula>
        <errorDisplayField>CustomerPricingGroup__c</errorDisplayField>
        <errorMessage>Customer Pricing Group should be entered only for member type &quot;Customer  Pricing Group Lead&quot;</errorMessage>
    </validationRules>
