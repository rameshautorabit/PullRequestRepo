    <validationRules>
        <fullName>BSI_VR01_CreateNewBSIRecordUnderOpp</fullName>
        <active>true</active>
        <description>User should be able to create only one record for each Business under the Opportunity.</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp; (IF(ISNEW() &amp;&amp; RecordTypeId = &apos;012A0000000ndkO&apos;, IF(Opportunity__r.BuildingsEnergySolutionsBSICount__c&gt;=1,true, false) , 
IF(ISNEW() &amp;&amp; RecordTypeId =&apos;012A0000000ndkT&apos;, IF(Opportunity__r.ITBBSICount__c&gt;=1,true, false),
IF(ISNEW() &amp;&amp; RecordTypeId =&apos;012A0000000nkNj&apos;, IF(Opportunity__r.BuildingsBSICount__c&gt;=1,true, false),
IF(ISNEW() &amp;&amp; RecordTypeId =&apos;012A0000000nySb&apos;, IF(Opportunity__r.SmartInfrastructureBSICount__c &gt;=1,true, false),
IF(ISNEW() &amp;&amp; RecordTypeId =&apos;012A0000000njg1&apos;, IF(Opportunity__r.PowerBSICount__c&gt;=1,true, false),
IF(ISNEW() &amp;&amp; RecordTypeId =&apos;012A0000000oE6x&apos;, IF(Opportunity__r.IndustryBSICount__c&gt;=1,true, false),false)))))))</errorConditionFormula>
        <errorMessage>You cannot create more than one record for each Business under the Opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BSI_VR02_GoNoGoRanking</fullName>
        <active>true</active>
        <description>Allow user to enter value from range of &quot;0.0&quot; to &quot;7.0&quot;</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp; (OR( GoNoGoRanking__c &lt; 0.0 , GoNoGoRanking__c &gt; 7.0))</errorConditionFormula>
        <errorDisplayField>GoNoGoRanking__c</errorDisplayField>
        <errorMessage>Enter value from range 0.0 to 7.0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BSI_VR_OCT14SLS01_AccessToCertainFields</fullName>
        <active>true</active>
        <description>Validates if the logged in user has permission to access certain fields</description>
        <errorConditionFormula>NOT($User.BypassVR__c)
&amp;&amp;

(AND(
	OR(ISBLANK($User.Permission_sets_assigned__c), NOT(CONTAINS($User.Permission_sets_assigned__c, &quot;SA - InvensysGlobalMajorProjectsTeam&quot;))), 
	OR(ISCHANGED(PursuitLeader__c), ISCHANGED(EstimatedPursuitValue__c), ISCHANGED(Licensor__c), ISCHANGED(ProjectManagementContractor__c), ISCHANGED(PursuitPhase__c), ISCHANGED(EndUsertoEPCAwardDate__c),ISCHANGED(Comments__c)),
	NOT(ISPICKVAL(GlobalMajorProjects__c, &apos;Regional MP: Major Project regionally handled&apos;)),
	NOT(ISPICKVAL(GlobalMajorProjects__c, &apos;&apos;))	
	)  
	)</errorConditionFormula>
        <errorMessage>You do not have appropriate permission to modify this BSI.</errorMessage>
    </validationRules>
