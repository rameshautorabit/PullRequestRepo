    <fields>
        <fullName>TECH_CountryCode__c</fullName>
        <externalId>false</externalId>
        <formula>Country__r.CountryCode__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
