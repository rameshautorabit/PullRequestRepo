    <fields>
        <fullName>TECH_CriteriaCode__c</fullName>
        <description>Technical field used to store the code obtained by concatenating the values of conditional fields. This field is used to compare and respectively assign the Account owner (if the code matches).</description>
        <externalId>false</externalId>
        <formula>IF( 
	ISPICKVAL( Functionality__c , &quot;AOA&quot;), 
	TEXT(ClassLevel1__c) &amp;  TEXT(LeadingBusiness__c)  &amp;  Country__r.CountryCode__c,
	IF( 
		ISPICKVAL( Functionality__c , &quot;EAO&quot;), 
 IF( ISBLANK( County__c )  , Country__r.CountryCode__c &amp; TEXT(ClassLevel1__c) &amp;  StateProvince__r.StateProvinceCode__c  , Country__r.CountryCode__c &amp; TEXT(ClassLevel1__c) &amp; County__c) 
		, 
		null
	) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>TECH_CriteriaCode</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
