    <validationRules>
        <fullName>CCM_VR_MAR18CCC01_ClassificationCheck</fullName>
        <active>true</active>
        <description>When the Case Qualification is selected as Custom Tree , you have to select related Custom tree</description>
        <errorConditionFormula>AND( !$User.BypassVR__c ,   ISPICKVAL( CaseQualification__c , &apos;Custom Tree Selection&apos;) , ISBlank( CustomTree__c ) )</errorConditionFormula>
        <errorMessage>when you select Case Qualification as custom tree you must select the related Custom tree</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCM_VR_MAR18CCC01_SalesOrderCheck</fullName>
        <active>true</active>
        <description>When you make Sales Order number Mandatory, Sales Order number must be checked.</description>
        <errorConditionFormula>AND( !$User.BypassVR__c ,  SalesOrderNumberMandatory__c  , NOT(  SalesOrderNumber__c ) )</errorConditionFormula>
        <errorMessage>You cannot make Sales Order number as Mandatory without checking Sales Order Number first</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCM_VR_R617CCC01_CheckProductExposed</fullName>
        <active>false</active>
        <errorConditionFormula>AND( !$User.BypassVR__c , ProductSelectionMandatory__c , NOT( DisplayProductSelection__c ) )</errorConditionFormula>
        <errorDisplayField>Channel__c</errorDisplayField>
        <errorMessage>You cannot make Product Selection as Mandatory without checking Display Product selection first</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCM_VR_R617CCC02_CheckContractEnabled</fullName>
        <active>true</active>
        <errorConditionFormula>AND( !$User.BypassVR__c ,  ContractSelectionMandatory__c , NOT(  ContractSearch__c ) )</errorConditionFormula>
        <errorMessage>You cannot make Contract Selection as Mandatory without checking Contract Search first</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
