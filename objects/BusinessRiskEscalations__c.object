        <externalId>false</externalId>
        <formula>IF ( MONTH(DATEVALUE(ClosedDate__c))=MONTH(OpenDate__c),
/* If the Business Risk is open and closed in the same month it should be counted in the month it was open */
TEXT(
  YEAR(
   OpenDate__c
  )
) 
&amp;&apos;-&apos;
&amp;LPAD(
  TEXT(
    MONTH(
      OpenDate__c
    )
  )
,2,&apos;0&apos;
)
, /* Otherwise the Business Risk should be counted in the month following the month it was opened */
TEXT(
  YEAR(
    OpenDate__c+30
  )
) 
&amp;&apos;-&apos;
&amp;LPAD(
  TEXT(
    MONTH(
      OpenDate__c+30)
    ),2,&apos;0&apos;)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>TECH_YearMonth</label>
