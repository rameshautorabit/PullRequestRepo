    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>BRE_VR01_AccountControl</fullName>
        <active>true</active>
        <description>July 2011: Ensures that the Account has not been marked for deletion</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp;  (ISNEW() || ISCHANGED(  AffectedCustomer__c  )) &amp;&amp;(  AffectedCustomer__r.ToBeDeleted__c = TRUE )</errorConditionFormula>
        <errorDisplayField>AffectedCustomer__c</errorDisplayField>
        <errorMessage>This Account has been marked for deletion, please choose another one.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BRE_VR02_ActionPlanAgreedWithCustomer</fullName>
        <active>true</active>
        <description>I2P - While Closing the case user has to provide the details of the Action Plan Agreed With Customer .</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),
    OR(AND(NOT(ISPICKVAL(Status__c,&quot;Closed&quot;)),
			NOT(ISPICKVAL(Status__c,&quot;Re-Open&quot;)),
           NOT(ISBLANK(ActionPlanAgreedWithCustomer__c))
		  ),
	   AND(ISPICKVAL(Status__c,&quot;Closed&quot;),
           ISBLANK(ActionPlanAgreedWithCustomer__c)
		  )
	  )
	)</errorConditionFormula>
        <errorDisplayField>ActionPlanAgreedWithCustomer__c</errorDisplayField>
        <errorMessage>The Action Plan Agreed with Customer must be filled in when closing the BRE. It cannot be filled in before.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BRE_VR03_StatusShouldNotBeChanged</fullName>
        <active>true</active>
        <description>I2P - User should not change the status field of Business Risks from Re-Open to &apos;Open&apos;.</description>
        <errorConditionFormula>AND($User.BypassVR__c =False,
             ISCHANGED ( Status__c ), ISPICKVAL( PRIORVALUE(Status__c),&quot;Re-Open&quot;),ISPICKVAL( Status__c , &quot;Open&quot; )
                     )</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Status can not be changed from Re-Open to &apos;Open&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BRE_VR04_ResolutionLeader_Sponsor</fullName>
        <active>true</active>
        <description>I2P - While saving a  Business Risk Resolution Leader and business risk sponsor should not be blank</description>
        <errorConditionFormula>(AND($User.BypassVR__c =False,(OR(ISBLANK(ResolutionLeader__c),ISBLANK(BusinessRiskSponsor__c)))))</errorConditionFormula>
        <errorMessage>We are unable to select an appropriate Resolution Leader.
Either Change the Entity,Sub-Entity or Country Or select that field manually</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BRE_VR05_NonGSAEstimatedFinancialImpact</fullName>
        <active>true</active>
        <description>Estimated financial impact is not mandatory for Strategic Accounts</description>
        <errorConditionFormula>$User.BypassVR__c == FALSE 
   &amp;&amp;
       (     NOT (AffectedCustomer__r.TECH_I2PNotification__c = &apos;GSA&apos; )  &amp;&amp;
             NOT ( ISPICKVAL ( AffectedCustomer__r.Type, &apos;GMO&apos; ))  &amp;&amp;
             NOT ( ISPICKVAL ( AffectedCustomer__r.Type, &apos;IA&apos; ))
       )
  &amp;&amp;
ISBLANK (EstimatedFinancialImpact__c)</errorConditionFormula>
        <errorDisplayField>EstimatedFinancialImpact__c</errorDisplayField>
        <errorMessage>Estimated Financial Impact cannot be blank for a non-global Affected Customer</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BRE_VR_DEC13I2P01_CheckRelatedProblem</fullName>
        <active>true</active>
        <description>Related problem  should not be blank while closing the BRE</description>
        <errorConditionFormula>And( $User.BypassVR__c =False,ISPICKVAL(Status__c, &apos;Closed&apos;),RelatedProblem__c =null)</errorConditionFormula>
        <errorDisplayField>RelatedProblem__c</errorDisplayField>
        <errorMessage>Related Problem is mandatory before Business Risk Escalation status set to closed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BRE_VR_JUN16I2P01_CheckSafeDomain</fullName>
        <active>false</active>
        <description>Check safe domain in Customer Contact Email</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), VLOOKUP($ObjectType.SafeDomains__c.Fields.Name, $ObjectType.SafeDomains__c.Fields.Name,SUBSTITUTE(CustomerContactEmail__c,left(CustomerContactEmail__c,FIND(&quot;@&quot;, CustomerContactEmail__c)), &quot;&quot;))!=SUBSTITUTE(CustomerContactEmail__c,left(CustomerContactEmail__c,FIND(&quot;@&quot;, CustomerContactEmail__c)), &quot;&quot;))</errorConditionFormula>
        <errorDisplayField>CustomerContactEmail__c</errorDisplayField>
        <errorMessage>Please enter valid email address within a Schneider safe domain.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BRE_VR_JUN16I2P02_CheckSafeDomain</fullName>
        <active>true</active>
        <description>Check safe domain in Local Contact Email</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), VLOOKUP($ObjectType.SafeDomains__c.Fields.Name, $ObjectType.SafeDomains__c.Fields.Name,SUBSTITUTE(LocalContactEmail__c,left(LocalContactEmail__c,FIND(&quot;@&quot;, LocalContactEmail__c)), &quot;&quot;))!=SUBSTITUTE(LocalContactEmail__c,left(LocalContactEmail__c,FIND(&quot;@&quot;, LocalContactEmail__c)), &quot;&quot;))</errorConditionFormula>
        <errorDisplayField>LocalContactEmail__c</errorDisplayField>
        <errorMessage>Please enter valid email address within a Schneider safe domain.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BRE_VR_Jul18I2P01_CheckLocationType</fullName>
        <active>true</active>
        <description>Check if Location Type of Originating Country is either Country Front Office HQ, Country Front Office Verticalised, and Country Front Office Other</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),NOT(ISPICKVAL(OriginatingOrganisation__r.Location_Type__c,&quot;Country Front Office HQ&quot;)),NOT(ISPICKVAL(OriginatingOrganisation__r.Location_Type__c,&quot;Country Front Office Verticalized&quot;)),NOT(ISPICKVAL(OriginatingOrganisation__r.Location_Type__c,&quot;Country Front Office Other&quot;)))</errorConditionFormula>
        <errorDisplayField>OriginatingOrganisation__c</errorDisplayField>
        <errorMessage>Originating County should only contain organizations with Location Type = Country Front Office HQ, Country Front Office Verticalised or Country Front Office Other</errorMessage>
    </validationRules>
    <validationRules>
