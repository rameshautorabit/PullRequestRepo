    <validationRules>
        <fullName>IPL_VR_FEB2015_tobedeletedIP</fullName>
        <active>true</active>
        <description>IP to be deleted</description>
        <errorConditionFormula>OR (AND(NOT($User.BypassVR__c), InstalledProduct__r.ToBeDeletedMerge__c , LinkToAsset__r.ToBeDeletedMerge__c )=TRUE)</errorConditionFormula>
        <errorMessage>The IP is &quot;to be deleted&quot; chose another one.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IPL_VR_OCT2013_FieldsShouldNotUpdate</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), OR( ISCHANGED( InstalledProduct__c ) , ISCHANGED( LinkToAsset__c ) , ISCHANGED( LinkType__c ) ) )</errorConditionFormula>
        <errorMessage>Fields From Installed Product,To Installed Product ,Link Type should not update</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_LinkToequalIntalledProduct</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),LinkToAsset__c = InstalledProduct__c)</errorConditionFormula>
        <errorMessage>You cannot link an Installed Product to itself.</errorMessage>
    </validationRules>
