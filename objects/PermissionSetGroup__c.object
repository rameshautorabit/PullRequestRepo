    <validationRules>
        <fullName>Approval_Level_Mandatory</fullName>
        <active>false</active>
        <description>When Approval Type is Manual then Approval Level field is mandatory</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),ISPICKVAL( Approval_Type__c , &apos;Manual&apos;) ,TEXT( Approval_Level__c )  = &apos;&apos;)</errorConditionFormula>
        <errorMessage>Approval Level is mandatory when the Approval Type is &apos;Manual&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approver_1_name_mandatory</fullName>
        <active>false</active>
        <description>If Approval Type is Manual, and Approval Level is selected as Level 1 or Level 2 then Approver 1 is mandatory</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),ISPICKVAL(Approval_Type__c, &apos;Manual&apos;), OR(ISPICKVAL( Approval_Level__c , &apos;Level 1&apos;),ISPICKVAL(Approval_Level__c , &apos;Level 2&apos;)), ISBLANK(Approver1_name__c) )</errorConditionFormula>
        <errorMessage>Approver 1 is mandatory when approval level is Level 1</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approver_2_name_mandatory</fullName>
        <active>false</active>
        <description>If Approval Type is Manual, and Approval Level is Level 2 then Approver 2 name is mandatory</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),ISPICKVAL(Approval_Type__c, &apos;Manual&apos;), ISPICKVAL(Approval_Level__c , &apos;Level 2&apos;),ISBLANK( Approver_2_name__c))</errorConditionFormula>
        <errorMessage>Approver 1 and Approver 2 is mandatory when approval level is Level 2</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approver_2_name_to_be_Emply</fullName>
        <active>false</active>
        <description>Approver 2 name field should be empty when approval type is manual and approval level is level 1</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),ISPICKVAL(Approval_Type__c , &apos;Manual&apos;),   !ISBLANK(Approver_2_name__c),ISPICKVAL(Approval_Level__c,
&apos;Level 1&apos;))</errorConditionFormula>
        <errorMessage>When Approval Level is Level 1, Approver 2 name should be empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Aproval_Level_to_be_empty_If_automatic</fullName>
        <active>false</active>
        <description>Approval Level and approver 1 name and approver 2 name should be empty if approval type is automatic</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),ISPICKVAL(Approval_Type__c , &apos;Automatic&apos;), OR(!ISBLANK(TEXT(Approval_Level__c)),!ISBLANK( Approver1_name__c),!ISBLANK(Approver_2_name__c)))</errorConditionFormula>
        <errorDisplayField>Approval_Level__c</errorDisplayField>
        <errorMessage>If Approval type is Automatic then Approval Level , Approver 1 name and Approver 2 name should be empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>USG_VR_MAR01_OnlyOneProfileAllowed</fullName>
        <active>true</active>
        <description>Validation rule to not allow more than one profile member</description>
        <errorConditionFormula>IF(RecordType.DeveloperName = &apos;Profile&apos; &amp;&amp;
RecordType.DeveloperName = &apos;NewProfileActivation&apos; &amp;&amp;  CountOfProfileMembers__c &gt; 1, true, false)</errorConditionFormula>
        <errorMessage>You cannot have more than one profile member for this security group</errorMessage>
    </validationRules>
