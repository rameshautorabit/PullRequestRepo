    <validationRules>
        <fullName>VR_Q216_Change_the_SignOffDecision</fullName>
        <active>true</active>
        <description>change the Sign Off Decision, the following conditions must be met, error messages shall be shown to the user when conditions are not met</description>
        <errorConditionFormula>IF(AND(NOT( $User.BypassVR__c ),
				ISCHANGED (SignOffDecision__c), 
	   OR(
					NOT(	ISPICKVAL(DocumentProofreadingStatus__c , &apos;Approved&apos;)), 
	    NOT(ISPICKVAL(Status__c , &apos;4. Pending Final Approval&apos;))
		)
  ),
			true ,false)</errorConditionFormula>
        <errorMessage>To change the Sign Off Decision, the following conditions must be met: Documents Proofreading Status is equal to Approved and FSB Status is Pending Final Approval</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Q216_FSBPendingtobeCancelled</fullName>
        <active>true</active>
        <description>Q2 2016 Release : The Field Service Bulletin Status must be moved to Cancelled as the selected document type is not FSB</description>
        <errorConditionFormula>AND( NOT($User.BypassVR__c) , OR(ISPICKVAL(Status__c , &apos;3a. Document in progress&apos;) , ISPICKVAL(Status__c , &apos;4. Pending Final Approval&apos;) , ISPICKVAL(Status__c , &apos;5. Execution&apos;) , ISPICKVAL(Status__c , &apos;6. Completed&apos;)), ISPICKVAL( TypeofDocumenttoGenerate__c , &apos;Pending&apos;), NOT(ISPICKVAL( TypeofDocumenttoGenerate__c , &apos;&apos;)))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Please change the Go/No-Go Decision to either &quot;Go&quot; or &quot;No Go&quot; before trying to move the Status to Document In Progress.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Q216_FSB_Status_Execution</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c ),ISCHANGED(Status__c),
				ISPICKVAL( Status__c , &apos;5. Execution&apos;), 
	   NOT( ISPICKVAL( SignOffDecision__c , &apos;Approved for Execution&apos;))
      )</errorConditionFormula>
        <errorMessage>To move an FSB to status 5. Execution: Sign Off Decision has to be Approved for Execution.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Q216_FSBtobeCancelled</fullName>
        <active>true</active>
        <description>Q2 2016 Release : The Field Service Bulletin Status must be moved to Cancelled as the selected document type is not FSB</description>
        <errorConditionFormula>AND( NOT($User.BypassVR__c) , OR(ISPICKVAL(Status__c , &apos;3a. Document in progress&apos;) , ISPICKVAL(Status__c , &apos;4. Pending Final Approval&apos;) , ISPICKVAL(Status__c , &apos;5. Execution&apos;) , ISPICKVAL(Status__c , &apos;6. Completed&apos;)), ISPICKVAL( TypeofDocumenttoGenerate__c , &apos;No Go&apos;), NOT(ISPICKVAL( TypeofDocumenttoGenerate__c , &apos;&apos;)))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>The Field Service Bulletin Status must be moved to Cancelled as the Go/No-Go Decision is not Go</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Q216_NameChange</fullName>
        <active>true</active>
        <description>Q2 2016 Release : FSB Name cannot be modified beyond Satus &apos;4. Pending Document Approval.&apos;</description>
        <errorConditionFormula>AND( NOT($User.BypassVR__c) , OR(ISPICKVAL(Status__c , &apos;4. Pending Final Approval&apos;) , ISPICKVAL(Status__c , &apos;5. Execution&apos;) , ISPICKVAL(Status__c , &apos;6. Completed&apos;)),   ISCHANGED(Name))</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>FSB Name cannot be modified beyond Satus &apos;4. Pending Document Approval.&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Q216_NotApproved</fullName>
        <active>true</active>
        <description>Q2 2016 Release : The FSB document must be validated by the Line of Business Quality Manager to change the status to &apos;4. Pending Final Approval&apos; or any further status. Please update &apos; Document Proofreading Status&apos; field.</description>
        <errorConditionFormula>AND( NOT($User.BypassVR__c) , OR(ISPICKVAL(Status__c , &apos;4. Pending Final Approval&apos;) , ISPICKVAL(Status__c , &apos;5. Execution&apos;) , ISPICKVAL(Status__c , &apos;6. Completed&apos;)) , NOT(ISPICKVAL( DocumentProofreadingStatus__c , &apos;Approved&apos;)))</errorConditionFormula>
        <errorDisplayField>DocumentProofreadingStatus__c</errorDisplayField>
        <errorMessage>The FSB document must be validated by the Line of Business Quality Manager to change the status to &apos;4. Pending Final Approval&apos; or any further status. Please update &apos; Document Proofreading Status&apos; field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Q216_ReasonMandatoryforCancel</fullName>
        <active>true</active>
        <description>Q2 2016 Release : Please enter a reason for cancelling the Field Service Bulletin.</description>
        <errorConditionFormula>AND(NOT( $User.BypassVR__c ) ,  ISPICKVAL(Status__c, &apos;3b. Cancelled&apos;) ,  ISBLANK(Reason__c)  )</errorConditionFormula>
        <errorDisplayField>Reason__c</errorDisplayField>
        <errorMessage>Please enter a reason for cancelling the Field Service Bulletin.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Q216_ReasonMandatoryforObsolete</fullName>
        <active>true</active>
        <description>Q2 2016 Release : Please provide a Reason to set the status to Obsolete.</description>
        <errorConditionFormula>AND(NOT( $User.BypassVR__c ) ,  ISPICKVAL(Status__c, &apos;7. Obsolete&apos;) ,  ISBLANK(Reason__c)  )</errorConditionFormula>
        <errorDisplayField>Reason__c</errorDisplayField>
        <errorMessage>Please provide a Reason to set the status to Obsolete.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Q216_SeverityChange</fullName>
        <active>true</active>
        <description>Q2 2016 Release : Severity of a &apos;Safety related&apos; FSB cannot be changed.</description>
        <errorConditionFormula>AND ( NOT( $User.BypassVR__c ) , ISPICKVAL(PRIORVALUE(Severity__c), &apos;Safety related&apos;), ISCHANGED( Severity__c) )</errorConditionFormula>
        <errorDisplayField>Severity__c</errorDisplayField>
        <errorMessage>Severity of a &apos;Safety related&apos; FSB cannot be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Q216_SeveritySafetyrelated</fullName>
        <active>true</active>
        <description>Q2 2016 Release : Please create this Field Service Bulletin from the corresponding Safety Problem.</description>
        <errorConditionFormula>AND ( NOT( $User.BypassVR__c ) , ISPICKVAL(Severity__c, &apos;Safety related&apos;), NOT(ISPICKVAL( Problem__r.Severity__c , &apos;Safety related&apos;)) )</errorConditionFormula>
        <errorMessage>Please create this Field Service Bulletin from the corresponding Safety Problem.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Q216_SignOffReason</fullName>
        <active>true</active>
        <description>Q2 2016 Release : Please enter Sign Off Desicion Comment for Sign Off Decision = &quot;Rejected - To be re-worked&quot;</description>
        <errorConditionFormula>AND( NOT( $User.BypassVR__c) ,  ISBLANK( SignOffDecisionComment__c ) ,  ISPICKVAL( SignOffDecision__c , &apos;Rejected - To be re-worked&apos;)  )</errorConditionFormula>
        <errorDisplayField>SignOffDecisionComment__c</errorDisplayField>
        <errorMessage>Please enter Sign Off Desicion Comment for Sign Off Decision = &quot;Rejected - To be re-worked&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Q216_StatusChange</fullName>
        <active>true</active>
        <description>Q2 2016 Release : Please provide the list of affected products in the FSB Affected Products related list to indicate who should validate this Field Service Bulletin.</description>
        <errorConditionFormula>AND( NOT($User.BypassVR__c) , OR(ISPICKVAL(Status__c , &apos;2. Go/NoGo Decision Pending&apos;) , ISPICKVAL(Status__c , &apos;3a. Document in progress&apos;) ,  ISPICKVAL(Status__c , &apos;4. Pending Final Approval&apos;) , ISPICKVAL(Status__c , &apos;5. Execution&apos;) , ISPICKVAL(Status__c , &apos;6. Completed&apos;)),  NumberOfAffectedProducts__c = 0 )</errorConditionFormula>
        <errorMessage>Please provide the list of affected products in the FSB Affected Products related list to indicate who should validate this Field Service Bulletin.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Q216_StatusChangeAfterCancelled</fullName>
        <active>true</active>
        <description>Q2 2016 Release : Status cannot be changed once it has been set to Cancelled.</description>
        <errorConditionFormula>AND ( NOT( $User.BypassVR__c ) , ISPICKVAL(PRIORVALUE(Status__c), &apos;3b. Cancelled&apos;), ISCHANGED( Status__c ) )</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Status cannot be changed once it has been set to Cancelled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Q216_StatusChangeWithoutFSBTeamMember</fullName>
        <active>true</active>
        <description>Q2 2016 Release : Please create FSB Team Members.</description>
        <errorConditionFormula>AND( NOT($User.BypassVR__c) , OR(ISPICKVAL(Status__c , &apos;2. Go/NoGo Decision Pending&apos;) , ISPICKVAL(Status__c , &apos;3a. Document in progress&apos;) ,  ISPICKVAL(Status__c , &apos;4. Pending Final Approval&apos;) , ISPICKVAL(Status__c , &apos;5. Execution&apos;) , ISPICKVAL(Status__c , &apos;6. Completed&apos;)),   NumberOfFSBTeamMembers__c = 0)</errorConditionFormula>
        <errorMessage>Please create FSB Team Members.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Q216_Technical_Risk_number</fullName>
        <active>true</active>
        <description>Q2 2016 Release : If &quot;Who support cost = LOB Technical Risk&quot; and &quot;Global Cost of Modification&quot; &gt; 100K dollars &quot;Technical Risk Number&quot; field is Mandatory</description>
