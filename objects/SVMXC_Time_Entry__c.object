    <validationRules>
        <fullName>SVMX_Ensure_Start_Time_Before_End_Time</fullName>
        <active>true</active>
        <description>Prevent negative entries on time entries by ensuring that start time is before end time</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),End_Date_Time__c &lt;= Start_Date_Time__c, ISNULL(Time_Entry_Request__c))</errorConditionFormula>
        <errorDisplayField>Activity__c</errorDisplayField>
        <errorMessage>Your start date and time must be earlier than your end date and time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SVMX_NonTravel_Time_Include_Miles_Driven</fullName>
        <active>true</active>
        <description>Ensure that if Activity Type is not Travel Time that Miles Driven may not have a value</description>
        <errorConditionFormula>AND(
NOT($User.BypassVR__c),
Not(contains(text(Activity__c),&quot;Travel&quot; )) &amp;&amp;  (Miles_Driven__c &gt;0))</errorConditionFormula>
        <errorDisplayField>Activity__c</errorDisplayField>
        <errorMessage>Miles driven must be entered using the Travel Time activity type</errorMessage>
    </validationRules>
