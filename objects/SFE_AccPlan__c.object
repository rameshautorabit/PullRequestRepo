        <active>true</active>
        <description>SR0 - Make sure selected currency is the same as account currency</description>
        <errorConditionFormula>NOT( $User.BypassVR__c ) 
&amp;&amp; 
TEXT(CurrencyIsoCode) &lt;&gt; TEXT( Account__r.CurrencyIsoCode )</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>Currency should be the same as the one selected at account level.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>APL_VR02_AccountControl</fullName>
        <active>true</active>
        <description>July 2011: Ensures that the Account has not been marked for deletion</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp; (ISNEW() || ISCHANGED(  Account__c )) &amp;&amp;( Account__r.ToBeDeleted__c = TRUE )</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>This Account has been marked for deletion, please choose another one.</errorMessage>
    </validationRules>
