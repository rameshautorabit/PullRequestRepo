    <allowInChatterGroups>false</allowInChatterGroups>
    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <deploymentStatus>Deployed</deploymentStatus>
    <description>SR0+ - Custom Object for capturing details of those Salespersons that contributed to a given Opportunity.  The user is able to capture the Contribution Percentage for each Salesperson.  The sytem determines the &apos;amount&apos; of their contribution based on this percentage.</description>
    <enableActivities>false</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <enableHistory>true</enableHistory>
    <enableLicensing>false</enableLicensing>
    <enableReports>true</enableReports>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>ControlledByParent</externalSharingModel>
    <fields>
        <fullName>Approved__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <label>Approved?</label>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>ContributionAmount__c</fullName>
        <description>SR0+ - Contribution Amount for Sales Contributor. Calculates the Amount contributed by a User for an Opportunity.</description>
        <externalId>false</externalId>
        <formula>Contribution__c * Opportunity__r.Amount</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Contribution Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Contribution__c</fullName>
        <defaultValue>0</defaultValue>
        <description>SR0+ - Contribution% for Sales Contributor. Stores the percentage of contribution of a user on an opportunity.</description>
        <externalId>false</externalId>
        <label>Contribution%</label>
        <precision>3</precision>
        <required>true</required>
        <scale>0</scale>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>DM_OpportunityCloseDate__c</fullName>
        <externalId>false</externalId>
        <formula>Opportunity__r.CloseDate</formula>
        <label>DM_OpportunityCloseDate</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>DM_OpportunityStage__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT(Opportunity__r.StageName)</formula>
        <label>DM_OpportunityStage</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>DM_TotalContributionPercentage__c</fullName>
        <externalId>false</externalId>
        <formula>Opportunity__r.TECH_TotalContributionPercentage__c</formula>
        <label>DM_TotalContributionPercentage</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>DM_UserActive__c</fullName>
        <externalId>false</externalId>
        <formula>User__r.IsActive</formula>
        <label>DM_UserActive</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>InternalOrganization__c</fullName>
        <description>SR0+ - Internal Organization for Sales Contributor. Takes the Department from the User and shows it as Internal Organization to make the related list on Opportunity more clear.</description>
        <externalId>false</externalId>
        <formula>User__r.Department</formula>
        <label>Internal Organization</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>IsOpportunityOwner__c</fullName>
        <externalId>false</externalId>
        <formula>Opportunity__r.OwnerId =  User__r.Id</formula>
        <label>IsOpportunityOwner</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Opportunity__c</fullName>
