    <validationRules>
        <fullName>ORGS_VR_NOV16_AllowOnlyCMIInformedRoles</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),BusinessRiskEscalationEntity__r.VerticalisedCustomerCare__c=True,ISPICKVAL( BusinessRiskEscalationEntity__r.Location_Type__c , &apos;Customer Care Center&apos;),NOT(ISPICKVAL(Role__c,&apos;CMI Informed&apos;)) )</errorConditionFormula>
        <errorMessage>You can add only CMI informed role users when the organisation is Verticalised Customer Care</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ORGS_VR_OCT15I2P01_Potential_SafetyIssue</fullName>
        <active>true</active>
        <description>October 2015 release</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),ISPICKVAL(Role__c , &apos;Potential Safety Issue Reviewer&apos;),NOT(OR(ISPICKVAL(BusinessRiskEscalationEntity__r.Location_Type__c,&apos;Country Front Office HQ&apos;),ISPICKVAL(BusinessRiskEscalationEntity__r.Location_Type__c,&apos;Country Front Office Other&apos;),ISPICKVAL(BusinessRiskEscalationEntity__r.Location_Type__c,&apos;Country Front Office Verticalized&apos;))))</errorConditionFormula>
        <errorMessage>In order to create this role the Organization location type should be &apos;Country Front Office HQ&apos; or &apos;Country Front Office Verticalized&apos; or &apos;Country Front Office Other&apos;</errorMessage>
    </validationRules>
