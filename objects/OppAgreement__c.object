    <validationRules>
        <fullName>AGR_VR01_DeliverValidateAgreement</fullName>
        <active>true</active>
        <description>SR0+ - Validation rule to ensure that users cannot create an Agreement with &quot;7 - Deliver &amp; Validate&quot; Phase/Sales Stage</description>
        <errorConditionFormula>AND ( ISNEW() , $User.BypassVR__c = False,  ISPICKVAL( PhaseSalesStage__c ,&apos;7 - Deliver &amp; Validate&apos;))</errorConditionFormula>
        <errorMessage>A Frame Opportunity should be associated with this Agreement prior to updating the Phase/Stage to &quot;7 - Deliver &amp; Validate&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AGR_VR02_FromDateLowerThanUntilDate</fullName>
        <active>true</active>
        <description>SR0+ - validation rule to ensure end date cannot be before the start date.</description>
        <errorConditionFormula>AND ($User.BypassVR__c = False, Agreementvalidfrom__c &gt; Agreementvaliduntil__c)</errorConditionFormula>
        <errorDisplayField>Agreementvalidfrom__c</errorDisplayField>
        <errorMessage>The FROM date cannot be before the UNTIL date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AGR_VR03_AccountControl</fullName>
        <active>true</active>
        <description>July 2011: Ensures that the Account has not been marked for deletion</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp;  ( ISNEW() || ISCHANGED(  AccountName__c  )) &amp;&amp;(  AccountName__r.ToBeDeleted__c = TRUE )</errorConditionFormula>
        <errorDisplayField>AccountName__c</errorDisplayField>
        <errorMessage>This Account has been marked for deletion, please choose another one.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AGR_VR05_RFPreceived_Control</fullName>
        <active>true</active>
        <description>December 2011 release: mandatory field of the Opportunity must be first completed at the OEM Agreement level so the update of all the opportunities can embed the values for these fields.</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp; ( ISBLANK(RFPReceivedOn__c)  ) &amp;&amp;  
ISPICKVAL( Opportunity_Type__c , &apos;Solutions&apos;) &amp;&amp; ( ISPICKVAL( PhaseSalesStage__c , &apos;6 - Negotiate &amp; Win&apos;) || ISPICKVAL(PhaseSalesStage__c , &apos;7 - Deliver &amp; Validate&apos;) )</errorConditionFormula>
        <errorDisplayField>RFPReceivedOn__c</errorDisplayField>
        <errorMessage>RFP received must be selected before stage 6 - Negotiate &amp; Win.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AGR_VR06_SolutionCenter_Control</fullName>
        <active>true</active>
        <description>December 2011 release: mandatory field of the Opportunity must be first completed at the OEM Agreement level so the update of all the opportunities can embed the values for these fields.</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp; (ISBLANK(SolutionCenter__c) ) &amp;&amp; 
ISPICKVAL( Opportunity_Type__c , &apos;Solutions&apos;) &amp;&amp; ( ISPICKVAL( PhaseSalesStage__c , &apos;6 - Negotiate &amp; Win&apos;) || ISPICKVAL(PhaseSalesStage__c , &apos;7 - Deliver &amp; Validate&apos;) )</errorConditionFormula>
        <errorDisplayField>SolutionCenter__c</errorDisplayField>
        <errorMessage>Solution Center must be selected before stage 6 - Negotiate &amp; Win.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AGR_VR07_ReasonForLost</fullName>
        <active>true</active>
        <description>December 2011 release: mandatory field of the Opportunity must be first completed at the OEM Agreement level so the update of all the opportunities can embed the values for these fields.</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp; (ISPICKVAL(Reason__c,&apos;&apos;) ) &amp;&amp;  
ISPICKVAL( Opportunity_Type__c  , &apos;Solutions&apos;) 
&amp;&amp; ( ISPICKVAL( PhaseSalesStage__c , &apos;0 - Closed&apos;) &amp;&amp; ISPICKVAL(Status__c, &apos;Lost&apos;) )</errorConditionFormula>
        <errorDisplayField>Reason__c</errorDisplayField>
        <errorMessage>The Reason is mandatory for the stage ‘0 – Closed’ and the status ‘Lost’</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AGR_VR08_WinningCompetitorForLost</fullName>
        <active>true</active>
        <description>December 2011 release: mandatory field of the Opportunity must be first completed at the OEM Agreement level so the update of all the opportunities can embed the values for these fields.</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp; (ISBLANK(WinningCompetitor__c) ) &amp;&amp; 
ISPICKVAL( Opportunity_Type__c , &apos;Solutions&apos;) &amp;&amp; ( ISPICKVAL( PhaseSalesStage__c , &apos;0 - Closed&apos;) &amp;&amp; ISPICKVAL(Status__c, &apos;Lost&apos;) )</errorConditionFormula>
        <errorDisplayField>WinningCompetitor__c</errorDisplayField>
        <errorMessage>A Winning Competitor is mandatory for the stage ‘0 – Closed’ and the status ‘Lost’</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AGR_VR09_CreationOEMstageAllowed</fullName>
        <active>true</active>
        <description>Prevent from creating the OEM Agreement at stage 5, 6 and 7 (for both Standard and Solution)</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp;  ISNEW() &amp;&amp; (ISPICKVAL(PhaseSalesStage__c , &apos;5 - Prepare &amp; Bid&apos;)  || ISPICKVAL(PhaseSalesStage__c ,&apos;6 - Negotiate &amp; Win&apos;) || ISPICKVAL(PhaseSalesStage__c ,&apos;7 - Deliver &amp; Validate&apos;) || ISPICKVAL(PhaseSalesStage__c ,&apos;0 - Closed&apos;) )</errorConditionFormula>
        <errorDisplayField>PhaseSalesStage__c</errorDisplayField>
        <errorMessage>The creation of agreement is not allowed above stage &quot;4 - Influence &amp; Develop&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AGR_VR_APR15SLS01_DurationIntervalNoChan</fullName>
        <active>true</active>
        <description>Prevents Duration/Interval to be changed once the series are created</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp; AND(OR(ISCHANGED(Agreement_Duration_in_month__c),ISCHANGED(FrameOpportunityInterval__c),ISCHANGED(Opportunity_Type__c), ISCHANGED(AgreementType__c)),  TECH_IsSeriesCreated__c=TRUE)</errorConditionFormula>
        <errorMessage>Agreement Duration/Interval/Opportunity Type/Agreement Type cannot be changed as the series are created</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AGR_VR_APR15SLS03_OtherCompetitorManda</fullName>
        <active>true</active>
        <description>When Competitor is Selected as Other, User has to provide the Competitor Name, Otherwise Other Name field should be blank.</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), OR(AND(CONTAINS(LOWER( WinningCompetitor__r.Name), LOWER($Label.CL00519) ), ISBLANK(OtherName__c)), AND(NOT(CONTAINS(LOWER(WinningCompetitor__r.Name), LOWER($Label.CL00519))), NOT(ISBLANK(OtherName__c)))))</errorConditionFormula>
        <errorDisplayField>OtherName__c</errorDisplayField>
        <errorMessage>Other Competitor Name should be populated if the competitor is selected as Other.</errorMessage>
    </validationRules>
