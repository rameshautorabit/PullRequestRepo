    <validationRules>
        <fullName>Abandoned_SubStatus_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
ISCHANGED(Substatus__c), 
TEXT(PRIORVALUE(Substatus__c)) = &apos;Abandoned&apos;,OR(ISPICKVAL( Substatus__c ,&apos;Awaiting Customer Review&apos;),ISPICKVAL( Substatus__c ,&apos;Awaiting Review/Activation&apos;)))</errorConditionFormula>
        <errorMessage>You can set the status to only In progress.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Active_Status_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),ISCHANGED(Status__c),ISPICKVAL( Status__c ,&apos;Active&apos;),
NOT(OR(TEXT(PRIORVALUE(Status__c))=&apos;Expired&apos;,AND(TEXT(PRIORVALUE(Status__c))=&apos;Draft&apos;,OR(TEXT(PRIORVALUE(Substatus__c))=&apos;In Progress&apos; , TEXT(PRIORVALUE(Substatus__c))=&apos;Awaiting Review/Activation&apos;) ))))</errorConditionFormula>
        <errorMessage>The Status can be set to Active only from Draft- Awaiting Review/Activation or from In Progress</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Draft_Status_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),
ISCHANGED(Status__c), 
TEXT(PRIORVALUE(Status__c)) = &apos;Draft&apos;,OR(ISPICKVAL( Status__c ,&apos;Terminated&apos;),ISPICKVAL( Status__c ,&apos;Expired&apos;)))</errorConditionFormula>
        <errorMessage>Status cannot be changed from Draft to Expired or Terminated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_Check_with_Active_status</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),ISPICKVAL( Status__c ,&apos;Active&apos;),ISBLANK( ContractEnddate__c ))</errorConditionFormula>
        <errorDisplayField>ContractEnddate__c</errorDisplayField>
        <errorMessage>The contract End date is mandatory for Active contracts.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_Check_with_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),ContractStartdate__c &gt; ContractEnddate__c)</errorConditionFormula>
        <errorDisplayField>ContractEnddate__c</errorDisplayField>
        <errorMessage>The contract End date cannot be less than the Contract Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Expired_Status_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
ISCHANGED(Status__c), 
TEXT(PRIORVALUE(Status__c)) = &apos;Expired&apos;,OR(ISPICKVAL( Status__c ,&apos;Terminated&apos;),ISPICKVAL( Status__c ,&apos;Draft&apos;)))</errorConditionFormula>
        <errorMessage>Status cannot be changed from Expired to Draft or Terminated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>InProgress_SubStatus_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
ISCHANGED(Substatus__c), 
OR(TEXT(PRIORVALUE(Substatus__c)) = &apos;Awaiting Customer Review&apos;,TEXT(PRIORVALUE(Substatus__c)) = &apos;Awaiting Review/Activation&apos;),ISPICKVAL( Substatus__c ,&apos;In Progress&apos;))</errorConditionFormula>
        <errorMessage>You cannot go back to In progress.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Terminated_Other_Reason_must_be_filled</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),ISPICKVAL(TerminatedReason__c,&apos;Other&apos;),ISBLANK(TerminatedReasonOther__c))</errorConditionFormula>
        <errorDisplayField>TerminatedReasonOther__c</errorDisplayField>
        <errorMessage>Please specify the reason for selecting &quot;Other&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Terminated_Status_Check</fullName>
        <active>true</active>
        <description>This Validation rule is to check the Terminated reason is filled or not upon the termination of the contract.</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),ISCHANGED(Status__c),ISPICKVAL( Status__c ,&apos;Terminated&apos;),ISBLANK(TEXT(TerminatedReason__c )))</errorConditionFormula>
        <errorDisplayField>TerminatedReason__c</errorDisplayField>
        <errorMessage>Please fill in the Terminated Reason for the selected status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TerminatedtoDraftAndExpired_Status_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
ISCHANGED(Status__c), 
TEXT(PRIORVALUE(Status__c)) = &apos;Terminated&apos;,OR(ISPICKVAL( Status__c ,&apos;Draft&apos;),ISPICKVAL( Status__c ,&apos;Expired&apos;)))</errorConditionFormula>
        <errorMessage>Status cannot be changed from Terminated to Draft or Expired.</errorMessage>
    </validationRules>
