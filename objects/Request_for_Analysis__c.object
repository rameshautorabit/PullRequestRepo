    <validationRules>
        <fullName>RA_VR_APROFM01_RequiredNumber</fullName>
        <active>true</active>
        <description>BFOI2P-953-
Add a filter based on date-code</description>
        <errorConditionFormula>IF(ISBLANK(FinalManufacturingDateCode__c),false,if(AND(ISNUMBER(FinalManufacturingDateCode__c),LEN(FinalManufacturingDateCode__c) =6),if((value(LEFT(FinalManufacturingDateCode__c, 2)))=20,if((value(RIGHT(FinalManufacturingDateCode__c, 2))) &gt;53,true,false),if((value(LEFT(FinalManufacturingDateCode__c, 2)))=19,if((value(RIGHT(FinalManufacturingDateCode__c, 2))) &gt; 53,true,false),true)),true) &amp;&amp; NOT($User.BypassVR__c))</errorConditionFormula>
        <errorDisplayField>FinalManufacturingDateCode__c</errorDisplayField>
        <errorMessage>Please enter valid number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RA_VR_APROFM02_InitialManufacturingAsNum</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISBLANK(InitialManufacturingDateCode__c),false,if(AND(ISNUMBER(InitialManufacturingDateCode__c),LEN(FinalManufacturingDateCode__c) =6 ),if((value(LEFT(InitialManufacturingDateCode__c, 2)))=20,if((value(RIGHT(InitialManufacturingDateCode__c, 2))) &gt;53,true,false),if((value(LEFT(InitialManufacturingDateCode__c, 2)))=19,if((value(RIGHT(InitialManufacturingDateCode__c, 2))) &gt; 53,true,false),true)),true) &amp;&amp; NOT($User.BypassVR__c))</errorConditionFormula>
        <errorDisplayField>InitialManufacturingDateCode__c</errorDisplayField>
        <errorMessage>Please enter valid number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RFA_VR_DateValidation</fullName>
        <active>true</active>
        <description>Validation for start and end date of return</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),OR(
 StartDateofReturn__c &gt;  EndDateReturn__c

))</errorConditionFormula>
        <errorMessage>End date must be later than Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_RFAMinimalStartDateValidation</fullName>
        <active>true</active>
        <description>If RFA startdateof return is less than CLR617OFMRFAMinimumStartDate, should not be accepted to save the record</description>
        <errorConditionFormula>DATEVALUE($Label.CLR617OFMRFAMinimumStartDate) &gt;  StartDateofReturn__c &amp;&amp; NOT($User.BypassVR__c)</errorConditionFormula>
        <errorMessage>Start Date of Return should always be greater than the Go-Live Date</errorMessage>
    </validationRules>
