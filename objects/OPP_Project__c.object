    <validationRules>
        <fullName>OPP_PRJ_VR_MAR16SLS01_MandatoryFields</fullName>
        <active>true</active>
        <description>Make fields mandatory in VR as well not just in layouts, inorder to ensure connectors as well pass data</description>
        <errorConditionFormula>AND(
	NOT( $User.BypassVR__c),
	OR(
		ISBLANK(ProjectEndUserInvestor__c),
		ISBLANK(CountryofDestination__c), 
		ISBLANK(TEXT(ProjectStatus__c)),
		ISBLANK(EstimateProjectCloseDate__c),
		ISBLANK(TEXT(MarketSegment__c)), 
		ISBLANK(SEPotentialProjectAmount__c) 
	)	
)</errorConditionFormula>
        <errorMessage>Please review the Master Project and fill all required fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP_PRJ_VR_OCT15SLS01_EndUserControl</fullName>
        <active>true</active>
        <description>Ensures that the Account has not been marked for deletion</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp; (ISNEW() || ISCHANGED( ProjectEndUserInvestor__c)) &amp;&amp; (  ProjectEndUserInvestor__r.ToBeDeleted__c  = TRUE )</errorConditionFormula>
        <errorDisplayField>ProjectEndUserInvestor__c</errorDisplayField>
        <errorMessage>A Master Project cannot be created for a To Be Deleted/Merged account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP_PRJ_VR_OCT15SLS03_NonNegativeAmt</fullName>
        <active>true</active>
        <description>Display an error message when the user enters a negative value for Amount</description>
        <errorConditionFormula>NOT( $User.BypassVR__c ) 
&amp;&amp; ( SEPotentialProjectAmount__c &lt;0 ||  TotalProjectInvestmentAmount__c &lt;0 )</errorConditionFormula>
        <errorMessage>Please enter non-negative numbers for Amount</errorMessage>
    </validationRules>
