    <validationRules>
        <fullName>TM_VR_OCT13PRM001_IsProgramManager</fullName>
        <active>true</active>
        <description>Determine if the user chosen is a program manager when Partner Program lookup is set</description>
        <errorConditionFormula>NOT( $User.BypassVR__c ) &amp;&amp;  NOT(ISBLANK( PartnerProgram__c )) &amp;&amp; NOT( OR(User__r.ProgramManager__c, User__r.ProgramAdministrator__c  ))</errorConditionFormula>
        <errorMessage>You can add Program Managers/Program Administrators only as members of program team</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TM_VR_OCT13PRM002_IsTeamMembrProgrmOwner</fullName>
        <active>true</active>
        <description>PRM OCT 13 REL: Determine if the team member being added is same as Program Owner</description>
        <errorConditionFormula>NOT( $User.BypassVR__c )  &amp;&amp;  NOT(ISBLANK( PartnerProgram__c )) &amp;&amp;  PartnerProgram__r.Owner:User.Id =  User__c</errorConditionFormula>
        <errorMessage>You cannot chose Program Owner as Program team member</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
