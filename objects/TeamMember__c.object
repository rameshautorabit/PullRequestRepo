                    <default>false</default>
                    <label>Edit</label>
                </value>
                <value>
                    <fullName>All</fullName>
                    <default>false</default>
                    <label>All</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>PartnerProgram__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Partner Program to which the team members we will be added</description>
        <externalId>false</externalId>
        <label>Partner Program</label>
        <lookupFilter>
            <active>true</active>
            <errorMessage>Program team members are allowed for Country / Cluster programs only.</errorMessage>
            <filterItems>
                <field>PartnerProgram__c.RecordTypeId</field>
                <operation>equals</operation>
                <value>Country Partner Program, Cluster Partner Program</value>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>PartnerProgram__c</referenceTo>
        <relationshipLabel>Program Team</relationshipLabel>
        <relationshipName>ProgramTeamMembers</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>User__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Member of the team</description>
        <externalId>false</externalId>
        <label>User</label>
        <lookupFilter>
            <active>true</active>
            <booleanFilter>(1 OR 2) AND 3</booleanFilter>
            <errorMessage>Only Program Managers / Program Administrator can be added to Program Team</errorMessage>
            <filterItems>
                <field>User.ProgramManager__c</field>
                <operation>equals</operation>
                <value>True</value>
            </filterItems>
            <filterItems>
                <field>User.ProgramAdministrator__c</field>
                <operation>equals</operation>
                <value>True</value>
            </filterItems>
            <filterItems>
                <field>User.ContactId</field>
                <operation>equals</operation>
                <value></value>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>User</referenceTo>
        <relationshipName>TeamMemberUser</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
