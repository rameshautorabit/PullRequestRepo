    <validationRules>
        <fullName>RC_VR01_Prevent_Supplier_RC</fullName>
        <active>true</active>
        <description>OCT 2015 Release : Supplier Root Cause cannot be created on a Schneider Problem.</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),RelatedProblem__r.RecordType.DeveloperName = &apos;Schneider&apos; , RecordType.DeveloperName = &apos;Supplier&apos;)</errorConditionFormula>
        <errorMessage>Supplier Root Cause cannot be created on a Schneider Problem.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RC_VR_OCT15I2P01_RCCrIfPrblmInPrognPrble</fullName>
        <active>true</active>
        <description>I2P : OCT 15 Release. Root Cause can be created only when the Related Problem Status is  In Progress and Schneider Problem Leader has been Nominated</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),  RecordType.DeveloperName == &apos;Schneider&apos; , ISNEW(), NOT( ISPICKVAL( RelatedProblem__r.Status__c , &quot;In Progress&quot;)), ISBLANK( RelatedProblem__r.ProblemLeader__c ) )</errorConditionFormula>
        <errorMessage>Root Cause can only be created when the related Problem Status is In Progress and Schneider Problem Leader is nominated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RC_VR_OCT19I2P01_RCCrIfOpStatIsCompleted</fullName>
        <active>true</active>
        <description>I2P : OCT 19 Release. Operational Root Cause can only be be created when the Related Problem Status of Operational Root Cause is not completed.</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), ISNEW(),ISPICKVAL( RelatedProblem__r.StatusOfRootCauseAnalysis__c , &quot;Completed&quot;),OR( ISPICKVAL( RootCauseType__c,&quot;Operational Root Cause - Problem creation&quot;),ISPICKVAL(RootCauseType__c,&quot;Operational Root Cause - Problem non detection&quot;)))</errorConditionFormula>
        <errorMessage>Operational Root Cause can only be be created when the Related Problem Status of Operational Root Cause is not completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RC_VR_OCT19I2P01_RCCrIfSyStatIsCompleted</fullName>
        <active>true</active>
        <description>I2P : OCT 19 Release. Systemic Root Cause can only be be created when the Related Problem Status of Systemic Root Cause is not completed.</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), ISNEW(), ISPICKVAL(  RelatedProblem__r.StatusofSystemicRootCause__c, &quot;Completed&quot;),OR( ISPICKVAL( RootCauseType__c,&quot;Systemic Root Cause - Problem creation&quot;),ISPICKVAL(RootCauseType__c,&quot;Systemic Root Cause - Problem non detection&quot;)))</errorConditionFormula>
        <errorMessage>Systemic Root Cause can only be be created when the Related Problem Status of Systemic Root Cause is not completed.</errorMessage>
    </validationRules>
