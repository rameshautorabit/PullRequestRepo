    <validationRules>
        <fullName>VR_Q216_BothSparesFilled</fullName>
        <active>true</active>
        <description>Q2 2016 Release : Please fill in either Spare part reference field or Other Spare part field, not both.</description>
        <errorConditionFormula>AND( NOT($User.BypassVR__c) , NOT( ISBLANK( SparePartReference__c ) ) , NOT( ISBLANK( GenericReferenceSpare__c ) ))</errorConditionFormula>
        <errorMessage>Please fill in either Spare part reference field or Other Spare part field, not both.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Q216_BothSparesNotFilled</fullName>
        <active>true</active>
        <description>Q2 2016 Release : Please fill in either Spare part reference field or Other Spare part field.</description>
        <errorConditionFormula>AND( NOT($User.BypassVR__c) , ISBLANK( SparePartReference__c ) , ISBLANK( GenericReferenceSpare__c ))</errorConditionFormula>
        <errorMessage>Please fill in either Spare part reference field or Other Spare part field.</errorMessage>
    </validationRules>
