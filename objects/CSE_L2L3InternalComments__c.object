    <deploymentStatus>Deployed</deploymentStatus>
    <description>ACC - To allow L2 and L3 agents to share critical information without sharing it with Level 1 and other services user (Sales, Servicesâ€¦), a L2/L3 internal comments functionality has been added.</description>
    <enableActivities>false</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <enableHistory>false</enableHistory>
    <enableLicensing>false</enableLicensing>
    <enableReports>true</enableReports>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>Private</externalSharingModel>
    <fields>
        <fullName>Activity__c</fullName>
        <description>September Release - Field to explain the content of the action done by an agent on the case.,</description>
        <externalId>false</externalId>
        <label>Activity</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Customer Interaction</fullName>
                    <default>false</default>
                    <label>Customer Interaction</label>
                </value>
                <value>
                    <fullName>Problem Simulation</fullName>
                    <default>false</default>
                    <label>Problem Simulation</label>
                </value>
                <value>
                    <fullName>Investigation / SME</fullName>
                    <default>false</default>
                    <label>Investigation / SME</label>
                </value>
                <value>
                    <fullName>Investigation / KB search</fullName>
                    <default>false</default>
                    <label>Investigation / KB search</label>
                </value>
                <value>
                    <fullName>Offline Case Documentation</fullName>
                    <default>false</default>
                    <label>Offline Case Documentation</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>CaseIcon__c</fullName>
        <externalId>false</externalId>
        <formula>Case__r.Icons__c</formula>
        <label>Case Icon</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CaseOwner__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISNULL(Case__r.Owner:User.Id), Case__r.Owner:Queue.QueueName,  Case__r.Owner:User.FirstName&amp;&apos; &apos;&amp;Case__r.Owner:User.LastName )</formula>
        <label>Case Owner</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Case__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>ACC - Displays Related Case Number.</description>
        <externalId>false</externalId>
        <label>Case</label>
        <referenceTo>Case</referenceTo>
        <relationshipName>L2_L3_internal_comments</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Comments__c</fullName>
        <description>ACC - Saving L2/L3 critical comments Related to cases</description>
        <externalId>false</externalId>
        <label>Comments</label>
        <length>32000</length>
        <trackTrending>false</trackTrending>
        <type>Html</type>
        <visibleLines>25</visibleLines>
    </fields>
    <fields>
        <fullName>Duration__c</fullName>
        <description>September Release - Field to store the time, spent in minutes, on an activity.</description>
        <externalId>false</externalId>
        <inlineHelpText>Time in minutes spent on an activity</inlineHelpText>
        <label>Duration</label>
        <precision>4</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>OdasevaArchivedCaseId__c</fullName>
        <description>This field is used by Odaseva archiving solution in order to store the case id of archived cases. Required in 18-char format</description>
        <externalId>true</externalId>
        <label>OdasevaArchivedCaseId</label>
        <length>18</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>OdasevaArchivedCaseNumber__c</fullName>
        <externalId>false</externalId>
        <label>OdasevaArchivedCaseNumber</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>OdasevaArchivedCaseURL__c</fullName>
        <externalId>false</externalId>
        <formula>IF(AND(NOT(ISBLANK(OdasevaArchivedCaseId__c)),NOT(ISBLANK(OdasevaArchivedCaseNumber__c))),HYPERLINK($Label.OdasevaArchivedCaseUrlPrefix &amp; OdasevaArchivedCaseId__c, OdasevaArchivedCaseNumber__c),&quot;No archived case to display&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Odaseva Archived Case</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Escalation Internal Comment</label>
    <listViews>
        <fullName>Escalation_Internal_Comments_View</fullName>
        <columns>Case__c</columns>
        <columns>OWNER.FIRST_NAME</columns>
        <columns>OWNER.LAST_NAME</columns>
        <columns>Activity__c</columns>
        <columns>Duration__c</columns>
        <columns>Comments__c</columns>
        <filterScope>Mine</filterScope>
        <label>My Escalation Internal Comments</label>
        <language>en_US</language>
        <sharedTo>
            <group>ECOBLD_GBL_BMS_ALL_L3</group>
        </sharedTo>
    </listViews>
    <nameField>
        <displayFormat>LIC-{0000}</displayFormat>
        <label>L2/L3 Internal Comments Name</label>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Escalation Internal Comments</pluralLabel>
    <searchLayouts>
        <excludedStandardButtons>ChangeOwner</excludedStandardButtons>
        <excludedStandardButtons>Accept</excludedStandardButtons>
        <lookupDialogsAdditionalFields>Comments__c</lookupDialogsAdditionalFields>
        <lookupDialogsAdditionalFields>CREATEDBY_USER</lookupDialogsAdditionalFields>
        <lookupDialogsAdditionalFields>CREATED_DATE</lookupDialogsAdditionalFields>
        <searchFilterFields>NAME</searchFilterFields>
        <searchResultsAdditionalFields>Case__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>Comments__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>CREATEDBY_USER</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>CREATED_DATE</searchResultsAdditionalFields>
    </searchLayouts>
    <sharingModel>Private</sharingModel>
    <validationRules>
        <fullName>LIC_VR01_ActivityTimeMandatory</fullName>
        <active>true</active>
        <description>SEPTEMBER RELEASE - When an agent select the activity he has to fill the time duration he has spent on that activity, and when he fill the time, he has to select the Activity, the time spent for.</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), OR(AND(NOT(ISPICKVAL(Activity__c, &quot;&quot;)),ISBLANK(Duration__c)), AND(ISPICKVAL(Activity__c,&quot;&quot;),NOT(ISBLANK(Duration__c)))))</errorConditionFormula>
        <errorMessage>Both Activity and Duration fields are mandatory if value selected/ typed in for any of them.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LIC_VR_JUN16CCC01_CaseArchived</fullName>
        <active>true</active>
        <description>This VR will execute when Case is archived</description>
        <errorConditionFormula>AND( ! $User.BypassVR__c ,OdasevaArchivedCaseURL__c !=&quot;No archived case to display&quot;)</errorConditionFormula>
        <errorMessage>This record can&apos;t be modified as the associated Case record has been archived</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
    <webLinks>
        <fullName>NewEscIntComment</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <encodingKey>UTF-8</encodingKey>
        <height>600</height>
        <linkType>url</linkType>
        <masterLabel>New</masterLabel>
        <openType>noSidebar</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>lightning/cmp/c__LCMP_CCCEscIntCommentCreate?c__caseNumber={!Case.Id}&amp;c__owner={!Case.OwnerId}</url>
    </webLinks>
</CustomObject>
