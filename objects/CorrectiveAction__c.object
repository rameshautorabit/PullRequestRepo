    <validationRules>
        <fullName>CRA_VR01_OwnerFieldMandatory</fullName>
        <active>true</active>
        <description>Owner field is mandatory for Corrective Action</description>
        <errorConditionFormula>AND
(
	NOT($User.BypassVR__c),
	$RecordType.Name !=&apos;Supplier&apos;,
	ISBLANK( Owner__c)
)</errorConditionFormula>
        <errorDisplayField>Owner__c</errorDisplayField>
        <errorMessage>The corresponding Owner for the Corrective Action is not found. Owner must be associated to a Corrective Action</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRA_VR02_Prevent_Supplier_CRA</fullName>
        <active>true</active>
        <description>OCT 2015 Release : Supplier Corrective Action cannot be created on a Schneider Problem.</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),RelatedProblem__r.RecordType.DeveloperName = &apos;Schneider&apos; , RecordType.DeveloperName = &apos;Supplier&apos;)</errorConditionFormula>
        <errorMessage>Supplier Corrective Action cannot be created on a Schneider Problem.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRA_VR_JUN16I2P01_CheckSafeDomain</fullName>
        <active>true</active>
        <description>Check the safe domain in AdditionalContact__c</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
 VLOOKUP($ObjectType.SafeDomains__c.Fields.Name, $ObjectType.SafeDomains__c.Fields.Name,SUBSTITUTE(AdditionalContact__c,left(AdditionalContact__c,FIND(&quot;@&quot;, AdditionalContact__c)), &quot;&quot;))!=SUBSTITUTE(AdditionalContact__c,left(AdditionalContact__c,FIND(&quot;@&quot;, AdditionalContact__c)), &quot;&quot;) )</errorConditionFormula>
        <errorDisplayField>AdditionalContact__c</errorDisplayField>
        <errorMessage>Please enter valid email address within a Schneider safe domain.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRA_VR_OCT15I2P01_DueDateGtStartDate</fullName>
        <active>true</active>
        <description>I2P: OCT15RELEASE. Due date should not be before Creation Date</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), IF(ISNEW(),TODAY() &gt; DueDate__c, DATEVALUE( CreatedDate ) &gt; DueDate__c ))</errorConditionFormula>
        <errorDisplayField>DueDate__c</errorDisplayField>
        <errorMessage>Due date should not be before Creation Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRA_VR_OCT15I2P02_CRACreifPrblmInPrognPr</fullName>
        <active>true</active>
        <description>I2P: OCT15 Release.
Corrective Action can be created only when the Related Problem is In Progress and Problem Leader has been Nominated</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),  ISNEW() , NOT( ISPICKVAL( RelatedProblem__r.Status__c , &quot;In Progress&quot;)), ISBLANK( RelatedProblem__r.ProblemLeader__c ) )</errorConditionFormula>
        <errorMessage>Corrective Action can only be created when the related Problem Status is In Progress and Schneider Problem Leader is nominated.</errorMessage>
    </validationRules>
