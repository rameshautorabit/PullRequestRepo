    <validationRules>
        <fullName>CSI_VR_OCT18I2P01_propertyDamage</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), ISPICKVAL(PropertyDamage__c, &apos;None Identified&apos;),
OR((AmountOfCustomerClaim__c &gt; 0), NOT(ISBLANK(TEXT(TypeOfPropertyDamage__c))), NOT(ISBLANK(TEXT(CauseOfPropertyDamage__c)))))</errorConditionFormula>
        <errorMessage>Property damage cannot be none identified if Type of Property damage, Amount of Customer Claim or Cause of property damage have a value. Please remove the value on these fields or change the Property damage value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CSI_VR_Q316I2P01_PreventUpdteROSA</fullName>
        <active>true</active>
        <description>Prevent the &apos;Related Offer Safety Alert&apos; update, When moving a CSI to &apos;Confirmed as Safety Risk&apos;, do not allow to populate a Problem with severity different to &apos;Safety Related&apos; in the  &apos;Related Offer Safety Alert&apos; field</description>
        <errorConditionFormula>AND(
		NOT($User.BypassVR__c), ISPICKVAL(CSIStatus__c,&apos;Confirmed as safety risk&apos;), 
		NOT(OR(ISPICKVAL(RelatedOfferSafetyAlert__r.Severity__c,&apos;Safety related&apos;), RelatedOfferSafetyAlert__r.LatestOSACDecision__c =&apos;No-go&apos;))
	)</errorConditionFormula>
        <errorMessage>You can select either a PRB with Severity=Safety related or a PRB with Latest OSAC decision=No-Go</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CSI_VR_SEP12I2P01_TypInjReqWhnBIInjNTnon</fullName>
        <active>true</active>
        <description>I2P September Release 2012: Type of Injury is Required if Bodily Injury is other than none affected.</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
NOT(OR(ISPICKVAL( BodilyInjury__c ,&apos;None Identified&apos;),ISPICKVAL( BodilyInjury__c ,&apos;&apos;))) , 
ISPICKVAL( TypeOfInjury__c, &apos;&apos;) 
)</errorConditionFormula>
        <errorDisplayField>TypeOfInjury__c</errorDisplayField>
        <errorMessage>Type of Injury is required if Bodily Injury is other than none.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CSI_VR_SEP12I2P02_PrtyAfliaReqWhnBINTnon</fullName>
        <active>true</active>
        <description>I2P September Release 2012: Injured Party Affiliation is Required if Bodily Injury is other than none affected.</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
NOT(OR(ISPICKVAL( BodilyInjury__c ,&apos;None Identified&apos;),ISPICKVAL( BodilyInjury__c ,&apos;&apos;))) ,
ISPICKVAL( InjuredPartyAffiliation__c,&apos;&apos;)
)</errorConditionFormula>
        <errorDisplayField>InjuredPartyAffiliation__c</errorDisplayField>
        <errorMessage>Injured Party Affiliation is required if Bodily Injury is other than none.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CSI_VR_SEP12I2P03_CauInjuryReqWhnBINTnon</fullName>
        <active>true</active>
        <description>I2P September Release 2012 : Cause of Injury is Required if Bodily Injury is other than none affected.</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
NOT(OR(ISPICKVAL( BodilyInjury__c ,&apos;None Identified&apos;),ISPICKVAL( BodilyInjury__c ,&apos;&apos;))) , 
ISPICKVAL( CauseOfInjury__c,&apos;&apos;) 
)</errorConditionFormula>
        <errorDisplayField>CauseOfInjury__c</errorDisplayField>
        <errorMessage>Cause of Injury is required if Bodily Injury is other than none.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CSI_VR_SEP12I2P04_TypofPDReqWhnRskReprtd</fullName>
        <active>true</active>
        <description>I2P September Release 2012 : Type of Property Damage is Required if Property Damage is Risk Reported</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
OR(ISPICKVAL( PropertyDamage__c,&apos;Risk Reported&apos;),ISPICKVAL( PropertyDamage__c,&apos;Occurrence Reported&apos;)), 
ISPICKVAL(  TypeOfPropertyDamage__c ,&apos;&apos;)
