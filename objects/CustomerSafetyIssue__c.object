    </validationRules>
    <validationRules>
        <fullName>CSI_VR_SEP12I2P06_BIandPDcannotbenone</fullName>
        <active>true</active>
        <description>I2P September Release 2012: Bodily Injury and Property Damage can not both be &quot;None Identified&quot; .</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
AND(OR(ISPICKVAL(BodilyInjury__c,&apos;&apos;),ISPICKVAL(BodilyInjury__c,&apos;None Identified&apos;)),OR(ISPICKVAL( PropertyDamage__c,&apos;&apos;),ISPICKVAL( PropertyDamage__c,&apos;None Identified&apos;))) 
)</errorConditionFormula>
        <errorMessage>Bodily Injury and Property Damage cannot both be &quot;None Identified&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CSI_VR_SEP12I2P07_RSftyRskReqWhnStaCnfrm</fullName>
        <active>true</active>
        <description>I2P September Release 2012: Related Safety Risk is required when status is confirmed.
I2P May2013 Release: Changed Status &quot;Confirmed&quot; to Confirmed as safety Risk&quot;</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
ISPICKVAL(CSIStatus__c,&apos;Confirmed as safety risk&apos;) , 
RelatedOfferSafetyAlert__c =null 
)</errorConditionFormula>
        <errorDisplayField>RelatedOfferSafetyAlert__c</errorDisplayField>
        <errorMessage>Related Safety Risk is required when status is confirmed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CSI_VR_SEP12I2P08_AmtofPDReqWhnOccurRptd</fullName>
        <active>true</active>
        <description>I2P September Release 2012 : Amount  of Customer Claim is Required if Property Damage is Occurrence Reported</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
ISPICKVAL( PropertyDamage__c,&apos;Occurrence Reported&apos;), 
ISBLANK(AmountOfCustomerClaim__c ) 
)</errorConditionFormula>
        <errorDisplayField>AmountOfCustomerClaim__c</errorDisplayField>
        <errorMessage>Amount Of Customer Claim is required if Property Damage is Occurrence Reported.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CSI_VR_SEP12I2P09_AftdSEActReqWhnSECustm</fullName>
        <active>true</active>
        <description>I2P September Release 2012 : Affected SE Account is required if Affected Party is SE Customer</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
OR 
( 
AND(AffectedCustomer__c!=null, 
AffectedParty__c != &apos;&apos;), 
AND(AffectedCustomer__c=null, 
AffectedParty__c = &apos;&apos;) 
) 
)</errorConditionFormula>
        <errorDisplayField>AffectedCustomer__c</errorDisplayField>
        <errorMessage>One of these two fields needs to be filled in (Affected Customer or Affected Party), they cannot both be filled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CSI_VR_SEP12I2P10_BIisNoneIdentified</fullName>
        <active>true</active>
        <description>I2P September Release 2012: BI cannot be none identified as injured Party Affiliation or Human Death Reported or Overnight hospitalization required has values . Please blank these fields to change BI to none identified.</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
ISPICKVAL(BodilyInjury__c, &apos;None Identified&apos;), 
OR(NOT(ISPICKVAL(TypeOfInjury__c, &apos;&apos;)), 
NOT(ISPICKVAL(InjuredPartyAffiliation__c, &apos;&apos;)), 
HumanDeathReported__c==true, 
OvernightHospitalizationRequired__c==true) 
)</errorConditionFormula>
        <errorMessage>Bodily Injury cannot be none identified if Type of Bodily Injury, Injured Party Affiliation, Human Death Reported or Overnight hospitalization have a value. Please remove the value on these fields or change the Bodily Injury value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CSI_VR_SEP12I2P11_AmountCstmReqIfRskRptd</fullName>
        <active>true</active>
        <description>I2P September Release 2012: Amount of Customer Claim cannot have value if Property Damage is Risk Reported.</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
NOT (ISBLANK(AmountOfCustomerClaim__c) ), 
ISPICKVAL(PropertyDamage__c,&apos;Risk Reported&apos;) 
)</errorConditionFormula>
        <errorDisplayField>AmountOfCustomerClaim__c</errorDisplayField>
        <errorMessage>Amount of Customer Claim cannot have value if Property Damage is Risk Reported.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CSI_VR_SEP12I2P12_CheckingIsSchneiderUse</fullName>
        <active>true</active>
        <description>I2P September Release 2012: Check whether User is internal User.</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
IssueInvestigationLeader__c!=null,
NOT(ISPICKVAL( IssueInvestigationLeader__r.UserType,&apos;Standard&apos;)) 

)</errorConditionFormula>
        <errorDisplayField>IssueInvestigationLeader__c</errorDisplayField>
        <errorMessage>Please select internal user.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CSI_VR_SEP12I2P13_ReldSaftyRiskisRequird</fullName>
        <active>true</active>
        <description>I2P September Release 2012: Related Safety Risk is mandatory when CSI status is confirmed.
I2P May2013 Release: Changed Status &quot;Confirmed&quot; to &quot;Confirmed as safety risk&quot;</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 

ISPICKVAL( CSIStatus__c ,&apos;Confirmed as safety risk&apos;), 
RelatedOfferSafetyAlert__c =null 

)</errorConditionFormula>
        <errorDisplayField>RelatedOfferSafetyAlert__c</errorDisplayField>
        <errorMessage>Please enter Related Safety Alert.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CSI_VR_SEP12I2P14_LeaderEmailFormat</fullName>
        <active>true</active>
        <description>I2P September Release 2012: Leader Email should contain Schneider Electric Domain.</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
 VLOOKUP($ObjectType.SafeDomains__c.Fields.Name, $ObjectType.SafeDomains__c.Fields.Name,SUBSTITUTE(LeaderEmailAddress__c,left(LeaderEmailAddress__c,FIND(&quot;@&quot;, LeaderEmailAddress__c)), &quot;&quot;))!=SUBSTITUTE(LeaderEmailAddress__c,left(LeaderEmailAddress__c,FIND(&quot;@&quot;, LeaderEmailAddress__c)), &quot;&quot;) )</errorConditionFormula>
        <errorDisplayField>LeaderEmailAddress__c</errorDisplayField>
        <errorMessage>Please enter valid email address within a Schneider safe domain.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CSI_VR_SEP12I2P15_CSIStatusnotClose</fullName>
        <active>true</active>
        <description>I2P September relase</description>
        <errorConditionFormula>AND
(
	NOT($User.BypassVR__c),
	OR
	(
		RelatedOfferSafetyAlert__c !=null ,
		RelatedOfferSafetyAlert__c!=&apos;&apos;
	),
	ISPICKVAL(CSIStatus__c , &apos;Closed (no safety risk)&apos;),
 ISCHANGED(CSIStatus__c) 

)</errorConditionFormula>
        <errorDisplayField>CSIStatus__c</errorDisplayField>
        <errorMessage>CSI Status cannot be closed (no safety risk ) if there is Related Offer Safety Alert associated to it</errorMessage>
    </validationRules>
