        <label>Active?</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Eligiblefor__c</fullName>
        <description>OCT 13 PRM: Program eligibility to set when requirement is expired</description>
        <externalId>false</externalId>
        <label>Eligible for</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Promotion</fullName>
                    <default>true</default>
                    <label>Promotion</label>
                </value>
                <value>
                    <fullName>Demotion</fullName>
                    <default>false</default>
                    <label>Demotion</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>PartnerProgramName__c</fullName>
        <description>September 2012 Release: Formula field to show the Partner Program Name from Partner Program instead of Name field</description>
        <externalId>false</externalId>
        <formula>PartnerProgram__r.Name</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Partner Program Name</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>PartnerProgram__c</fullName>
        <description>APRIL RELEASE - Field relative to a Partner Program</description>
        <externalId>false</externalId>
        <label>Partner Program</label>
        <lookupFilter>
            <active>true</active>
            <booleanFilter>1 AND 2</booleanFilter>
            <filterItems>
                <field>PartnerProgram__c.ProgramStatus__c</field>
                <operation>notEqual</operation>
                <value>Draft, Pending Approval, Rejected</value>
            </filterItems>
            <filterItems>
                <field>PartnerProgram__c.RecordTypeId</field>
                <operation>equals</operation>
                <value>Country Partner Program, Cluster Partner Program</value>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>PartnerProgram__c</referenceTo>
        <relationshipLabel>Account Assigned Programs</relationshipLabel>
        <relationshipName>PartnerPrograms</relationshipName>
        <relationshipOrder>1</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>true</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>ProgramLevel__c</fullName>
        <deleteConstraint>Restrict</deleteConstraint>
        <externalId>false</externalId>
        <label>Program Level</label>
        <lookupFilter>
            <active>true</active>
            <booleanFilter>1 AND 2</booleanFilter>
            <errorMessage>Choose a Active Program Level associated with Partner Program</errorMessage>
            <filterItems>
                <field>$Source.PartnerProgram__c</field>
                <operation>equals</operation>
                <valueField>ProgramLevel__c.PartnerProgram__c</valueField>
            </filterItems>
            <filterItems>
                <field>ProgramLevel__c.LevelStatus__c</field>
                <operation>equals</operation>
                <value>Active, Inactive</value>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>ProgramLevel__c</referenceTo>
        <relationshipLabel>Account Assigned Programs</relationshipLabel>
        <relationshipName>AccountAssignedPrograms</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>TECH_UniqueAssignedId__c</fullName>
        <externalId>false</externalId>
        <formula>Account__c &amp; PartnerProgram__c &amp; ProgramLevel__c</formula>
        <label>TECH_UniqueAssignedId</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
