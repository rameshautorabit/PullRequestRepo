    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>AAP_VR_APR14PRM01_CheckActivePrgLevel</fullName>
        <active>true</active>
        <description>Checking whether the selected Program Level is active</description>
        <errorConditionFormula>AND(
NOT( $User.BypassVR__c ),
Active__c = true,
OR(ISNEW(), ISCHANGED( ProgramLevel__c )),
NOT(ISPICKVAL(ProgramLevel__r.LevelStatus__c,&apos;Active&apos;))
)</errorConditionFormula>
        <errorDisplayField>ProgramLevel__c</errorDisplayField>
        <errorMessage>The selected program level is not in active status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AAP_VR_APR14PRM02_CheckProgram</fullName>
        <active>true</active>
        <description>PRM: APR 14 ensure only active programs are chosen while creating a new account assigned program</description>
        <errorConditionFormula>AND (
 NOT( $User.BypassVR__c ),
 OR(ISNEW(), ISCHANGED ( PartnerProgram__c )),
 Active__c = true,
 NOT(ISPICKVAL( PartnerProgram__r.ProgramStatus__c , &apos;Active&apos;))
)</errorConditionFormula>
        <errorDisplayField>PartnerProgram__c</errorDisplayField>
        <errorMessage>Only active programs can be assigned to an Account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AAP_VR_MAY13PRM01_CheckProgramCountry</fullName>
        <active>true</active>
        <description>Partner Program should be available to Account Assigned Program&apos;s Account&apos;s country</description>
        <errorConditionFormula>AND( NOT($User.BypassVR__c),
IF( PartnerProgram__r.RecordType.DeveloperName == &apos;CountryPartnerProgram&apos;,( Account__r.Country__c &lt;&gt;  PartnerProgram__r.Country__c ) ,
NOT(CONTAINS( PartnerProgram__r.TECH_CountriesId__c , Account__r.Country__c))

)
)</errorConditionFormula>
        <errorMessage>This Program is not available for the selected Account.Please select another Program which belong to selected Account&apos;s Country</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
