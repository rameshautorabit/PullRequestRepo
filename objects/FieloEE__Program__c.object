    <validationRules>
        <fullName>FieloEE__Complete_Site_URL</fullName>
        <active>true</active>
        <errorConditionFormula>FieloEE__Default__c  =  FALSE &amp;&amp; FieloEE__SiteURL__c  = NULL</errorConditionFormula>
        <errorDisplayField>FieloEE__SiteURL__c</errorDisplayField>
        <errorMessage>Complete Site URL. You can find it going to Setup -&gt; Develop -&gt; Sites</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloEE__Open_Redemption_Days</fullName>
        <active>true</active>
        <errorConditionFormula>FieloEE__OpenRedemptionDays__c &lt; 0</errorConditionFormula>
        <errorDisplayField>FieloEE__OpenRedemptionDays__c</errorDisplayField>
        <errorMessage>Value cannot be negative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloEE__Validate_Expiration_Period</fullName>
        <active>true</active>
        <errorConditionFormula>FieloEE__ExpirationPeriod__c &lt; 0</errorConditionFormula>
        <errorDisplayField>FieloEE__ExpirationPeriod__c</errorDisplayField>
        <errorMessage>Value cannot be negative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloEE__Validate_Inactive_Days</fullName>
        <active>true</active>
        <errorConditionFormula>FieloEE__InactiveDays__c &lt; 1</errorConditionFormula>
        <errorDisplayField>FieloEE__InactiveDays__c</errorDisplayField>
        <errorMessage>Quantity should be positive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloEE__Validate_Numer_of_voucher_digits</fullName>
        <active>true</active>
        <errorConditionFormula>FieloEE__NumberOfVoucherDigits__c &lt; 4</errorConditionFormula>
        <errorDisplayField>FieloEE__NumberOfVoucherDigits__c</errorDisplayField>
        <errorMessage>Must be greater than 4 digits</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloEE__Validate_RecentRewardsDays</fullName>
        <active>true</active>
        <errorConditionFormula>FieloEE__RecentRewardsDays__c &lt; 1</errorConditionFormula>
        <errorDisplayField>FieloEE__RecentRewardsDays__c</errorDisplayField>
        <errorMessage>The quantity should be positive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloEE__Validate_TimeOut_Orphan_Transactions</fullName>
        <active>true</active>
        <errorConditionFormula>FieloEE__TimeOutOrphanTransactions__c &lt; 0 || FieloEE__TimeOutOrphanTransactions__c &gt; 30</errorConditionFormula>
        <errorDisplayField>FieloEE__TimeOutOrphanTransactions__c</errorDisplayField>
        <errorMessage>Value must be between 0 and 30</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fielo_SegmentationRefIterationValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),F_PRM_SegmentationRefreshIterationLimit__c &lt; 0)</errorConditionFormula>
        <errorDisplayField>F_PRM_SegmentationRefreshIterationLimit__c</errorDisplayField>
        <errorMessage>Segmentation Refresh Iteration Limit can&apos;t contain a negative value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fielo_SegmentationRefreshTimeValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),F_PRM_SegmentationRefreshTime__c  &lt; 0)</errorConditionFormula>
        <errorDisplayField>F_PRM_SegmentationRefreshTime__c</errorDisplayField>
        <errorMessage>Segmentation Refresh Time can&apos;t contain a negative value.</errorMessage>
    </validationRules>
