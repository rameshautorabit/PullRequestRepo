    <validationRules>
        <fullName>PLAT_USGAR_VR01_ChampionMandatory</fullName>
        <active>true</active>
        <description>Champion field on Assignee cannot be left blank</description>
        <errorConditionFormula>$User.BypassVR__c = FALSE &amp;&amp;
$User.TECH_ProfileName__c !=  $Label.CLJUN19PLAT01 &amp;&amp;
(($User.FirstName &amp; &quot; &quot; &amp; $User.LastName) != &apos;Automated Process&apos;) &amp;&amp;
(PermissionSetGroup__r.RecordType.DeveloperName = &apos;Profile&apos; ||
 PermissionSetGroup__r.RecordType.DeveloperName = &apos;NewProfileActivation&apos;) &amp;&amp;
ISBLANK(Assignee__r.Champion__c)</errorConditionFormula>
        <errorMessage>Champion field of Assignee cannot be left blank.
Please assign the champion to the user submitting assignment request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PSAR_VR01_CannotChangeName</fullName>
        <active>false</active>
        <description>The name cannot be changed after the creation of the record.</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp; ISCHANGED(Name)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>You cannot change the name of a request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PSAR_VR02_Duration_Mandatory</fullName>
        <active>true</active>
        <description>When Auto Remove Assignment is true the Duration selection is manadate</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),(Auto_Remove_Assignment__c =True), TEXT( Duration_Applicable__c) = &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Duration_Applicable__c</errorDisplayField>
        <errorMessage>When Auto Removal Assignment is &quot;True&quot; then “Duration Applicable” is mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PSAR_VR_Auto_Remove_Assignmen_Mandatory</fullName>
        <active>true</active>
        <description>Auto Remove Assignment is Mandatory when Duration Applicable is selected</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),(Auto_Remove_Assignment__c =false), TEXT( Duration_Applicable__c) != &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Auto_Remove_Assignment__c</errorDisplayField>
        <errorMessage>Auto Remove Assignment is mandatory when Duration Applicable is selected</errorMessage>
    </validationRules>
