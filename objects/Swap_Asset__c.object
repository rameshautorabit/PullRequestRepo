    <validationRules>
        <fullName>IPS_VR_DEC15SRV01_TripletCheck</fullName>
        <active>false</active>
        <errorConditionFormula>AND((DeinstalledInstalledProduct__r.ProductTech_ConcatBRDType__c &lt;&gt;  InstalledProduct__r.ProductTech_ConcatBRDType__c),NOT( $User.BypassVR__c ))</errorConditionFormula>
        <errorMessage>Brand, Range and Device Type must be the same for losing and gaining Installed Products.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IPS_VR_DEC15SRV02_CommissioningDate</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISNULL( InstalledProduct__r.CommissioningDateInstallDate__c )),NOT( $User.BypassVR__c ))</errorConditionFormula>
        <errorMessage>It is not permitted to swap an Installed Product with an already commissioned Installed Product.</errorMessage>
    </validationRules>
