        <fullName>Included_Service_part_of_contract</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),NOT(ISBLANK(IncludedService__c)), IncludedService__r.SVMXC__Service_Contract__c !=  SVMXC__Service_Contract__c)</errorConditionFormula>
        <errorDisplayField>IncludedService__c</errorDisplayField>
        <errorMessage>&apos;Included Service&apos; must be associated to this contract.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SMCVR01APR2015CoveredProductdateCheck</fullName>
        <active>true</active>
        <description>This Validation Rule will Check the Covered Products Start Date and End Date Falls within the Service Line Start Date and End Date</description>
        <errorConditionFormula>AND(
		NOT($User.BypassVR__c),
		OR(
			((SVMXC__Start_Date__c &lt; SVMXC__Service_Contract__r.SVMXC__Start_Date__c ) || (SVMXC__Start_Date__c&gt;SVMXC__Service_Contract__r.SVMXC__End_Date__c )) ||
			((SVMXC__End_Date__c &gt; SVMXC__Service_Contract__r.SVMXC__End_Date__c ) || (SVMXC__End_Date__c &lt;SVMXC__Service_Contract__r.SVMXC__Start_Date__c))
		  )
    )</errorConditionFormula>
        <errorMessage>Start date &amp; End date of the Covered product must be included within the Start date &amp; End date of the Service line</errorMessage>
    </validationRules>
