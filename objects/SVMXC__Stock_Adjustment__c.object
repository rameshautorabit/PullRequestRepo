    <fields>
        <fullName>SVMXC__Change_Quantity2__c</fullName>
        <deprecated>false</deprecated>
        <description>Positive number indicating the change in quantity (Higher quantity – lower quantity)</description>
        <externalId>false</externalId>
        <formula>IF( SVMXC__New_Quantity2__c  &gt;  SVMXC__Old_Quantity2__c , SVMXC__New_Quantity2__c   -   SVMXC__Old_Quantity2__c ,  SVMXC__Old_Quantity2__c  -  SVMXC__New_Quantity2__c )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Positive number indicating the change in quantity (Higher quantity – lower quantity)</inlineHelpText>
        <label>Change Qty</label>
        <precision>18</precision>
        <required>false</required>
        <scale>4</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>SVMXC__Change_Type__c</fullName>
        <deprecated>false</deprecated>
        <description>Indicates if the change was an increase or decrease</description>
        <externalId>false</externalId>
        <formula>IF( SVMXC__New_Quantity2__c  &gt;  SVMXC__Old_Quantity2__c  , &quot;Increase&quot;,  IF( SVMXC__Old_Quantity2__c &gt;  SVMXC__New_Quantity2__c  , &quot;Decrease&quot;, &quot;&quot;) )</formula>
        <inlineHelpText>Indicates if the change was an increase or decrease</inlineHelpText>
        <label>Change Type</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
