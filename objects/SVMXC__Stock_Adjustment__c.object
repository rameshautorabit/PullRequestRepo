    <validationRules>
        <fullName>SVMXC__SVMXC_ValidateAdjustmentQuantityChange</fullName>
        <active>true</active>
        <description>Adjustment quantity cannot be changed.</description>
        <errorConditionFormula>AND(!ISNEW(),  ISCHANGED(SVMXC__New_Quantity2__c))</errorConditionFormula>
        <errorDisplayField>SVMXC__New_Quantity2__c</errorDisplayField>
        <errorMessage>Once saved, adjustment quantity cannot be modified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SVMXC__SVMXC_ValidateStockableLocation</fullName>
        <active>true</active>
        <description>The adjustment location should be a stocking location.</description>
        <errorConditionFormula>(SVMXC__Location__r.SVMXC__Stocking_Location__c = false)</errorConditionFormula>
        <errorDisplayField>SVMXC__Location__c</errorDisplayField>
        <errorMessage>This is not a stocking location. Please select stocking locations only.</errorMessage>
    </validationRules>
