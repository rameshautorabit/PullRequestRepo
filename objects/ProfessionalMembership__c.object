    <validationRules>
        <fullName>PFA_VR_OCT13PRM01_CheckContactCountry</fullName>
        <active>true</active>
        <description>For PRM October 2013 Release
To restrict Users to select Contact from the Affiliation Catalog&apos;s Country</description>
        <errorConditionFormula>AND( NOT($User.BypassVR__c), 
 NOT(Contact__r.Country__c = MembershipCatalog__r.Country__c )
)</errorConditionFormula>
        <errorMessage>Contact&apos;s Country and  Membership Affiliation&apos;s Country do not match. Please select another Contact.</errorMessage>
    </validationRules>
