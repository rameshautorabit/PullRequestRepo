    <validationRules>
        <fullName>ASQ_VR_OCT13PRM01_RestrictRadioBtnSelect</fullName>
        <active>true</active>
        <description>For October 13 PRM Release
Restrict Users to select radio button for Opportunity Assessment Question</description>
        <errorConditionFormula>AND($User.BypassVR__c=False,(
IF(  Assessment__r.RecordTypeId =  $Label.CLOCT13PRM28,true,false)),
OR(
 ISPICKVAL( AnswerType__c , &apos;Radio Button&apos;) ,
 ISPICKVAL( AnswerType__c , &apos;MultiSelect&apos;) )
)</errorConditionFormula>
        <errorMessage>Radio Button/MultiSelect cannot be selected for Opportunity Assessment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP_VR_SEP12SLS01_ASSE_QUES_Weightage</fullName>
        <active>true</active>
        <description>Weightage is applicable for only Picklist type Question</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c),
AND(TEXT( AnswerType__c)  &lt;&gt; &apos;Picklist&apos; , NOT( ISBLANK(Weightage__c) ) ) )</errorConditionFormula>
        <errorDisplayField>Weightage__c</errorDisplayField>
        <errorMessage>This field is applicable only for Answer Type &apos;Picklist&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP_VR_SEP12SLS02_ASSE_QUES_Weightage</fullName>
        <active>true</active>
        <description>Weightage is mandatory for Picklist type Question</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c),
AND(TEXT( AnswerType__c)  = &apos;Picklist&apos; ,  ISBLANK(Weightage__c) ) )</errorConditionFormula>
        <errorDisplayField>Weightage__c</errorDisplayField>
        <errorMessage>This field is mandatory for Answer Type &apos;Picklist&apos;</errorMessage>
    </validationRules>
