    <validationRules>
        <fullName>MPSI_VR_OCT15SLS01_SingleRecForPrj</fullName>
        <active>true</active>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp; 
IF(ISNEW() &amp;&amp; RecordTypeId = &apos;01212000000zskl&apos; &amp;&amp;    MasterProject__r.IndustryPSICount__c&gt;=1,true,false
)</errorConditionFormula>
        <errorMessage>You cannot create more than one record under the Master Project.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PSI_VR_MAR16SLS01_AccessToCertainFields</fullName>
        <active>true</active>
        <description>Reference from BSI_VR_OCT14SLS01_AccessToCertainFields VR</description>
        <errorConditionFormula>NOT($User.BypassVR__c)
&amp;&amp;
(
	AND(
		OR(
			ISBLANK($User.Permission_sets_assigned__c), 
			NOT(CONTAINS($User.Permission_sets_assigned__c, &quot;SA - InvensysGlobalMajorProjectsTeam&quot;))
		), 
		OR(
			ISCHANGED(Licensor__c), 
			ISCHANGED(ProjectManagementContractor__c), 
			ISCHANGED(PursuitPhase__c), 
			ISCHANGED(EndUsertoEPCAwardDate__c),
			ISCHANGED(Comments__c),
			ISCHANGED(ContractType__c),
			ISCHANGED(GlobalMajorProjects__c),
			ISCHANGED(GMPGates__c),
			ISCHANGED(KeyRisks__c),
			ISCHANGED(MainAutomationContractor__c),
			ISCHANGED(NextActions__c),
			ISCHANGED(StatusRemarks__c),
			ISCHANGED(MasterProject__c)
		),
		NOT(ISPICKVAL(GlobalMajorProjects__c, &apos;Regional MP: Major Project regionally handled&apos;)),
		NOT(ISPICKVAL(GlobalMajorProjects__c, &apos;&apos;))	
	) 
)</errorConditionFormula>
        <errorMessage>You do not have appropriate permission to modify this Master Project Specific Information.</errorMessage>
    </validationRules>
