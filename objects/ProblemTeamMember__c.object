        <active>true</active>
        <description>Team Member lookup or Email Id field is mandatory while creating Team Member</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),ISBLANK(User__c),ISBLANK(EmailID__c))</errorConditionFormula>
