    <validationRules>
        <fullName>RPDT_VR_DEC12RR01_CountryAndPFAndCRReqd</fullName>
        <active>true</active>
        <errorConditionFormula>AND 
(
NOT($User.BypassVR__c) ,
Ruleactive__c=True,
OR(ISBLANK(Country__c),ISNULL(Country__c)),
OR(ISBLANK(CommercialReference__c),ISNULL(CommercialReference__c)),
OR(ISBLANK(Family__c),ISNULL(Family__c))
)</errorConditionFormula>
        <errorMessage>Country and Commercial Reference / Family can&apos;t be blank at same time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RPDT_VR_DEC12RR02_TechGMRCodeRequired</fullName>
        <active>true</active>
        <description>TechGMRCode can&apos;t be null when Commercial Ref / Product Family field is filled.</description>
        <errorConditionFormula>AND (
NOT($User.BypassVR__c) ,
Ruleactive__c=True,
OR(
NOT(ISBLANK(CommercialReference__c)),
NOT(ISBLANK(TECH_ProductFamily__c))
),
ISBLANK(TECH_GMRCode__c)
)</errorConditionFormula>
        <errorMessage>GMRCode is requried.</errorMessage>
    </validationRules>
