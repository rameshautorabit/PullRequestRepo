    <validationRules>
        <fullName>F_PRM_BannerFilterFilledWithPCountryCode</fullName>
        <active>true</active>
        <description>Content Filter must begin with P-{countrycode}</description>
        <errorConditionFormula>NOT($User.BypassVR__c) 

&amp;&amp; 

NOT(BEGINS(F_PRM_ContentFilter__c , &quot;TMPL-&quot; )) 

&amp;&amp; 

(ISNEW() || ISCHANGED(F_PRM_ContentFilter__c ) || ISCHANGED(F_PRM_Country__c )) 

&amp;&amp; 

((If(F_PRM_Country__r.CountryCode__c=&quot;AE&quot;,true,false) &amp;&amp; NOT(BEGINS(F_PRM_ContentFilter__c , &quot;R-AE-Rewards&quot; )) &amp;&amp; NOT(BEGINS(F_PRM_ContentFilter__c , &quot;P-&quot;+F_PRM_Country__r.CountryCode__c ))) 
|| (If(F_PRM_Country__r.CountryCode__c != &quot;AE&quot;,true,false) &amp;&amp; NOT(BEGINS(F_PRM_ContentFilter__c , &quot;P-&quot;+F_PRM_Country__r.CountryCode__c ))))</errorConditionFormula>
        <errorDisplayField>F_PRM_ContentFilter__c</errorDisplayField>
        <errorMessage>Content Filter must begin with P-{countrycode}</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>F_PRM_SurveySelection</fullName>
        <active>true</active>
        <description>Surveys lookup can only be filled if the link behaviour picklist = &apos;Survey&apos;</description>
        <errorConditionFormula>IF(
NOT($User.BypassVR__c) &amp;&amp; 
((
NOT(ISBLANK( F_PRM_Survey__c )) &amp;&amp;
TEXT( F_PRM_LinkBehavior__c ) &lt;&gt; &apos;Survey&apos;
) ||
(
ISBLANK( F_PRM_Survey__c ) &amp;&amp;
TEXT( F_PRM_LinkBehavior__c ) =&apos;Survey&apos;
)),
true,
false
)</errorConditionFormula>
        <errorMessage>To configure a survey, please select your survey in the &apos;Survey&apos; field and select the &apos;Survey&apos; option in the &apos;Link Behavior&apos; field.</errorMessage>
