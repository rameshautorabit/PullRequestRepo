    <validationRules>
        <fullName>SVMXC__ONS_Validation_LimitConsONS</fullName>
        <active>true</active>
        <description>Maximum consecutive ONS per week should be greater than or equal to 0 and less than or equals to the total ONS per week.</description>
        <errorConditionFormula>OR(SVMXC__SM_Consecutive_ONS_Per_Week__c &lt; 0, SVMXC__SM_Consecutive_ONS_Per_Week__c &gt; SVMXC__SM_Total_ONS_Per_Week__c)</errorConditionFormula>
        <errorMessage>Maximum consecutive overnight stays per week should be between 0 and total overnight stays per week.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SVMXC__ONS_Validation_LimitMinGapBetnConsONS</fullName>
        <active>true</active>
        <description>Minimum gap between consecutive overnights should be greater than or equal to 0 and less than or equal to 7.</description>
        <errorConditionFormula>OR(SVMXC__SM_Minimum_Recovery_Days__c &lt; 0 , SVMXC__SM_Minimum_Recovery_Days__c &gt; 7)</errorConditionFormula>
        <errorMessage>Minimum gap between consecutive overnights should be between 0 and 7.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SVMXC__ONS_Validation_LimitTotalNumberOfONS</fullName>
        <active>true</active>
        <description>In a week total ONS should be greater than or equal to 0 and less than or equal to 7 days</description>
        <errorConditionFormula>OR(SVMXC__SM_Total_ONS_Per_Week__c &lt; 0, SVMXC__SM_Total_ONS_Per_Week__c &gt; 7)</errorConditionFormula>
        <errorMessage>Total overnight stays should be between 0 and 7 days per week.</errorMessage>
    </validationRules>
