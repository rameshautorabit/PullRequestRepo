    <validationRules>
        <fullName>ContainsAndNotEqualNotSupported</fullName>
        <active>true</active>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp; (ExternalPropertiesCatalog__r.MinimumLengthCheck__c &gt;0 &amp;&amp; (Text(Operator__c)=&apos;Contains&apos; || Text(Operator__c)=&apos;Not equal&apos;))</errorConditionFormula>
        <errorDisplayField>Operator__c</errorDisplayField>
        <errorMessage>&apos;Contains&apos; and &apos;Not equal&apos; operators are only supported if the MinimumLengthCheck__c = 0 (technical field to optimize queries).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Country_Channel_Validation</fullName>
        <active>false</active>
        <description>Country Channel Validation for Internal Activities Record Type</description>
        <errorConditionFormula>IF(NOT(ISBLANK(CountryChannel__c)),  
 IF(ExternalPropertiesCatalog__r.RecordType.DeveloperName!=&quot;Internal_Activities&quot;, TRUE, FALSE), FALSE)</errorConditionFormula>
        <errorDisplayField>CountryChannel__c</errorDisplayField>
        <errorMessage>Country Channel must be added for the Properties and Activities Catalog of Type Internal Activities</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Country_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>(ISBLANK(PRMCountry__c)&amp;&amp; ExternalPropertiesCatalog__r.RecordType.DeveloperName==&quot;BFOProperties&quot;)</errorConditionFormula>
        <errorDisplayField>PRMCountry__c</errorDisplayField>
        <errorMessage>The PRM Country must be added for the Properties and Activities Catalog of Type Internal Properties</errorMessage>
    </validationRules>
