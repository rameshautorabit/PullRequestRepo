    <validationRules>
        <fullName>SPA_VR_SEP12SLS01_TypeDiscount</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), OR(ISPICKVAL(Type__c,&apos;Additional Discount&apos;),ISPICKVAL(Type__c,&apos;Compensation%&apos;)),OR(  ThresholdValue0__c  &lt; 0 , ThresholdValue0__c  &gt;100 , ThresholdValue1__c  &lt; 0 ,ThresholdValue1__c  &gt;100 , ThresholdValue2__c  &lt; 0 ,ThresholdValue2__c  &gt;100 ))</errorConditionFormula>
        <errorMessage>Threshold value should be between 0 and 100</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_SEP12SLS02_TypeMultiplier</fullName>
        <active>true</active>
        <errorConditionFormula>AND (NOT($User.BypassVR__c), 
AND(ISPICKVAL(Type__c,&apos;Final Multiplier&apos;),OR(  ThresholdValue0__c  &lt; 0  || ThresholdValue0__c  &gt;1 , ThresholdValue1__c  &lt; 0  || ThresholdValue1__c  &gt;1 , ThresholdValue2__c  &lt; 0  || ThresholdValue2__c  &gt;1 )  ) )</errorConditionFormula>
        <errorMessage>Threshold value should be between 0 and 1</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_SEP12SLS03_ThresholdDiscount1</fullName>
        <active>true</active>
        <description>Threshold 1 should greater than or equal to Threshold 0</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c),OR(ISPICKVAL( Type__c,&apos;Additional Discount&apos;),ISPICKVAL(Type__c,&apos;Compensation%&apos;)),NOT (  ThresholdValue1__c &gt;=  ThresholdValue0__c )  )</errorConditionFormula>
        <errorDisplayField>ThresholdValue1__c</errorDisplayField>
        <errorMessage>Threshold Value 1 should be greater than or equal to Threshold Value 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_SEP12SLS04_ThresholdDiscount2</fullName>
        <active>true</active>
        <description>Threshold 2 should greater than or equal to Threshold 1</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),OR(ISPICKVAL( Type__c,&apos;Additional Discount&apos;),ISPICKVAL(Type__c,&apos;Compensation%&apos;)),NOT (  ThresholdValue2__c &gt;=  ThresholdValue1__c ))</errorConditionFormula>
        <errorDisplayField>ThresholdValue2__c</errorDisplayField>
        <errorMessage>Threshold Value 2 should be greater than or equal to Threshold Value 1</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_SEP12SLS05_ThresholdMultiplier1</fullName>
        <active>true</active>
        <description>Threshold 1 should less than or equal to Threshold 0</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c), AND (ISPICKVAL( Type__c,&apos;Final Multiplier&apos;), NOT ( ThresholdValue1__c &lt;= ThresholdValue0__c ) )  )</errorConditionFormula>
        <errorDisplayField>ThresholdValue1__c</errorDisplayField>
        <errorMessage>Threshold Value 1 should be less than or equal to Threshold Value 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_SEP12SLS06_ThresholdMultiplier2</fullName>
        <active>true</active>
        <description>Threshold 2 should less than or equal to Threshold 1</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c), AND (ISPICKVAL( Type__c,&apos;Final Multiplier&apos;), NOT ( ThresholdValue2__c &lt;= ThresholdValue1__c ) )  )</errorConditionFormula>
        <errorDisplayField>ThresholdValue2__c</errorDisplayField>
        <errorMessage>Threshold Value 2 should be less than or equal to Threshold Value 1</errorMessage>
    </validationRules>
