    <validationRules>
        <fullName>LTR_VR01_SalesOrderAndPOnumber</fullName>
        <active>true</active>
        <description>This Validation rule is to check only Delegated Administrator  and SE - Interface LR User can change the PO Number and Sales Order.</description>
        <errorConditionFormula>AND($User.BypassVR__c = False, 
NOT(ISNEW()),
NOT(CONTAINS($Label.CL00632,$Profile.Name)),
NOT(CONTAINS($Label.CLCCCMAY120001,$Profile.Id)),
OR(ISCHANGED(PONumber__c),ISCHANGED(SalesOrder__c)))</errorConditionFormula>
        <errorMessage>PO Number or Sales Order can not be modified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LTR_VR02_CommRefmandatory</fullName>
        <active>true</active>
        <description>Atleast one Literature request product should be entered</description>
        <errorConditionFormula>AND(ISBLANK( CommercialReference__c ),NOT($User.BypassVR__c))</errorConditionFormula>
        <errorMessage>A master product should be assigned to the Literature Request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LTR_VR03_Quantitylessthanone</fullName>
        <active>true</active>
        <description>This Validation rule is to restrict the user to enter the quantity as less than one.</description>
        <errorConditionFormula>AND($User.BypassVR__c = False, 
 Quantity__c &lt; 1)</errorConditionFormula>
        <errorMessage>Quantity should not be less than one .</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
