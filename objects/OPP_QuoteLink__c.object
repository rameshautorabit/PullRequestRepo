        <fullName>QuoteReadyApproval__c</fullName>
        <externalId>false</externalId>
        <formula>IF( ActiveQuote__c = False, IMAGE(&quot;/img/samples/flag_red.gif&quot;, &quot;&quot;) &amp; &quot; The Quote needs to be &apos;Active&apos; before being sent for Approval&quot;,

     IF(AND(OR(ISBLANK(Amount__c),ISBLANK(Margin__c)),ISPICKVAL( OpportunityName__r.OpptyType__c , &quot;Solutions&quot;),(OpportunityName__r.TECH_AmountEUR__c &gt;= 500000)), IMAGE(&quot;/img/samples/flag_red.gif&quot;, &quot;&quot;) &amp; &quot; Please fill in the Amount and the CCO Gross Margin% before submitting for Approval&quot;,

     IF( OR(OpportunityName__r.TechIsLevel3__c = 1,  OpportunityName__r.TechIsLevel2__c) &amp;&amp; ISBLANK(Margin__c) &amp;&amp; ISPICKVAL( OpportunityName__r.IncludedInForecast__c , &quot;Yes&quot;) &amp;&amp;  ISBLANK(OpportunityName__r.AgreementReference__c) , IMAGE(&quot;/img/samples/flag_red.gif&quot;, &quot;&quot;) &amp; &quot; The CCO Gross Margin needs to be filled in for all Level 2 or Level 3 Solutions Opportunity before being sent for Approval&quot;,

          IF( ISPICKVAL( OpportunityName__r.OpptyType__c , &quot;Solutions&quot;) &amp;&amp;  OpportunityName__r.TECH_AmountEUR__c &gt; 2000000 &amp;&amp; ISBLANK(Margin__c) &amp;&amp; ISPICKVAL( OpportunityName__r.IncludedInForecast__c , &quot;Yes&quot;) &amp;&amp; ISBLANK(OpportunityName__r.AgreementReference__c), IMAGE(&quot;/img/samples/flag_red.gif&quot;, &quot;&quot;) &amp; &quot; The CCO Gross Margin needs to be filled in for Solutions Opportunities  above 2,000,000€ before being sent for Approval&quot;, 

               IF( OR(ISPICKVAL( QuoteStatus__c, &quot;Approved&quot;), ISPICKVAL( QuoteStatus__c, &quot;Closed&quot;),  ISPICKVAL( QuoteStatus__c, &quot;Approval in Progress&quot;), ISPICKVAL( QuoteStatus__c, &quot;Clarification in Progress&quot;)), &quot;&quot;, IMAGE(&quot;/img/samples/flag_green.gif&quot;, &quot;&quot;) &amp; &quot; Your Quote is ready to be sent for Approval&quot; )))))</formula>
        <label>Quote Readiness for Approval</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
