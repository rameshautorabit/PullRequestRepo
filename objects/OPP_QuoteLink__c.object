    <validationRules>
        <fullName>QLK_VR01_ApproverVerify</fullName>
        <active>true</active>
        <description>SR0 - Approvers must be entered sequentially</description>
        <errorConditionFormula>IF( NOT(ISBLANK( Approver8__c)) &amp;&amp; ISBLANK( Approver7__c) , true, IF( NOT(ISBLANK( Approver7__c)) &amp;&amp; ISBLANK( Approver6__c) , true, IF( NOT(ISBLANK( Approver6__c)) &amp;&amp; ISBLANK( Approver5__c) , true, IF( NOT(ISBLANK( Approver5__c)) &amp;&amp; ISBLANK( Approver4__c) , true, IF( NOT(ISBLANK( Approver4__c)) &amp;&amp; ISBLANK( Approver3__c) , true, IF( NOT(ISBLANK( Approver3__c)) &amp;&amp; ISBLANK( Approver2__c) , true, IF( NOT(ISBLANK( Approver2__c)) &amp;&amp; ISBLANK( Approver1__c) , true, false) ) ) ) ) ) ) &amp;&amp;  NOT($User.BypassVR__c)</errorConditionFormula>
        <errorMessage>You must complete Approvers sequentially.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QLK_VR02_OnOpportunityChange</fullName>
        <active>true</active>
        <description>Opportunity Name should not be changed on Quote Link</description>
        <errorConditionFormula>Not($User.BypassVR__c) &amp;&amp; ISCHANGED(OpportunityName__c)</errorConditionFormula>
        <errorDisplayField>OpportunityName__c</errorDisplayField>
        <errorMessage>Opportunity Name cannot be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QLK_VR04_EnforceMarginCaptured</fullName>
        <active>true</active>
        <description>2012 April Release - Check that the Margin has been field if theOpportunity is Solutions at Stage 6 and:
the Opportunity Amount exceeds   2 million Euro OR
the Opportunity Scope is Level 2 or Level 3</description>
        <errorConditionFormula>Not($User.BypassVR__c) &amp;&amp; 
ActiveQuote__c &amp;&amp;
ISBLANK( Margin__c ) &amp;&amp;  
ISPICKVAL( OpportunityName__r.OpptyType__c , &quot;Solutions&quot;) &amp;&amp;  ISPICKVAL(OpportunityName__r.StageName, &quot;6 - Negotiate &amp; Win&quot;) &amp;&amp; 
ISPICKVAL( OpportunityName__r.IncludedInForecast__c, &quot;Yes&quot;) &amp;&amp;
ISBLANK( OpportunityName__r.AgreementReference__c )&amp;&amp;
OR(OpportunityName__r.TechIsLevel2__c,  OpportunityName__r.TechIsLevel3__c = 1,  OpportunityName__r.TECH_AmountEUR__c &gt; 2000000)</errorConditionFormula>
        <errorDisplayField>Margin__c</errorDisplayField>
        <errorMessage>Please be sure to capture the CCO Gross Margin (%) when your Opportunity is Solutions at Stage 6 and its amount is above 2,000,000 Euros or its scope is Level 2 or 3.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QLK_VR05_FreezeMarginAtStage7</fullName>
        <active>true</active>
        <description>2012 April Release - Prevent the User from changing the CCO Margin(%) the Opportunity it&apos;s attached to is at stage 7</description>
        <errorConditionFormula>Not($User.BypassVR__c) &amp;&amp; 
ISPICKVAL(OpportunityName__r.StageName, &quot;7 - Deliver &amp; Validate&quot;) &amp;&amp; 
ISCHANGED(Margin__c)</errorConditionFormula>
        <errorDisplayField>Margin__c</errorDisplayField>
        <errorMessage>The CCO Gross Margin (%) should not be edited when the related Opportunity is at Stage 7 - Deliver &amp; Validate.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QLK_VR06_MarginNotMoreThan100</fullName>
        <active>true</active>
        <description>July2012 - The field ʺCCO Gross Margin (%)ʺ must not be above 100%</description>
        <errorConditionFormula>(Margin__c  &gt; 1) &amp;&amp;  NOT($User.BypassVR__c)</errorConditionFormula>
        <errorDisplayField>Margin__c</errorDisplayField>
        <errorMessage>Gross Margin (%) must not be above 100</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QLK_VR07_RuleToAvoidChangingStatus</fullName>
        <active>true</active>
        <description>This VR is used for ensuring that the user does not change the Status of the QLK using the Kanban View. This VR does not block the approval process from changing the Status.The Id refers to &quot;Quote Link Connector (Cordys-Eurotherm) Interface&quot; User.</description>
        <errorConditionFormula>AND(
NOT(Contains($User.Id,&apos;0512000005cmvEAAQ&apos;)),
ISCHANGED(QuoteStatus__c), NOT($User.BypassVR__c)
)</errorConditionFormula>
        <errorMessage>You cannot change the status of the Quote Link</errorMessage>
    </validationRules>
