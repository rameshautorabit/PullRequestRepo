        <description>If the badge is &quot;Program level&quot;, validates that the channel field is filled.</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp;
(ISPICKVAL(F_PRM_TypeSelection__c  , &apos;Program Level&apos;) ||  F_PRM_Type__c = &apos;Progam Level&apos; ) &amp;&amp;
ISBLANK(F_PRM_Challenge__c)</errorConditionFormula>
        <errorDisplayField>F_PRM_Challenge__c</errorDisplayField>
        <errorMessage>You must fill the Challenge field for Program Level badges</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPRM_VR_ChallengeNullIfNotPL</fullName>
        <active>true</active>
        <description>If the badge is not &quot;Program level&quot;, validates that the channel field is null.</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp;
NOT((ISPICKVAL(F_PRM_TypeSelection__c  , &apos;Program Level&apos;) ||  F_PRM_Type__c = &apos;Program Level&apos; )) &amp;&amp;
NOT(ISBLANK(F_PRM_Challenge__c))</errorConditionFormula>
        <errorDisplayField>F_PRM_Challenge__c</errorDisplayField>
        <errorMessage>The Challenge field must only be filled if the badge is a Progam Level</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPRM_VR_OnlyProgramLevelSegments</fullName>
        <active>true</active>
        <description>Only Program Level badges can grant segments</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp;
Text( F_PRM_TypeSelection__c ) &lt;&gt; &apos;Program Level&apos; &amp;&amp;
NOT(ISBLANK( F_PRM_Segment__c ))</errorConditionFormula>
        <errorDisplayField>F_PRM_Segment__c</errorDisplayField>
        <errorMessage>Only Program Level badges can grant segments</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRM_VR_ProgramACLName</fullName>
        <active>true</active>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp;  ISCHANGED(ACLName__c) &amp;&amp; NOT(ISBLANK(PRIORVALUE(ACLName__c)))</errorConditionFormula>
        <errorDisplayField>ACLName__c</errorDisplayField>
        <errorMessage>You are not authorized to update Program ACL Name.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
