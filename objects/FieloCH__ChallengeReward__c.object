    </searchLayouts>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>F_PRM_OnlyProgramLevelBadges</fullName>
        <active>true</active>
        <description>Validates that only &apos;Program Level&apos; can be configured as rewards</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp;
(
Text( FieloCH__Badge__r.F_PRM_TypeSelection__c ) &lt;&gt; &apos;Program Level&apos; ||
 FieloCH__Badge__r.F_PRM_Type__c &lt;&gt; &apos;Program Level&apos;
)</errorConditionFormula>
        <errorMessage>Only Program Level badges can be granted as rewards</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>F_PRM_PreventChallengeRewardModification</fullName>
        <active>true</active>
        <description>Cannot modify the Challenge Reward if the Challenge is Active.</description>
        <errorConditionFormula>NOT($User.BypassVR__c) 
&amp;&amp; 
(
FieloCH__Challenge__r.FieloCH__IsActive__c
)</errorConditionFormula>
        <errorMessage>Cannot modify the Challenge Reward if the Challenge is Active.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloCH__Edit_Reward</fullName>
        <active>false</active>
        <description>If the Challenge is active then the reward cannot be modified</description>
        <errorConditionFormula>FieloCH__Challenge__r.FieloCH__IsActive__c</errorConditionFormula>
        <errorMessage>Cannot modified the Reward if the Challenge is Active.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloCH__Reward_Program_Validation</fullName>
        <active>true</active>
        <description>Checks that the reward belongs to the same program as the challenge.</description>
        <errorConditionFormula>AND(
    NOT( ISBLANK(FieloCH__Reward__c) ) ,
    FieloCH__Reward__r.FieloEE__Program__c !=  FieloCH__Challenge__r.FieloCH__Program__c)</errorConditionFormula>
        <errorMessage>The Reward does not belong to the same Program as the Challenge.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fielo_DemotionValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND 
( 
	NOT($User.BypassVR__c), 
	TEXT(FieloCH__Challenge__r.FieloCH__Mode__c)=&apos;Teams&apos;, 
	NOT(ISBLANK(TEXT(F_PRM_ExpirationType__c))) 
)</errorConditionFormula>
        <errorDisplayField>F_PRM_ExpirationType__c</errorDisplayField>
        <errorMessage>Demotion is not supported on &apos;Teams&apos; mode challenges</errorMessage>
    </validationRules>
