    <validationRules>
        <fullName>RMA_VR02_POBoxOrStreetShouldBeFilled</fullName>
        <active>true</active>
        <description>2012R04 - Required field</description>
        <errorConditionFormula>NOT($User.BypassVR__c) 
&amp;&amp; 
ISBLANK(AccountPOBox__c) 
&amp;&amp; 
ISBLANK(AccountStreet__c)</errorConditionFormula>
        <errorDisplayField>AccountStreet__c</errorDisplayField>
        <errorMessage>Either PO Box or Street is required at one time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RMA_VR03_POBoxOrStreetShouldBeFilled</fullName>
        <active>true</active>
        <description>2012R04 - Required field</description>
        <errorConditionFormula>NOT($User.BypassVR__c) 
&amp;&amp; 
ISBLANK(AccountPOBox__c) 
&amp;&amp; 
ISBLANK(AccountStreet__c)</errorConditionFormula>
        <errorDisplayField>AccountPOBox__c</errorDisplayField>
        <errorMessage>Either PO Box or Street is required at one time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RMA_VR_APR15I2P01_SympotomCheckforDefect</fullName>
        <active>true</active>
        <description>RMA_VR_APR15I2P01_SympotomCheckforDefect</description>
        <errorConditionFormula>IF(
	ISNEW(),
	And(
		NOT($User.BypassVR__c),
		ISPICKVAL(ProductCondition__c, &apos;Defective&apos;),
		Case__r.CaseSymptom__c=&apos;&apos; , Case__r.CaseSubSymptom__c =&apos;&apos;
	),
	false
	)</errorConditionFormula>
        <errorMessage>The symptom and subsymptom on Case are mandatory in order to create a Return Request with Product Condition = Defective.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RMA_VR_JUN16I2P01_CaseArchived</fullName>
        <active>true</active>
        <description>This VR will prevent user from editing the RMA once the associated case is archived</description>
        <errorConditionFormula>AND(!$User.BypassVR__c ,OdasevaArchivedCaseURL__c !=&quot;No archived case to display&quot;)</errorConditionFormula>
        <errorMessage>This record can&apos;t be modified as the associated case has been archived</errorMessage>
    </validationRules>
