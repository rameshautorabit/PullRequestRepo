    <validationRules>
        <fullName>FieloPRM_VR_InvoiceAndProductCountry</fullName>
        <active>true</active>
        <description>Product Country and Invoice Country must be the same</description>
        <errorConditionFormula>NOT($User.BypassVR__c) 
&amp;&amp; 
( NOT( F_PRM_LoyaltyEligibleProduct__r.F_Country__r.CountryCode__c  =  F_PRM_Invoice__r.F_CountryCode__c ) )</errorConditionFormula>
        <errorDisplayField>F_PRM_LoyaltyEligibleProduct__c</errorDisplayField>
        <errorMessage>The selected product does not belong to the country specified in the invoice. Please select a different product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>INV_VR_APR19PRM01_QtyPriceMandatory</fullName>
        <active>false</active>
        <description>Price and Qty is mandatory</description>
        <errorConditionFormula>AND(ISPICKVAL(F_PRM_Invoice__r.F_PRM_Status__c,&apos;Approved&apos;),
	OR(F_PRM_UnitPrice__c =0,
	   ISBLANK(F_PRM_UnitPrice__c),
	   F_PRM_Volume__c = 0,
	   ISBLANK(F_PRM_Volume__c)
	)
)</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>Quantity and Price are mandatory</errorMessage>
    </validationRules>
