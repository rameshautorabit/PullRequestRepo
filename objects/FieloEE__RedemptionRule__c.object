    <validationRules>
        <fullName>F_PRM_HybridSegmentFeatureAssignment</fullName>
        <active>true</active>
        <description>Only Hybrid segments can assign features</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp; 
( 
TEXT( F_PRM_Type__c ) &lt;&gt; &apos;Hybrid&apos; &amp;&amp; 
NOT(ISBLANK( F_PRM_Feature__c )) 
)</errorConditionFormula>
        <errorDisplayField>F_PRM_Feature__c</errorDisplayField>
        <errorMessage>Only Hybrid segments can assign features</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>F_PRM_RestrictDynamicSegmentCreation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),RecordType.Name = &apos;Dynamic&apos; )</errorConditionFormula>
        <errorMessage>The creation of Dynamic segments is deactivated. Please use Manual Segment Record Type with the Hybrid option to create a new segment with the same behaviour.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>F_PRM_SegmenFilterFilledWithPCountryCode</fullName>
        <active>true</active>
        <description>Segment Filter must begin with P-{countrycode} in case of PRM records or R-AE-Rewards in case of Rewards Program records</description>
        <errorConditionFormula>NOT($User.BypassVR__c) 

&amp;&amp; 

NOT(BEGINS(F_PRM_SegmentFilter__c , &quot;TMPL-&quot; )) 

&amp;&amp; 

(ISNEW() || ISCHANGED(F_PRM_SegmentFilter__c )|| ISCHANGED(F_PRM_Country__c )) 

&amp;&amp; 

IF(F_PRM_Country__r.CountryCode__c=&quot;AE&quot;, 
NOT(BEGINS(F_PRM_SegmentFilter__c , &quot;R-AE-Rewards&quot; )), 
NOT(BEGINS(F_PRM_SegmentFilter__c , &quot;P-&quot;+F_PRM_Country__r.CountryCode__c )) 
)</errorConditionFormula>
        <errorDisplayField>F_PRM_SegmentFilter__c</errorDisplayField>
        <errorMessage>Segment Filter must begin with P-{countrycode} in case of PRM records or R-AE-Rewards in case of Rewards Program records</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
