    <validationRules>
        <fullName>VR_Q316_CS_CertifiedMandatoryFields</fullName>
        <active>true</active>
        <description>Q3 2016 Release : Please fill in the following fields : Start Training Date, Last Training Date, Initial Certification Date and Last Certified Date for certification status &quot;Certified&quot;</description>
        <errorConditionFormula>AND( NOT( $User.BypassVR__c ) ,  ISPICKVAL( CSQ_Certificationstatus__c , &quot;Certified&quot;) , OR( ISBLANK( CSQ_StartTrainingDate__c ) , ISBLANK( CSQ_LastTrainingDate__c) , ISBLANK(  CSQ_InitialcertifiedDate__c ) , ISBLANK(  CSQ_LastcertifiedDate__c )))</errorConditionFormula>
        <errorMessage>Please fill in the following fields : Start Training Date, Last Training Date, First Certified Date and Last Certified Date for certification status &quot;Certified&quot;</errorMessage>
    </validationRules>
