    <validationRules>
        <fullName>VR_CustomerReqDate</fullName>
        <active>true</active>
        <description>CustomerReqDate_Bw_StartDate_And_EndDate</description>
        <errorConditionFormula>IF(AND((SVMXC__PM_Plan__r.SVMXC__Start_Date__c &lt;= SVMXC__Scheduled_On__c),
(SVMXC__Scheduled_On__c &gt;= SVMXC__PM_Plan__r.SVMXC__End_Date__c),NOT($Profile.Id = $Label.CL00110), 
NOT($User.BypassVR__c)) ,true,false)</errorConditionFormula>
        <errorMessage>Customer Requested Date should be in between Start Date and End Date</errorMessage>
    </validationRules>
