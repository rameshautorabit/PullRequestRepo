    <validationRules>
        <fullName>SPA_VR_OCT14SLS01_TypeMandatory</fullName>
        <active>true</active>
        <errorConditionFormula>NOT($User.BypassVR__c)&amp;&amp; ISBLANK(TEXT(Type__c))</errorConditionFormula>
        <errorMessage>Type is mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_SEP12SLS01_ThresholdAmount1</fullName>
        <active>true</active>
        <description>Threshold 1should be greater than or equal to Threshold 0</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c),
NOT ( ThresholdAmount1__c  &gt;=  ThresholdAmount0__c ) )</errorConditionFormula>
        <errorDisplayField>ThresholdAmount1__c</errorDisplayField>
        <errorMessage>Threshold Amount 1 should be greater than or equal to Threshold Amount 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_SEP12SLS02_ThresholdAmount2</fullName>
        <active>true</active>
        <description>Threshold 2 should be greater than or equal to Threshold 1</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c),
NOT ( ThresholdAmount2__c  &gt;=  ThresholdAmount1__c ) )</errorConditionFormula>
        <errorDisplayField>ThresholdAmount2__c</errorDisplayField>
        <errorMessage>Threshold Amount 2 should be greater than or equal to Threshold Amount 1</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_SEP12SLS03_MinimumDealSize</fullName>
        <active>true</active>
        <description>Minimum Deal Size should be less than or equal to Threshold 0</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c),
NOT( MinimumDealSize__c  &lt;=  ThresholdAmount0__c  ) )</errorConditionFormula>
        <errorDisplayField>MinimumDealSize__c</errorDisplayField>
        <errorMessage>Minimum Deal Size should be less than or equal to Threshold Amount 0</errorMessage>
    </validationRules>
