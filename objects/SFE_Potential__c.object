    <fields>
        <fullName>PerDirect__c</fullName>
        <description>SR0 - % of direct sales at business unit level</description>
        <externalId>false</externalId>
        <label>% Direct</label>
        <precision>3</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>PerIndirect__c</fullName>
        <description>SR0 - 100% minus % Direct</description>
        <externalId>false</externalId>
        <formula>IF ( ISBLANK(PerDirect__c),0, 1 -  PerDirect__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>% Indirect</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>PerShareWallet__c</fullName>
        <description>SR0 - % pf share wallet at business unit level</description>
        <externalId>false</externalId>
        <formula>IF  (ISBLANK( PAM__c) || PAM__c=0 , 0, SESales__c  /  PAM__c)</formula>
        <label>% Share of Wallet</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
