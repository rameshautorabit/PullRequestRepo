    <validationRules>
        <fullName>APP_VR01_CurrencyConsistency</fullName>
        <active>true</active>
        <description>SR0 - Make sure selected currency is the same as account currency</description>
        <errorConditionFormula>NOT( $User.BypassVR__c )
&amp;&amp;
 TEXT(CurrencyIsoCode) &lt;&gt;  TEXT(AccountPlan__r.Account__r.CurrencyIsoCode)</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>Currency should be the same as the one selected at account level.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
    <webLinks>
        <fullName>SR0_APP_NewPotential</fullName>
        <availability>online</availability>
        <description>SR0 - Custom button to create a potential record with currency defaulted from account plan</description>
        <displayType>massActionButton</displayType>
        <encodingKey>UTF-8</encodingKey>
        <linkType>url</linkType>
        <masterLabel>New Potential</masterLabel>
        <openType>replace</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>/a0R/e?CurrencyIsoCode={!SFE_AccPlan__c.CurrencyIsoCode}&amp;CF00NA000000559La={!SFE_AccPlan__c.Name}&amp;CF00NA000000559La_lkid={!SFE_AccPlan__c.Id}</url>
    </webLinks>
