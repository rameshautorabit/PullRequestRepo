    <validationRules>
        <fullName>CCL_VR01_ReasonDependencies</fullName>
        <active>true</active>
        <description>2012R04 - Enforce the reason to be filled in if a sub reason has been entered.</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), ISBLANK( Reason__c ), NOT(ISBLANK(SubReason__c)))</errorConditionFormula>
        <errorDisplayField>Reason__c</errorDisplayField>
        <errorMessage>You must enter a reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCL_VR02_CategoryDependencies</fullName>
        <active>true</active>
        <description>2012R04 - Enforce the category to be filled in if a reason has been entered.</description>
        <errorConditionFormula>AND(ISBLANK( Category__c ) ,NOT(ISBLANK(Reason__c)), NOT($User.BypassVR__c))</errorConditionFormula>
        <errorDisplayField>Category__c</errorDisplayField>
        <errorMessage>You must enter a category.</errorMessage>
    </validationRules>
