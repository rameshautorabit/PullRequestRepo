    <validationRules>
        <fullName>PRF_VR_MAY13PRM01_CheckGlobalFeatureSts</fullName>
        <active>true</active>
        <description>Check if the global feature status is active or not when the child feature is updated to active</description>
        <errorConditionFormula>AND($User.BypassVR__c=False,(IF(
AND(
 GlobalProgramFeature__c  != null ,
NOT( ISPICKVAL(  GlobalProgramFeature__r.FeatureStatus__c  , &apos;Active&apos;) ),
ISPICKVAL(FeatureStatus__c,&apos;Active&apos;),NOT( ISCHANGED( TECH_InactiveGlobalFeature__c ))),true,false)))</errorConditionFormula>
        <errorMessage>Please activate the Parent Feature at Global Level and then try again</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRF_VR_MAY13PRM02_CheckUserAccess</fullName>
        <active>true</active>
        <description>To check that only Program Owners can create/edit global program features.</description>
        <errorConditionFormula>AND($User.BypassVR__c=False,(IF(
AND(
(
NOT
(OR(
$User.ProgramAdministrator__c ,
$User.ProgramApprover__c , 
$User.ProgramOwner__c
))),
RecordType.Id = $Label.CLMAY13PRM06),
true,
false)))</errorConditionFormula>
        <errorMessage>Only Program Owners/Program Administrators can create/edit global program features.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRF_VR_MAY13PRM03_CheckParentLevelSts</fullName>
        <active>true</active>
        <description>Feature can be activated only if the parent Level is active.</description>
        <errorConditionFormula>AND($User.BypassVR__c=False,(IF(
AND(
 ProgramLevel__c  != null ,
NOT( ISPICKVAL(  ProgramLevel__r.LevelStatus__c  , &apos;Active&apos;) ),
ISPICKVAL( FeatureStatus__c ,&apos;Active&apos;)),true,false)))</errorConditionFormula>
        <errorMessage>Please activate the Parent Level and then try again</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRF_VR_MAY13PRM04_CantChangeLevelFeature</fullName>
        <active>true</active>
        <description>For May 13 PRM release
Feature Catalog and Program Level cannot be updated</description>
        <errorConditionFormula>AND($User.BypassVR__c=False,(IF(
AND(
OR(
ISCHANGED( ProgramLevel__c ) ,
ISCHANGED( FeatureCatalog__c )),
OR(
$User.ProgramManager__c ,
$User.ProgramOwner__c )


),
true,false
)))</errorConditionFormula>
        <errorMessage>Feature catalog or program level cannot be edited. Please inactivate this feature and assign a new one with the right Feature and/or Program Level selection</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRF_VR_OCT14PRM01_CheckGlobalProgramOwnr</fullName>
        <active>true</active>
        <description>To check whether the user is global program owner.</description>
        <errorConditionFormula>AND($User.BypassVR__c=False,(IF(
AND(
(
NOT
(OR(
$User.ProgramAdministrator__c ,
$User.ProgramApprover__c , 
 AND($User.ProgramOwner__c, 
     $User.Id  =   PartnerProgram__r.OwnerId)
))),
RecordType.Id = $Label.CLMAY13PRM06),
true,
false)))</errorConditionFormula>
        <errorMessage>Only Global Program owners can create/edit global program features.</errorMessage>
    </validationRules>
