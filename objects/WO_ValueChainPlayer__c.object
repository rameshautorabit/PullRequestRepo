    <validationRules>
        <fullName>WOVCP_Account_PM</fullName>
        <active>true</active>
        <description>Prevent a user to fil both account and contact</description>
        <errorConditionFormula>AND(
		NOT($User.BypassVR__c),
OR(
(!ISBLANK( Account__c ) &amp;&amp; (!ISBLANK( Contact__c ) || !ISBLANK( User__c ) || !ISPICKVAL( ContactRole__c , &quot;&quot;) ))
|| ( !ISBLANK( AccountDetails__c ) &amp;&amp; (!ISBLANK( Contact__c ) || !ISBLANK( User__c ) || !ISPICKVAL( ContactRole__c , &quot;&quot;) ))
|| ( !ISPICKVAL( AccountRole__c , &quot;&quot;) &amp;&amp; (!ISBLANK( Contact__c ) || !ISBLANK( User__c ) || !ISPICKVAL( ContactRole__c , &quot;&quot;) ))
|| (!ISBLANK( Contact__c ) &amp;&amp; (!ISBLANK( Account__c ) || !ISBLANK( AccountDetails__c ) || !ISPICKVAL( AccountRole__c , &quot;&quot;) ))
|| ( !ISBLANK( User__c ) &amp;&amp; (!ISBLANK( Account__c ) || !ISBLANK( AccountDetails__c ) || !ISPICKVAL( AccountRole__c , &quot;&quot;) ))
|| ( !ISPICKVAL( ContactRole__c , &quot;&quot;) &amp;&amp; (!ISBLANK( Account__c ) || !ISBLANK( AccountDetails__c ) || !ISPICKVAL( AccountRole__c , &quot;&quot;) ))
)
)</errorConditionFormula>
        <errorMessage>You cannot fill both Account and contact user</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WOVCP_Id_role</fullName>
        <active>true</active>
        <description>user cannot modify the a wcp with a account to a vcp with a contact (he will have to create another one)</description>
        <errorConditionFormula>AND(
		NOT($User.BypassVR__c),

OR(
( PRIORVALUE(Account__c ) != null &amp;&amp; (!ISBLANK( Contact__c ) || !ISBLANK( User__c ) || !ISPICKVAL( ContactRole__c , &quot;&quot;) ))
|| (PRIORVALUE(AccountDetails__c) != null &amp;&amp; (!ISBLANK( Contact__c ) || !ISBLANK( User__c ) || !ISPICKVAL( ContactRole__c , &quot;&quot;) ))
|| ( PRIORVALUE(AccountRole__c) != &quot;&quot; &amp;&amp; (!ISBLANK( Contact__c ) || !ISBLANK( User__c ) || !ISPICKVAL( ContactRole__c , &quot;&quot;) ))
|| (PRIORVALUE(Contact__c ) != null &amp;&amp; (!ISBLANK( Account__c ) || !ISBLANK( AccountDetails__c ) || !ISPICKVAL( AccountRole__c , &quot;&quot;) ))
|| (PRIORVALUE(User__c ) != null &amp;&amp; (!ISBLANK( Account__c ) || !ISBLANK( AccountDetails__c ) || !ISPICKVAL( AccountRole__c , &quot;&quot;) ))
|| (PRIORVALUE(ContactRole__c) != &quot;&quot; &amp;&amp; (!ISBLANK( Account__c ) || !ISBLANK( AccountDetails__c ) || !ISPICKVAL( AccountRole__c , &quot;&quot;) ))
)
)</errorConditionFormula>
        <errorMessage>You can&apos;t modify this VCP to change his type (account/contact)</errorMessage>
    </validationRules>
