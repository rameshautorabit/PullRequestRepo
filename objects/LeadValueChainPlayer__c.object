    <validationRules>
        <fullName>LVCP_VR01_CheckContact</fullName>
        <active>true</active>
        <description>If Account &amp; Contact are both mapped, Contact should be associated with the Account.</description>
        <errorConditionFormula>NOT($User.BypassVR__c) 
&amp;&amp; NOT(ISBLANK( Account__c )) 
&amp;&amp; NOT(ISBLANK( Contact__c )) 
&amp;&amp; Contact__r.AccountId &lt;&gt; Account__r.Id</errorConditionFormula>
        <errorDisplayField>Contact__c</errorDisplayField>
        <errorMessage>This contact is not attached to the selected account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LVCP_VR02_InformationConsistency</fullName>
        <active>true</active>
        <description>If Account Role or Contact Role is specified and Account or Contact is blank, error message should be displayed.</description>
        <errorConditionFormula>NOT($User.BypassVR__c) 
&amp;&amp;( 
( 
NOT(ISBLANK(TEXT( AccountRole__c ))) 
&amp;&amp; ISBLANK(Account__c) 
) 
|| 
( 
NOT(ISBLANK(TEXT( ContactRole__c ))) 
&amp;&amp; ISBLANK( Contact__c ) 
) 
|| 
( 
ISBLANK( Contact__c ) &amp;&amp; ISBLANK(Account__c) 
) 
)</errorConditionFormula>
        <errorMessage>Please enter either an Account or a Contact with an associated role.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LVCP_VR03_AccountControl</fullName>
        <active>true</active>
        <description>If Account selected has been marked for deletion, then please select another Account.</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp; ( Account__r.ToBeDeleted__c = TRUE )</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>This Account has been marked for deletion, please choose another one.</errorMessage>
    </validationRules>
