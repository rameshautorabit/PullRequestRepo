    <validationRules>
        <fullName>CRC_VR_Q316I2P01_CommercialRefOroderdChe</fullName>
        <active>true</active>
        <errorConditionFormula>AND($User.BypassVR__c=False,AND(
OR(
AND(
ISPICKVAL(Complaint_Request__r.Category__c, &apos;Pricing and Availability&apos;)
) ,
AND(
ISPICKVAL(Complaint_Request__r.Category__c, &apos;Quotation Request&apos;) ,
ISPICKVAL(Complaint_Request__r.Reason__c, &apos;Non-standard Offer&apos;)
) ,

AND(
ISPICKVAL(Complaint_Request__r.Category__c, &apos;Quotation Request&apos;) ,
ISPICKVAL(Complaint_Request__r.Reason__c, &apos;Standard Offer&apos;)
),
AND(
ISPICKVAL(Complaint_Request__r.Category__c, &apos;Others*&apos;) ,
ISPICKVAL(Complaint_Request__r.Reason__c, &apos;Special Price&apos;)
),
AND(ISPICKVAL( Complaint_Request__r.Reason__c , &apos;Delivery before standard lead time&apos;),
ISPICKVAL(Complaint_Request__r.Category__c ,&apos;Order Modification&apos;)
),

AND(ISPICKVAL( Complaint_Request__r.Reason__c , &apos;Late Delivery, not Received Yet*&apos;),
ISPICKVAL(Complaint_Request__r.Category__c ,&apos;Delivery or Collection Issues*&apos;)
),
AND(ISPICKVAL( Complaint_Request__r.Reason__c , &apos;Labeling not Compliant*&apos;),
ISPICKVAL(Complaint_Request__r.Category__c ,&apos;Delivery or Collection Issues*&apos;)
),
AND(ISPICKVAL( Complaint_Request__r.Reason__c , &apos;Packaging not Compliant*&apos;),
ISPICKVAL(Complaint_Request__r.Category__c ,&apos;Delivery or Collection Issues*&apos;)
),
AND(
ISPICKVAL( Complaint_Request__r.Reason__c , &apos;Reference Received Incorrect*&apos;),
ISPICKVAL(Complaint_Request__r.Category__c ,&apos;Delivery or Collection Issues*&apos;),
OR
(

AND(Complaint_Request__r.CommercialReferenceReceived__c==&apos;&apos;,Complaint_Request__r.CommercialReferenceOrdered__c=&apos;&apos;)

)
)
),
AND(Complaint_Request__r.CommercialReferenceOrdered__c=&apos;&apos;,Complaint_Request__r.TECH_CROrderFamily__c=&apos;&apos;)
),NOT(ISPICKVAL(Complaint_Request__r.Status__c,&apos;Cancelled&apos;)))</errorConditionFormula>
        <errorMessage>Please enter commercial reference ordered Or Family ordered on Complaint Request before create comments.</errorMessage>
    </validationRules>
