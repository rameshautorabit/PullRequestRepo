    <validationRules>
        <fullName>FieloEE__Available_Days</fullName>
        <active>true</active>
        <errorConditionFormula>FieloEE__AvailableDays__c &lt; 0</errorConditionFormula>
        <errorDisplayField>FieloEE__AvailableDays__c</errorDisplayField>
        <errorMessage>value must be positive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloEE__ChangeStockMode</fullName>
        <active>true</active>
        <errorConditionFormula>((ISPICKVAL(PRIORVALUE(FieloEE__VoucherCreationMode__c), &apos;Controlled By Quantity&apos;) &amp;&amp; (ISPICKVAL(FieloEE__VoucherCreationMode__c, &apos;Unlimited&apos;) || ISPICKVAL(FieloEE__VoucherCreationMode__c, &apos;Controlled By Item&apos;))) ||
(ISPICKVAL(PRIORVALUE(FieloEE__VoucherCreationMode__c), &apos;Controlled By Item&apos;) &amp;&amp; (ISPICKVAL(FieloEE__VoucherCreationMode__c, &apos;Unlimited&apos;) ||
ISPICKVAL(FieloEE__VoucherCreationMode__c, &apos;Controlled By Quantity&apos;)))) &amp;&amp;
FieloEE__Stock__c &gt; 0</errorConditionFormula>
        <errorDisplayField>FieloEE__VoucherCreationMode__c</errorDisplayField>
        <errorMessage>This operation is allowed when stock is equal to zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloEE__ModifyProgram</fullName>
        <active>true</active>
        <description>Is not possible to modify the associated Program</description>
        <errorConditionFormula>ISCHANGED(FieloEE__Program__c) &amp;&amp; NOT(ISBLANK(PRIORVALUE(FieloEE__Program__c)))</errorConditionFormula>
        <errorDisplayField>FieloEE__Program__c</errorDisplayField>
        <errorMessage>Is not possible to modify the associated Program</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloEE__MultiVoucher</fullName>
        <active>true</active>
        <errorConditionFormula>FieloEE__UniqueVoucher__c &amp;&amp;  ISPICKVAL(FieloEE__VoucherCreationMode__c, &apos;Controlled By Item&apos;)</errorConditionFormula>
        <errorMessage>Rewards controlled By Item with preloaded vouchers can&apos;t be unique voucher</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloEE__Points_Required</fullName>
        <active>true</active>
        <description>Validation to set allways a point value to the reward</description>
        <errorConditionFormula>ISNULL( FieloEE__PointsCant__c )</errorConditionFormula>
        <errorDisplayField>FieloEE__PointsCant__c</errorDisplayField>
        <errorMessage>The points of the reward are mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloEE__Price_Possitive</fullName>
        <active>true</active>
        <errorConditionFormula>FieloEE__Price__c &lt; 0</errorConditionFormula>
        <errorDisplayField>FieloEE__Price__c</errorDisplayField>
        <errorMessage>value must be positive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloEE__Quantity_Points_Positive</fullName>
        <active>true</active>
        <errorConditionFormula>FieloEE__PointsCant__c &lt; 0</errorConditionFormula>
        <errorDisplayField>FieloEE__PointsCant__c</errorDisplayField>
        <errorMessage>value must be positive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloEE__Stock_Positive</fullName>
        <active>true</active>
        <description>Stock Must be positive</description>
        <errorConditionFormula>AND(IF(ISCHANGED(FieloEE__Stock__c) ,
IF( OR(ISBLANK(FieloEE__Stock__c) , FieloEE__Stock__c &lt; 0) ,
true,
false),
false), NOT( ISPICKVAL(FieloEE__VoucherCreationMode__c, &apos;Unlimited&apos;) ))</errorConditionFormula>
        <errorMessage>The stock should be positive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloEE__Unlimited</fullName>
        <active>false</active>
        <errorConditionFormula>FieloEE__Intangible__c = TRUE &amp;&amp;  ISPICKVAL( FieloEE__VoucherCreationMode__c , &apos;Preload&apos; )</errorConditionFormula>
        <errorMessage>A reward cannot be unlimited and preload at the same time</errorMessage>
    </validationRules>
