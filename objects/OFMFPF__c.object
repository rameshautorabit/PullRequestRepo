    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>Read</externalSharingModel>
    <fields>
        <fullName>FPF_Name__c</fullName>
        <description>FPF Name</description>
        <externalId>false</externalId>
        <formula>Name</formula>
        <inlineHelpText>Name of the FPF File Uploaded</inlineHelpText>
        <label>FPF Name</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>NoofProducts__c</fullName>
        <description>Will store the number of products having childs</description>
        <externalId>false</externalId>
        <label>Number of Products</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>noOfTechRtns__c</fullName>
        <description>Shows the total number technical returns associated with FPF</description>
        <externalId>false</externalId>
        <label>Number of Technical Returns</label>
        <summaryForeignKey>FPF_TechnicalReturn__c.OFMFPF__c</summaryForeignKey>
        <summaryOperation>count</summaryOperation>
        <trackTrending>false</trackTrending>
        <type>Summary</type>
    </fields>
    <label>OFM FPF</label>
    <listViews>
        <fullName>All</fullName>
        <columns>NAME</columns>
        <columns>NoofProducts__c</columns>
        <columns>noOfTechRtns__c</columns>
        <columns>CREATEDBY_USER</columns>
        <columns>CREATED_DATE</columns>
        <columns>LAST_UPDATE</columns>
        <columns>OWNER.FIRST_NAME</columns>
        <columns>OWNER.LAST_NAME</columns>
        <filterScope>Everything</filterScope>
        <label>All Favourite Product Family</label>
    </listViews>
    <listViews>
        <fullName>FPF_For_Indonesia</fullName>
        <columns>NAME</columns>
        <columns>NoofProducts__c</columns>
        <columns>noOfTechRtns__c</columns>
        <columns>CREATEDBY_USER</columns>
        <columns>CREATED_DATE</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>NAME</field>
            <operation>contains</operation>
            <value>Indonesia</value>
        </filters>
        <label>FPF For Indonesia</label>
        <language>en_US</language>
        <sharedTo>
            <group>OFM_Support_Group</group>
            <group>OFM_Target_Audience_Users</group>
        </sharedTo>
    </listViews>
