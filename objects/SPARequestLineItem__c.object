    <fields>
        <fullName>AdditionalDiscount__c</fullName>
        <description>This value determine the Final Discount/Multiplier. (Final Discount = Standard Discount x Additional Discount)</description>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(RequestedType__c,&apos;Multiplier&apos;), 1- (Round(RequestedValue__c,4)/ (1- StandardDiscount__c )) ,
IF(ISPICKVAL(RequestedType__c,&apos;Discount&apos;),Round(RequestedValue__c,2)/100,
IF(ISPICKVAL(RequestedType__c,&apos;Price&apos;), 1-(RequestedValue__c/ StandardPrice__c ) ,
IF(ISPICKVAL(RequestedType__c,&apos;Advised Discount %&apos;),Round(RequestedValue__c,2)/100,
IF(ISPICKVAL(RequestedType__c,&apos;Standard Discount&apos;),Round(RequestedValue__c,2)/100,
IF(ISPICKVAL(RequestedType__c,&apos;Free of charge&apos;),1,
0))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>This value determine the Final Discount/Multiplier. (Final Discount = Standard Discount x Additional Discount)</inlineHelpText>
        <label>Additional Discount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
