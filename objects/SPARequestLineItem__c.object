    <validationRules>
        <fullName>SPA_VR_OCT14SLS01_CompensationMandatory</fullName>
        <active>true</active>
        <description>Compensation is mandatory field for Advised Discount.</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp;  ISPICKVAL(RequestedType__c, &apos;Advised Discount %&apos;)  &amp;&amp; ISBLANK(Compensation__c)</errorConditionFormula>
        <errorMessage>Compensation % is mandatory for the Advised Discount % Requested Type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_SEP12SLS01_RLI_TargetQuantity</fullName>
        <active>true</active>
        <description>Target Quantity should be greater than 0</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c),
OR ( TargetQuantity__c  &lt;= 0 ,   ISBLANK(TargetQuantity__c)  ) )</errorConditionFormula>
        <errorDisplayField>TargetQuantity__c</errorDisplayField>
        <errorMessage>Target Quantity should be greater than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_SEP12SLS03_RLI_ReqMultiplLessThn1</fullName>
        <active>true</active>
        <description>Requested Multiplier should not be greater than 1</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c), 
ISPICKVAL( RequestedType__c, &apos;Multiplier&apos;), 
(RequestedValue__c &gt;1))</errorConditionFormula>
        <errorDisplayField>RequestedValue__c</errorDisplayField>
        <errorMessage>Final Multiplier should not exceed 1</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_SEP12SLS04_RLI_NetPrice</fullName>
        <active>true</active>
        <description>Net Price should less than Standard Price and List Price</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c),
AND ( ISPICKVAL( RequestedType__c ,&apos;Price&apos; ) , OR (RequestedValue__c  &gt; StandardPrice__c,RequestedValue__c &gt; ListPrice__c ) ) )</errorConditionFormula>
        <errorDisplayField>RequestedValue__c</errorDisplayField>
        <errorMessage>Net Price should be less than both, Standard Price &amp; List Price</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_SEP12SLS05_RLI_FinalMultiplier</fullName>
        <active>true</active>
        <description>Final Multiplier should less than Standard Multiplier</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c),
AND ( ISPICKVAL( RequestedType__c ,&apos;Multiplier&apos; ) ,  RequestedValue__c  &gt;  StandardMultiplier__c ) )</errorConditionFormula>
        <errorDisplayField>RequestedValue__c</errorDisplayField>
        <errorMessage>Final Multiplier should be less than Standard Multiplier</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_SEP12SLS06_RLI_RequestedDiscount</fullName>
        <active>true</active>
        <description>Additional Discount should not be greater than 100</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c), 
ISPICKVAL( RequestedType__c, &apos;Discount&apos;), 
(RequestedValue__c &gt;100))</errorConditionFormula>
        <errorDisplayField>RequestedValue__c</errorDisplayField>
        <errorMessage>Additional Discount should not exceed 100</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_SEP12SLS07_RLI_NumericFields</fullName>
        <active>true</active>
        <description>Numeric fields should not be less than</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c), 
OR ( ListPrice__c &lt; 0 , RequestedValue__c&lt; 0 , StandardDiscount__c&lt; 0 ,  StandardMultiplier__c&lt; 0 , StandardPrice__c&lt; 0) )</errorConditionFormula>
        <errorMessage>Value should not be less than 0</errorMessage>
    </validationRules>
