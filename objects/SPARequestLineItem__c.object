    <fields>
        <fullName>FinalMultiplier__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(RequestedType__c,&apos;Multiplier&apos;),Round(RequestedValue__c,4),
IF(ISPICKVAL(RequestedType__c,&apos;Discount&apos;), ((100- Round(RequestedValue__c,2))/100) * (1- StandardDiscount__c) ,
IF(ISPICKVAL(RequestedType__c,&apos;Price&apos;), RequestedValue__c/ ListPrice__c ,
IF(ISPICKVAL(RequestedType__c,&apos;Advised Discount %&apos;),1-(Round(RequestedValue__c,2)/100),
IF(ISPICKVAL(RequestedType__c,&apos;Standard Discount&apos;), 1-(Round(RequestedValue__c,2)/100) ,
0)))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Final Multiplier</label>
        <precision>18</precision>
        <required>false</required>
        <scale>4</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
