<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <deploymentStatus>Deployed</deploymentStatus>
    <description>This platforming event is sent when an account or a contact that has been created by self-registration (so, with a self-service creation status equal to &quot;New&quot;) receives a Golden Id from SDH. Objective is to notify UIMS that the cross-ref bFO Id to Golden Id is available in SDH for further synchronization between systems.</description>
    <eventType>StandardVolume</eventType>
    <fields>
        <fullName>IdList__c</fullName>
        <description>Contains the list of Ids for the Account or Contacts to be sent to UIMS.</description>
        <externalId>false</externalId>
        <isFilteringDisabled>false</isFilteringDisabled>
        <isNameField>false</isNameField>
        <isSortingDisabled>false</isSortingDisabled>
        <label>List of Ids</label>
        <length>32768</length>
        <type>LongTextArea</type>
        <visibleLines>10</visibleLines>
    </fields>
    <fields>
        <fullName>ObjectAPIName__c</fullName>
        <description>Name of the related Object (Account or Contact).</description>
        <externalId>false</externalId>
        <isFilteringDisabled>false</isFilteringDisabled>
        <isNameField>false</isNameField>
        <isSortingDisabled>false</isSortingDisabled>
        <label>Object API Name</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>RecordId__c</fullName>
        <description>ID of the related Account or Contact.</description>
        <externalId>false</externalId>
        <isFilteringDisabled>false</isFilteringDisabled>
        <isNameField>false</isNameField>
        <isSortingDisabled>false</isSortingDisabled>
        <label>Record Id</label>
        <length>18</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Digital Account - UIMS SDH Notification</label>
    <pluralLabel>Digital Account - UIMS SDH Notifications</pluralLabel>
    <publishBehavior>PublishImmediately</publishBehavior>
</CustomObject>
