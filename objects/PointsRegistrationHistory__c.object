    <validationRules>
        <fullName>PRH_VR_June16_CCC_PreventPointsUpdate</fullName>
        <active>true</active>
        <description>Prevent change of points and Contract on Points Registration record (like transaction on bank account). Thanks to this rule we can keep a track of point balance at the time of points registration (cf Total Points field)</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
OR( ISCHANGED( Points__c ), ISCHANGED( Contract__c))
)</errorConditionFormula>
        <errorMessage>Points and Contract cannot be updated</errorMessage>
    </validationRules>
