    <validationRules>
        <fullName>Approvers_for_Knowledge_Article</fullName>
        <active>true</active>
        <description>Knowledge Article RecordType do not have Management Reviewer / Upper Management reviewer</description>
        <errorConditionFormula>(ISPICKVAL(KnowledgeRecordType__c,&apos;KnowledgeArticle&apos;)) &amp;&amp;
(NOT(ISBLANK(ManagementReviewer__c)) ||
NOT(ISBLANK(UpperManagementReviewer__c)))</errorConditionFormula>
        <errorMessage>For  Knowledge Article Record , Management Reviewer and Upper Management Reviewer should be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Management_review_is_reqd_for_AdvNot</fullName>
        <active>true</active>
        <description>Upper Management Reviewer and Management Reviewer are mandatory for Advisory and Notification</description>
        <errorConditionFormula>( OR( ISPICKVAL(KnowledgeRecordType__c,&apos;Advisory&apos;), ISPICKVAL(KnowledgeRecordType__c,&apos;Notification&apos;)) &amp;&amp; OR(ISBLANK(ManagementReviewer__c), ISBLANK(UpperManagementReviewer__c), ISBLANK(ManagementReviewerSec__c), ISBLANK(UpperManagementReviewerSec__c)) &amp;&amp; NOT($User.BypassVR__c))</errorConditionFormula>
        <errorMessage>Primary and Secondary Upper Management and Management Reviewers are mandatory for Advisory and Notification Articles</errorMessage>
    </validationRules>
