    <validationRules>
        <fullName>PMP_VR01_OCT2013_ActiveSLATerms</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), NOT( SVMXC__SLA_Terms__r.SVMXC__Active__c))</errorConditionFormula>
        <errorDisplayField>SVMXC__SLA_Terms__c</errorDisplayField>
        <errorMessage>Please select an Active SLA Terms</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PM_Plan_VR_FSJUL18_FirstScheduleDate</fullName>
        <active>true</active>
        <description>First Schedule date should be between PM Plan Start and End dates</description>
        <errorConditionFormula>AND(
NOT($User.BypassVR__c),
Recurring_PM_Template__c == true,
OR(First_Schedule_Date__c &lt; SVMXC__Start_Date__c , 
First_Schedule_Date__c &gt; SVMXC__End_Date__c ))</errorConditionFormula>
        <errorMessage>First Schedule Date should be with in PM Plan Start Date and End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PM_Plan_VR_FSJUL18_NoDateChangeForActive</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),
ISCHANGED(First_Schedule_Date__c), 
OR(AND(CountPMHistory__c &gt; 0, Recurring_PM_Template__c == true),Recurring_PM_Template__c == false))</errorConditionFormula>
        <errorMessage>First Schedule Date Can not be modified for Active PM Plan / Non Recurring PM Template</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PM_VR_NOV16_NumberofWO_LOCATIONSPM</fullName>
        <active>true</active>
        <description>For Coverage type=&apos; Location (Must Have Location)&apos;, the number of Work Orders should be &apos;One per Plan&apos;.</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),TEXT(SVMXC__Coverage_Type__c) = &apos;Location (Must Have Location)&apos;, NOT(TEXT(SVMXC__Number_of_Work_orders__c) = &apos;One Per Plan&apos;))</errorConditionFormula>
        <errorMessage>For Coverage type=&apos; Location (Must Have Location)&apos;, the number of Work Orders should be &apos;One per Plan&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PM_VR_R6_NumberofWO_LocationAccount</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT($User.BypassVR__c), OR(ISBLANK(SVMXC__Location__c),ISBLANK(SVMXC__Account__c)),(TEXT(SVMXC__Number_of_Work_orders__c) = &apos;One Per Plan&apos;))</errorConditionFormula>
        <errorMessage>Account and Location is required if Number of Work orders is &quot;One Per Plan&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SVMXC__PM_Plan_Start_End_Date_Validation</fullName>
        <active>true</active>
        <description>Start date of preventive maintenance plan cannot be less than end time.</description>
        <errorConditionFormula>IF( SVMXC__Start_Date__c &gt;  SVMXC__End_Date__c, true, false)</errorConditionFormula>
        <errorMessage>End date should be greater than start date of Preventive Maintenance Plan</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
