    <fields>
        <fullName>CustomerProfile__c</fullName>
        <description>SR0 - Formula field to define Account profile based on Penetration and score</description>
        <externalId>false</externalId>
        <formula>IF(OR(ISBLANK(Score__c),ISBLANK(MktShare__c)),&quot;Not Completed&quot;,(IF (  (( MktShare__c &gt;  IndivCAP__r.MktShareThreshold2__c )  &amp;&amp; ( (Score__c / 100)&lt;=  IndivCAP__r.AttractiAxisThreshold1__c )) , &quot;S1&quot;,

IF (  ((  MktShare__c &gt; IndivCAP__r.MktShareThreshold2__c )  &amp;&amp; ( ((Score__c / 100) &gt; IndivCAP__r.AttractiAxisThreshold1__c ) &amp;&amp; ( (Score__c / 100) &lt;=  IndivCAP__r.AttractiAxisThreshold2__c ))), &quot;S2&quot;,

IF (  ((  MktShare__c &gt; IndivCAP__r.MktShareThreshold2__c )  &amp;&amp; (  (Score__c / 100) &gt; IndivCAP__r.AttractiAxisThreshold2__c )), &quot;S3&quot;,

IF (  (((  MktShare__c &gt;   IndivCAP__r.MktShareThreshold1__c )  &amp;&amp; ( MktShare__c &lt;= IndivCAP__r.MktShareThreshold2__c ))  &amp;&amp; ( (Score__c / 100) &lt;=  IndivCAP__r.AttractiAxisThreshold1__c )), &quot;G1&quot;,

IF (  (((  MktShare__c &gt;   IndivCAP__r.MktShareThreshold1__c )  &amp;&amp; ( MktShare__c &lt;= IndivCAP__r.MktShareThreshold2__c ))  &amp;&amp; ((( (Score__c / 100) &gt; IndivCAP__r.AttractiAxisThreshold1__c ) &amp;&amp; ( (Score__c / 100) &lt;=  IndivCAP__r.AttractiAxisThreshold2__c )))), &quot;G2&quot;,

IF (  (((  MktShare__c &gt;   IndivCAP__r.MktShareThreshold1__c )  &amp;&amp; ( MktShare__c &lt;= IndivCAP__r.MktShareThreshold2__c ))  &amp;&amp; (  (Score__c / 100) &gt; IndivCAP__r.AttractiAxisThreshold2__c )), &quot;G3&quot;,

IF (  ((  MktShare__c &lt;=   IndivCAP__r.MktShareThreshold1__c )  &amp;&amp; ( (Score__c / 100) &lt;=  IndivCAP__r.AttractiAxisThreshold1__c )) , &quot;Q1&quot;,

IF (  ((  MktShare__c &lt;=   IndivCAP__r.MktShareThreshold1__c )  &amp;&amp; (( (Score__c / 100) &gt; IndivCAP__r.AttractiAxisThreshold1__c ) &amp;&amp; ( (Score__c / 100) &lt;=  IndivCAP__r.AttractiAxisThreshold2__c ))), &quot;Q2&quot;,

IF (  ((  MktShare__c &lt;=   IndivCAP__r.MktShareThreshold1__c )  &amp;&amp; ( (Score__c / 100) &gt; IndivCAP__r.AttractiAxisThreshold2__c )), &quot;Q3&quot;, &quot;Not Completed&quot;)))))))))))</formula>
        <label>CAP Profile</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
