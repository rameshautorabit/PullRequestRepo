    <validationRules>
        <fullName>SCO_VR01_DirectIndirectSalesControl</fullName>
        <active>true</active>
        <description>SR0 - Ensure that Direct sales + indirect sales are less or equal to PAM</description>
        <errorConditionFormula>NOT($User.BypassVR__c) 
&amp;&amp; 
PAMPlaforming__c &lt; (FLOOR(DirectSalesPlatforming__c) + FLOOR(IndirectSalesPlatforming__c) )</errorConditionFormula>
        <errorDisplayField>PAMPlaforming__c</errorDisplayField>
        <errorMessage>PAM should be higher or equal to Direct Sales + Indirect Sales</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SCO_VR02_OnActionPlanUpdt</fullName>
        <active>true</active>
        <description>September Release - To prevent users from modifying the Action Plan to which the Platforming scoring is attached.</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp; 
ISCHANGED( IndivCAP__c )</errorConditionFormula>
        <errorDisplayField>IndivCAP__c</errorDisplayField>
        <errorMessage>Commercial Action Plan can&apos;t be changed.</errorMessage>
    </validationRules>
