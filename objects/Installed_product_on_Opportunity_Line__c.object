    <fields>
        <fullName>LifeCycleStatus__c</fullName>
        <externalId>false</externalId>
        <formula>IF((TEXT(Installed_Product__r.LifeCycleStatusOfTheInstalledProduct__c))==&quot;NOT_USED&quot;,&quot;At Customer Location - Not Used&quot;,
IF((TEXT(Installed_Product__r.LifeCycleStatusOfTheInstalledProduct__c))==&quot;LOST&quot;,&quot;Lost or Stolen&quot;,
IF((TEXT(Installed_Product__r.LifeCycleStatusOfTheInstalledProduct__c))==&quot;REMOVED&quot;,&quot;Removed for Repair&quot;,
IF((TEXT(Installed_Product__r.LifeCycleStatusOfTheInstalledProduct__c))==&quot;IN_USE&quot;,&quot;Commissioned / In Use&quot;,
IF((TEXT(Installed_Product__r.LifeCycleStatusOfTheInstalledProduct__c))==&quot;SCRAPPED&quot;,&quot;Scrapped&quot;,
IF((TEXT(Installed_Product__r.LifeCycleStatusOfTheInstalledProduct__c))==&quot;SUPPLY_CHAIN&quot;,&quot;In Supply Chain&quot;,&quot;&quot;))))))</formula>
        <label>LifeCycle Status</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
