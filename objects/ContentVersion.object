    <validationRules>
        <fullName>DeviceTypeMandatory</fullName>
        <active>true</active>
        <description>Device type is mandatory</description>
        <errorConditionFormula>ISPICKVAL(Product_range__c, &apos;&apos;) &amp;&amp;  RecordType.DeveloperName = &apos;Partners_Com_X&apos; &amp;&amp; NOT($User.BypassVR__c)</errorConditionFormula>
        <errorDisplayField>Product_range__c</errorDisplayField>
        <errorMessage>Device type is missing</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DeviceTypeOtherMandatory</fullName>
        <active>true</active>
        <description>Device Type Other Value is mandatory if Device type equals Other</description>
        <errorConditionFormula>AND 
( 
ISPICKVAL( Product_range__c, &apos;Other&apos;) , OR (ISNULL(Device_Type_New_Value__c ) , ISBLANK(Device_Type_New_Value__c )) 
) &amp;&amp; NOT($User.BypassVR__c) &amp;&amp;  RecordType.DeveloperName = &apos;Partners_Com_X&apos;</errorConditionFormula>
        <errorMessage>*** The device Type &apos;Other&apos; value is missing</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ProductBrandMandatory</fullName>
        <active>true</active>
        <description>Product Brand is mandatory</description>
        <errorConditionFormula>ISPICKVAL( Product_Manufacturer__c , &apos;&apos;) &amp;&amp; NOT($User.BypassVR__c) &amp;&amp;  RecordType.DeveloperName = &apos;Partners_Com_X&apos;</errorConditionFormula>
        <errorMessage>*** Brand is missing, select the country before</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ProductBrandOtherMandatory</fullName>
        <active>true</active>
        <description>The product brand &apos;Other&apos; value is mandatory if Product Brand equals Other</description>
        <errorConditionFormula>AND 
( 
ISPICKVAL( Product_Manufacturer__c , &apos;Other&apos;) , OR (ISNULL(Product_Brand_New_Value__c ) , ISBLANK(Product_Brand_New_Value__c )) 
) &amp;&amp; NOT($User.BypassVR__c) &amp;&amp;  RecordType.DeveloperName = &apos;Partners_Com_X&apos;</errorConditionFormula>
        <errorMessage>*** The product brand &apos;Other&apos; value is missing</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ProductPartNumberMandatory</fullName>
        <active>true</active>
        <description>Product part number is mandatory</description>
        <errorConditionFormula>OR (ISBLANK( Product_Part_Number__c ), ISNULL( Product_Part_Number__c )) &amp;&amp; NOT($User.BypassVR__c) &amp;&amp;  RecordType.DeveloperName = &apos;Partners_Com_X&apos;</errorConditionFormula>
        <errorMessage>*** The product part number is missing</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidatedbySEProfile</fullName>
        <active>true</active>
        <description>Only standard users can update this field</description>
        <errorConditionFormula>AND( 
ISCHANGED(Validated_by_Schneider__c), 
ISPICKVAL($Profile.UserType, &quot;Partner&quot;)|| 
ISPICKVAL($Profile.UserType, &quot;PowerPartner&quot;)|| 
ISPICKVAL($Profile.UserType, &quot;Customer Portal Manager&quot;)|| 
ISPICKVAL($Profile.UserType, &quot;Customer Portal User&quot;)|| 
ISPICKVAL($Profile.UserType, &quot;Guest&quot;)|| 
ISPICKVAL($Profile.UserType, &quot;High Volume Portal&quot;)|| 
ISPICKVAL($Profile.UserType, &quot;CSN Only&quot;)|| 
ISPICKVAL($Profile.UserType, &quot;Self Service&quot;)) &amp;&amp; NOT($User.BypassVR__c) &amp;&amp;  RecordType.DeveloperName = &apos;Partners_Com_X&apos;</errorConditionFormula>
        <errorDisplayField>Validated_by_Schneider__c</errorDisplayField>
        <errorMessage>Not allow to update this field</errorMessage>
    </validationRules>
