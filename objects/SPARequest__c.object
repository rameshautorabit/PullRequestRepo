    <validationRules>
        <fullName>SPA_VR_Jul14SLS01_RequestedTypeChange</fullName>
        <active>true</active>
        <errorConditionFormula>NOT($User.BypassVR__c)&amp;&amp; ISCHANGED(RequestedType__c) &amp;&amp; TECH_HasSPARequestLineItem__c &gt;0</errorConditionFormula>
        <errorMessage>Requested Type cannot be changed for SPA Request with atleast one Line Item</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_Jul14SLS02_RequestedTypeMandatory</fullName>
        <active>true</active>
        <errorConditionFormula>NOT($User.BypassVR__c)&amp;&amp;  ISBLANK(TEXT(RequestedType__c))</errorConditionFormula>
        <errorMessage>Requested Type is mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_May13SLS01_OppOrAccMandatory</fullName>
        <active>true</active>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp; ISBLANK( Account__c ) &amp;&amp; ISBLANK(  Opportunity__c )</errorConditionFormula>
        <errorMessage>Opportunity or Account is mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_May13SLS02_StartDateBeforeEndDate</fullName>
        <active>true</active>
        <description>Validation rule to ensure that the Start Date is before the End Date</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), StartDate__c &gt;= EndDate__c)</errorConditionFormula>
        <errorMessage>Start Date should be before the End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_May13SLS03_IEDLessThanED</fullName>
        <active>true</active>
        <description>Validation rule to ensure that the Initial End Date is before the End Date</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), NOT ISNULL(InitialEndDate__c), OR( InitialEndDate__c &gt;= EndDate__c, InitialEndDate__c &lt;= StartDate__c ))</errorConditionFormula>
        <errorMessage>Initial End Date should be between the Start Date and End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPA_VR_May14SLS04_ChnlCtcMandatory</fullName>
        <active>true</active>
        <description>Channel Contact mandatory if Channel contains value</description>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp; NOT(ISBLANK( Channel__c )) &amp;&amp; ISBLANK( ChannelContact__c )</errorConditionFormula>
        <errorDisplayField>ChannelContact__c</errorDisplayField>
        <errorMessage>Please enter a Channel Contact</errorMessage>
    </validationRules>
