        <formula>HYPERLINK(IF($User.Country=&apos;NL&apos;,(&quot;/lightning/r/Report/00OA0000005cYhv/view?fv0=&quot;+Account__c+&quot;&amp;fv1=&quot;+Account__c+&quot;&amp;fv2=&quot;+Account__c+&quot;&amp;fv3=&quot;+(TEXT(DAY(StartDate__c))+&quot;-&quot;+TEXT(MONTH(StartDate__c))+&quot;-&quot;+TEXT(YEAR(StartDate__c)))+&quot;&amp;fv4=&quot;+(TEXT(DAY(EndDate__c))+&quot;-&quot;+TEXT(MONTH(EndDate__c))+&quot;-&quot;+TEXT(YEAR(EndDate__c)))+&quot;&amp;fv5=&quot;+(TEXT(DAY(EndDate__c))+&quot;-&quot;+TEXT(MONTH(EndDate__c))+&quot;-&quot;+TEXT(YEAR(EndDate__c)))+&quot;&amp;fv10=&quot;+REP_UltimateParentAccount__c+
&quot;&amp;fv11=&quot;+REP_UltimateParentChannel__c+&quot;&amp;fv12=&quot;+REP_UltimateParentAccount__c+&quot;&amp;fv13=&quot;+REP_UltimateParentAccount__c+&quot;&amp;fv14=&quot;+REP_UltimateParentChannel__c+&quot;&amp;fv15=&quot;+REP_UltimateParentAccount__c+&quot;&amp;fv16=&quot;+REP_UltimateParentChannel__c+&quot;&amp;fv17=&quot;+REP_UltimateParentAccount__c+ 
&quot;&amp;fv18=&quot;+IF(ISPICKVAL(BackOfficeSystemID__c,&apos;NL_SAP&apos;),&apos;NL_SAP&apos;,&apos;HK_SAP&apos;)+&quot;&amp;fv19=&quot;+Account__c),(&quot;/lightning/r/Report/00OA0000005cYhv/view?fv0=&quot;+Account__c+&quot;&amp;fv1=&quot;+Account__c+&quot;&amp;fv2=&quot;+Account__c+&quot;&amp;fv3=&quot;+(TEXT(DAY(StartDate__c))+&quot;%2F&quot;+TEXT(MONTH(StartDate__c))+&quot;%2F&quot;+TEXT(YEAR(StartDate__c)))+&quot;&amp;fv4=&quot;+(TEXT(DAY(EndDate__c))+&quot;%2F&quot;+TEXT(MONTH(EndDate__c))+&quot;%2F&quot;+TEXT(YEAR(EndDate__c)))+&quot;&amp;fv5=&quot;+(TEXT(DAY(EndDate__c))+&quot;%2F&quot;+TEXT(MONTH(EndDate__c))+&quot;%2F&quot;+TEXT(YEAR(EndDate__c)))+&quot;&amp;fv10=&quot;+REP_UltimateParentAccount__c+
&quot;&amp;fv11=&quot;+REP_UltimateParentChannel__c+&quot;&amp;fv12=&quot;+REP_UltimateParentAccount__c+&quot;&amp;fv13=&quot;+REP_UltimateParentAccount__c+&quot;&amp;fv14=&quot;+REP_UltimateParentChannel__c+&quot;&amp;fv15=&quot;+REP_UltimateParentAccount__c+&quot;&amp;fv16=&quot;+REP_UltimateParentChannel__c+&quot;&amp;fv17=&quot;+REP_UltimateParentAccount__c+ 
&quot;&amp;fv18=&quot;+IF(ISPICKVAL(BackOfficeSystemID__c,&apos;NL_SAP&apos;),&apos;NL_SAP&apos;,&apos;HK_SAP&apos;)+&quot;&amp;fv19=&quot;+Account__c)),&quot;Click here to display the report&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
