        <errorConditionFormula>AND( NOT( $User.BypassVR__c ),CSQ_DefineMilestoneReviewCompleted__c = TRUE, OR( ISBLANK(CSQ_EstimatedBenefitForTheProject__c) ,  ISBLANK(CSQ_EstimatedCostOfTheProject__c) ))</errorConditionFormula>
        <errorDisplayField>CSQ_DefineMilestoneReviewCompleted__c</errorDisplayField>
