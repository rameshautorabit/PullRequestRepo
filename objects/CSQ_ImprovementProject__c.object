        <fullName>VR_NOV16_IP_MandatoryTeamMemberRecord</fullName>
        <active>true</active>
        <description>At least one Team Member is required  when &quot;Define Milestone Review Completed&quot; is checked.</description>
        <errorConditionFormula>AND( NOT( $User.BypassVR__c ), CSQ_DefineMilestoneReviewCompleted__c = True,  CSQ_NumberOfProjectTeamMembers__c = 0 )</errorConditionFormula>
