        <active>true</active>
        <description>Q3 2016 Release : Please add an End Date which is post the corresponding Start Date.</description>
        <errorConditionFormula>AND( NOT( $User.BypassVR__c ) , 
OR(  CSQ_AnalysisEndDate__c &lt; CSQ_AnalysisStartDate__c ,  CSQ_ControlEndDate__c &lt; CSQ_ControlStartDate__c , CSQ_DefineEndDate__c &lt; CSQ_DefineStartDate__c ,  CSQ_ImproveEndDate__c &lt; CSQ_ImproveStartDate__c , CSQ_MeasureEndDate__c  &lt; CSQ_MeasureStartDate__c , CSQ_ProjectEndDate__c &lt; CSQ_ProjectStartDate__c ,    
CSQ_SetUpEndDate__c &lt;  CSQ_SetUpStartDate__c , CSQ_ProcessEndDate__c &lt; CSQ_ProcessStartDate__c , CSQ_RootCauseEndDate__c &lt; CSQ_RootCauseStartDate__c ,
CSQ_CounterMeasureEndDate__c &lt; CSQ_CounterMeasureStartDate__c ,  
CSQ_FollowUpEndDate__c &lt; CSQ_FollowUpStartDate__c 
