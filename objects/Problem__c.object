    <validationRules>
        <fullName>PRB_AUG19_StatusCntCmpOnRCStatusNtAppRej</fullName>
        <active>true</active>
        <description>Systemic Root Cause status cannot be changed to ‘Complete’ if systemic Root Cause validation by Schneider Electric is not Approved</description>
        <errorConditionFormula>AND
(
	NOT($User.BypassVR__c),
	AND
	(
		NOT(ISPICKVAL(SystemicRootCausesValidationBySE__c , &apos;Approved&apos;)), 
		ISPICKVAL(StatusofSystemicRootCause__c,&apos;Completed&apos;)
	) ,
	$RecordType.Name==&apos;Supplier&apos;, PartnerAccount__r.ProblemtoPreventionDeployed__c = true
)</errorConditionFormula>
        <errorDisplayField>StatusofSystemicRootCause__c</errorDisplayField>
        <errorMessage>Systemic Root Cause status cannot be changed to ‘Complete’ if systemic Root Cause validation by Schneider Electric is not Approved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRB_VR01_JUL14I2P07_WhnCrrcCmpl</fullName>
        <active>true</active>
        <description>Setting the Status of Correction to &quot;Completed&quot; is only allowed if the Summary of Correction Effectiveness Checks has content.</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
OR((RecordTypeId = $Label.CLFEB14I2P01),AND((RecordTypeId = $Label.CLFEB14I2P02),( PartnerAccount__r.ProblemtoPreventionDeployed__c = true))) 
, 
ISPICKVAL( StatusOfCorrection__c ,&quot;Completed&quot;), 
Summary_of_Correction_Effectiveness_Chec__c =&apos;&apos; 
)</errorConditionFormula>
        <errorDisplayField>Summary_of_Correction_Effectiveness_Chec__c</errorDisplayField>
        <errorMessage>Setting the Status of Correction to &quot;Completed&quot; is only allowed if the Summary of Correction Effectiveness Checks has content.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRB_VR01_StatusShouldNotBeChanged</fullName>
        <active>true</active>
        <description>Status should not get modified while Problem creation</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),ISNEW(),NOT(ISPICKVAL(Status__c,&quot;New&quot;)))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Please keep the Status as &quot;New&quot; while you are reporting a New Problem</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRB_VR02_JUL14I2P07_WhnPreCmpl</fullName>
        <active>true</active>
        <description>Setting the Status of Pevention to &quot;Completed&quot; is only allowed if the Summary of Prevention Effectiveness Checks has content</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c), 
OR((RecordTypeId = $Label.CLFEB14I2P01),AND((RecordTypeId = $Label.CLFEB14I2P02),( PartnerAccount__r.ProblemtoPreventionDeployed__c = true))) 
, 
ISPICKVAL( StatusOfPrevention__c ,&quot;Completed&quot;), 
Summary_of_Prevention_Effectiveness_Chec__c =&apos;&apos; 
)</errorConditionFormula>
        <errorDisplayField>StatusOfPrevention__c</errorDisplayField>
        <errorMessage>Setting the Status of Pevention to &quot;Completed&quot; is only allowed if the Summary of Prevention Effectiveness Checks has content</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRB_VR02_ProblemLeaderRequiredonInProgre</fullName>
        <active>true</active>
        <description>Problem Leader is required when status is set to In Progress</description>
        <errorConditionFormula>AND( NOT(ISNEW()),$User.BypassVR__c =False,  $RecordType.Name !=&apos;Supplier&apos;,ISPICKVAL(Status__c, &apos;In Progress&apos;),  ISBLANK(ProblemLeader__c))</errorConditionFormula>
        <errorDisplayField>ProblemLeader__c</errorDisplayField>
        <errorMessage>By setting the Status to In Progress, you are indicating that the 8D project has started. Please specify the Problem Leader as well.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRB_VR03_StatusReasonCompulsoryOnStandBy</fullName>
        <active>true</active>
        <description>Status Reason is Mandetory when Status is Set to &apos;Stand By&apos; or Canceled</description>
        <errorConditionFormula>AND( NOT($User.BypassVR__c),OR(ISPICKVAL( Status__c,&quot;Stand By&quot;),ISPICKVAL( Status__c,&quot;Canceled&quot;)),  ISBLANK(StatusReason__c))</errorConditionFormula>
        <errorDisplayField>StatusReason__c</errorDisplayField>
        <errorMessage>Status Reason is mandatory when the Status is &quot;Stand By&quot; or &quot;Canceled&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRB_VR04_StatusCantBeStandbyonSev10</fullName>
        <active>true</active>
        <description>Status cannot be Stand by if the Severity is equal to 10</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),ISPICKVAL( Status__c ,&quot;Stand By&quot;), ISPICKVAL( Severity__c ,&quot;Safety related&quot;))</errorConditionFormula>
        <errorMessage>Status cannot be changed to &quot;Stand By&quot; for the Problem with the  Severity 10[Safety Related]</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRB_VR05_StatusCantBeNewWhenClosed</fullName>
        <active>true</active>
        <description>Status cannot  be changed to &quot;New&quot; or &quot;Stand By&quot;  for the &quot;Closed&quot; Problem</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),OR(ISPICKVAL( Status__c ,&quot;Stand By&quot;),ISPICKVAL( Status__c ,&quot;New&quot;)), ISPICKVAL(PRIORVALUE(Status__c),&quot;Closed&quot;) )</errorConditionFormula>
        <errorMessage>Status cannot  be changed to &quot;New&quot; or &quot;Stand By&quot;  for the &quot;Closed&quot; Problem</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRB_VR06_StatusCantBeChangedWhen8DStarts</fullName>
        <active>true</active>
        <description>Status cannot be set to Standby once 8D Process has started</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),OR(ISPICKVAL( Status__c ,&quot;New&quot;),ISPICKVAL( Status__c ,&quot;Stand By&quot;)), ISPICKVAL(PRIORVALUE(Status__c),&quot;In Progress&quot;) )</errorConditionFormula>
        <errorMessage>8D Process is already started. The Status of the Problem cannot be changed  to  &quot;New&quot; or &quot;Stand By&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRB_VR07_StatusCantBeCancelledOnAction</fullName>
        <active>true</active>
        <description>Status cannot be set to Canceled if Problem has atleast one Containment Action or a Root Cause or a corrective Action or a Preventive Action</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),ISPICKVAL(Status__c,&quot;Canceled&quot;),(OR((TECH_CountOfContainmentActions__c&gt;0),(TECH_CountOfCorrectiveAction__c&gt;0),(TECH_CountOfPreventiveAction__c&gt;0))))</errorConditionFormula>
        <errorMessage>In order to cancel this Problem please make sure that all the associated actions are either Cancelled or Completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRB_VR08_StatusCannotBeSetToClosed</fullName>
        <active>true</active>
        <description>Status cannot be set to Closed if Problem has atleast status of one Containment Action or a Root Cause or a corrective Action or a Preventive Action not closed</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),
OR((RecordTypeId = $Label.CLFEB14I2P01),AND((RecordTypeId = $Label.CLFEB14I2P02),( PartnerAccount__r.ProblemtoPreventionDeployed__c = true))) 
, 
        Not(ISPICKVAL(PRIORVALUE(Status__c) ,&apos;Closed&apos;)),TEXT(Status__c)=&apos;Closed&apos;, 
		NOT(ISNEW()),
        OR(
	(NOT(ISPICKVAL(StatusOfContainment__c, &quot;Completed&quot;))),
	(NOT(ISPICKVAL(StatusOfRootCauseAnalysis__c, &quot;Completed&quot;))),
	(NOT(ISPICKVAL(StatusOfCorrection__c, &quot;Completed&quot;))),
	(NOT(ISPICKVAL(StatusOfPrevention__c, &quot;Completed&quot;)))
	)
	)</errorConditionFormula>
        <errorMessage>To set the Problem status to ‘Closed’, status of Containment, Operational Root Cause Analysis, Systemic Root Cause Analysis, Correction and Prevention must be ‘Completed&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRB_VR09_StatusCantBeStandbyWhenBRE</fullName>
        <active>true</active>
        <description>Status cannot be Stand by if atleast one BRE is associated</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),
$RecordType.Name !=&apos;Supplier&apos;,
ISPICKVAL(Status__c,&quot;Stand By&quot;), 
 TECH_IsBRERelated__c)</errorConditionFormula>
        <errorMessage>Status cannot be changed to &quot;Stand By&quot; when a Business Risk is associated to the Problem</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRB_VR10_StatusCantBeChangedWhenCanceled</fullName>
        <active>true</active>
        <description>Problem Status cannot be set to New or Closed, when the Problem status is Cancelled</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),OR(ISPICKVAL( Status__c ,&quot;New&quot;),ISPICKVAL( Status__c ,&quot;Closed&quot;)), ISPICKVAL(PRIORVALUE(Status__c),&quot;Canceled&quot;) )</errorConditionFormula>
        <errorMessage>Status cannot be changed to &quot;New&quot; or &quot;Closed&quot; for the &quot;Canceled&quot; Problem</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRB_VR11_StatusShouldNotBeChangedOnStand</fullName>
