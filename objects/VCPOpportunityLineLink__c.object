    <allowInChatterGroups>false</allowInChatterGroups>
    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <deploymentStatus>Deployed</deploymentStatus>
    <description>A junction Object linking a Value chain player and an Opportunity Line</description>
    <enableActivities>false</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <enableHistory>false</enableHistory>
    <enableLicensing>false</enableLicensing>
    <enableReports>true</enableReports>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>ControlledByParent</externalSharingModel>
    <fields>
        <fullName>OpportunityLine__c</fullName>
        <externalId>false</externalId>
        <label>Opportunity Line</label>
        <lookupFilter>
            <active>true</active>
            <errorMessage>Please select the Opportunity Line belonging to the Opportunity</errorMessage>
            <filterItems>
                <field>OPP_ProductLine__c.Opportunity__c</field>
                <operation>equals</operation>
                <valueField>$Source.Opportunity__c</valueField>
            </filterItems>
            <filterItems>
                <field>OPP_ProductLine__c.LineStatus__c</field>
                <operation>notEqual</operation>
                <value>To be deleted</value>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>OPP_ProductLine__c</referenceTo>
        <relationshipLabel>VCP - Opportunity Line links</relationshipLabel>
        <relationshipName>VCPOpportunityLineLinks2</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>true</reparentableMasterDetail>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>Opportunity__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Opportunity</label>
        <referenceTo>Opportunity</referenceTo>
        <relationshipName>VCPOpportunityLinelinks3</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>TECH_UniqueVCPOpportunityLineLink__c</fullName>
        <caseSensitive>false</caseSensitive>
        <externalId>false</externalId>
        <label>TECH_Unique VCP - Opportunity Line link</label>
        <length>30</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>VCPAmount__c</fullName>
        <externalId>false</externalId>
        <formula>VCPUnitAmount__c  *  VCPQuantity__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>There is no automation between the “Amount” and “Quantity” fields from VCP object and the “Line Amount” from Opportunity Line Object, thus no impact into Opportunity auto-calculated “Amount” field.</inlineHelpText>
        <label>VCP- Line Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>VCPOppLineExternalID__c</fullName>
        <caseSensitive>false</caseSensitive>
        <description>Used to populate the VCP Opportunity Line External reference that we receive from a quoting system</description>
        <externalId>true</externalId>
        <label>VCP - OppLine External ID</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>VCPQuantity__c</fullName>
        <externalId>false</externalId>
        <label>VCP Quantity</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>VCPUnitAmount__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>There is no automation between the “Amount” and “Quantity” fields from VCP object and the “Line Amount” from Opportunity Line Object, thus no impact into Opportunity auto-calculated “Amount” field.</inlineHelpText>
        <label>VCP- Unit Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>ValueChainPlayer__c</fullName>
        <externalId>false</externalId>
        <label>Value Chain Player</label>
        <lookupFilter>
            <active>true</active>
            <errorMessage>Please select Value Chain Player  belonging to the Opportunity</errorMessage>
            <filterItems>
                <field>OPP_ValueChainPlayers__c.OpportunityName__c</field>
                <operation>equals</operation>
                <valueField>$Source.Opportunity__c</valueField>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>OPP_ValueChainPlayers__c</referenceTo>
        <relationshipName>VCPOpportunityLinelinks</relationshipName>
        <relationshipOrder>1</relationshipOrder>
        <reparentableMasterDetail>true</reparentableMasterDetail>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>Winner__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <label>Winner</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>VCP - Opportunity Line link</label>
    <nameField>
        <displayFormat>VCP-OPLink-{0000}</displayFormat>
        <label>VCP – Opportunity Line link ID</label>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>VCP - Opportunity Line links</pluralLabel>
    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>SLS_VR_OCT14SLS01_NonNegativeValue</fullName>
        <active>true</active>
        <description>Display an error message when the user enters zero or a negative value in the VCP Unit Amount and / or VCP Quantity</description>
        <errorConditionFormula>NOT( $User.BypassVR__c ) 
&amp;&amp; 
OR( VCPUnitAmount__c  &lt;  0,  VCPQuantity__c  &lt;  0)</errorConditionFormula>
        <errorMessage>Please enter a valid number for Amount and / or Quantity</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
    <webLinks>
        <fullName>AddVCPOLineLink</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <encodingKey>UTF-8</encodingKey>
        <linkType>url</linkType>
        <masterLabel>Add VCP – OL Link</masterLabel>
        <openType>replace</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>/a9Q/e?CF00NA000000ANPyE={!OPP_ValueChainPlayers__c.Name}&amp;CF00NA000000ANPyE_lkid={!OPP_ValueChainPlayers__c.Id}&amp;CF00NA000000ANQ6J={!OPP_ValueChainPlayers__c.OpportunityName__c}&amp;CF00NA000000ANQ6J_lkid={!OPP_ValueChainPlayers__c.OpportunityNameId__c}&amp;saveURL=%2F{!OPP_ValueChainPlayers__c.Id}&amp;retURL=%2F{!OPP_ValueChainPlayers__c.Id}%2Fd</url>
    </webLinks>
