    <validationRules>
        <fullName>SVMXC__Expertise_Start_End_Date</fullName>
        <active>true</active>
        <description>Expertise should have valid start and end dates</description>
        <errorConditionFormula>SVMXC__Availability_End_Date__c &lt;  SVMXC__Availability_Start_Date__c</errorConditionFormula>
        <errorMessage>End date should be greater than or equal to Start date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SkillLevelValidation</fullName>
        <active>true</active>
        <description>The Skill Level of the expertise cannot be higher than the Skill Level - High on Skill</description>
        <errorConditionFormula>NOT( $User.BypassVR__c )
&amp;&amp;  SVMXC__Skill_Level__c &gt;  SVMXC__Skill__r.SVMXC__Skill_Level_High__c</errorConditionFormula>
        <errorDisplayField>SVMXC__Skill_Level__c</errorDisplayField>
        <errorMessage>The skill level of the expertise cannot be higher than the highest level in the skill.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Skil_Jul2018_CREDRestrictionsforDA</fullName>
        <active>false</active>
        <description>Throw validation when DA tries to create/edit/delete  skill record</description>
        <errorConditionFormula>AND(NOT($User.BypassVR__c),$User.ProfileId =$Label.CLJULY16SRV02, $User.UploadCertified__c=False)</errorConditionFormula>
        <errorMessage>Local Admin&apos;s without DUT access cannot Create/Edit/Delete Skill record</errorMessage>
    </validationRules>
