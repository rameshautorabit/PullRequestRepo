    <validationRules>
        <fullName>FSBAP_VR_OCT19I2P_ProductORFamilyRequire</fullName>
        <active>true</active>
        <description>Product or family is required to create FSB Affected product record</description>
        <errorConditionFormula>AND( OR(ISNULL(Product__c),ISBLANK(Product__c)) , OR(ISNULL(Family__c ),ISBLANK(Family__c )))</errorConditionFormula>
        <errorMessage>Please enter Product or Family</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FSB_VR_RestrictFamilyOrderedBlank</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT($User.BypassVR__c), AND(ISCHANGED( Family__c ), Product__c != &apos;&apos;, !ISCHANGED(Product__c)))</errorConditionFormula>
        <errorDisplayField>Family__c</errorDisplayField>
        <errorMessage>You cannot modify the Family when Product already exists</errorMessage>
    </validationRules>
