AND(ISPICKVAL(OtherTemporaryActions__c, &quot;Required&quot;),NOT(ISPICKVAL( OtherTemporaryActionsStatus__c , &quot;Discontinued&quot;))),
AND(ISPICKVAL(OtherTemporaryActions__c, &quot;Discontinue!&quot;),NOT(ISPICKVAL( OtherTemporaryActionsStatus__c , &quot;Discontinued&quot;))),  
AND(ISPICKVAL(StockSortQuarantine__c, &quot;Required&quot;),NOT(ISPICKVAL( StockSortQuarantineStatus__c , &quot;Completed&quot;))), 
