    <validationRules>
        <fullName>CNA_VR01_072016_FSBProblemCheck</fullName>
        <active>true</active>
        <description>July 2016 Release : The Problem linked to the selected FSB and the Problem linked to this Containment Action are different. Please select the appropriate FSB.</description>
        <errorConditionFormula>AND(NOT( $User.BypassVR__c ) , NOT(ISBLANK(FieldServiceBulletin__c)) , IF( RelatedProblem__c ==  FieldServiceBulletin__r.Problem__c , False , True))</errorConditionFormula>
        <errorDisplayField>FieldServiceBulletin__c</errorDisplayField>
        <errorMessage>The Problem linked to the selected FSB and the Problem linked to this Containment Action are different. Please select the appropriate FSB.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CNA_VR01_OwnerFieldMandatory</fullName>
        <active>true</active>
        <description>Owner field is mandatory for Containment Action</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c), $RecordType.Name !=$Label.CLMAY13I2P09 ,
ISBLANK( Owner__c) )</errorConditionFormula>
        <errorDisplayField>Owner__c</errorDisplayField>
        <errorMessage>The corresponding Owner for the Containment Action is not found. Owner must be associated to a Containment Action</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CNA_VR02_NotStartedOnlyAllowedWhenIsNew</fullName>
        <active>true</active>
        <description>The only status allowed when creating an action is &quot;Not Started&quot;</description>
        <errorConditionFormula>AND($User.BypassVR__c = False,  NOT( ISPICKVAL( Status__c , &apos;Not Started&apos;) ), ISNEW() , RecordType.DeveloperName !=&apos;SupplierContainment&apos;)</errorConditionFormula>
        <errorMessage>Containment Actions can only be created in &quot;Not Started&quot; status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CNA_VR22_Customer_Notification</fullName>
        <active>true</active>
        <description>I2P: APR2015 RELEASE :  The new values entered in &quot;Customer Notification&quot; section must be higher than the prior values.</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c), 

((PRIORVALUE( AffectedSEAccountsN1__c ) &gt; AffectedSEAccountsN1__c) || 

(PRIORVALUE( AffectedNonSEAccountsN2__c ) &gt; AffectedNonSEAccountsN2__c) ||

(PRIORVALUE(  NotifiedSEAccounts__c  ) &gt; NotifiedSEAccounts__c) ||

(PRIORVALUE(  NotifiedNonSEAccounts__c  ) &gt; NotifiedNonSEAccounts__c) ||

(PRIORVALUE( SEAccountsResponded__c  ) &gt; SEAccountsResponded__c) ||

(PRIORVALUE( NonSEAccountsResponded__c  ) &gt; NonSEAccountsResponded__c) ||

(PRIORVALUE( FinalNoticeSenttoSEAccounts__c  ) &gt; FinalNoticeSenttoSEAccounts__c) ||

(PRIORVALUE( FinalNoticeSenttoNonSEAccounts__c  ) &gt; FinalNoticeSenttoNonSEAccounts__c)) ,

ISPICKVAL(StatusofValidation__c,&apos;Validated&apos;) )</errorConditionFormula>
        <errorMessage>The new values entered in &quot;Customer Notification&quot; section must be higher than the prior values.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CNA_VR23_Customer_Recovery</fullName>
        <active>true</active>
        <description>I2P: APR2015 RELEASE : The new values entered in &quot;Customer Recovery&quot; section must be higher than the prior values.</description>
        <errorConditionFormula>AND (NOT($User.BypassVR__c), 

((PRIORVALUE( AffectedEndUsersR1__c  ) &gt; AffectedEndUsersR1__c) || 

(PRIORVALUE( AffectedNonEndUsersR2__c ) &gt; AffectedNonEndUsersR2__c) || 

(PRIORVALUE( RecoveredEndUsers__c ) &gt; 
