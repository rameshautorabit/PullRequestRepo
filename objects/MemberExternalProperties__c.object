    <validationRules>
        <fullName>Either_ExternalID_or_Subtype</fullName>
        <active>true</active>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp; (ExternalPropertiesCatalog__r.SubType__c + ExternalId__c ==null)</errorConditionFormula>
        <errorDisplayField>ExternalId__c</errorDisplayField>
        <errorMessage>External Id and ExternalPropertiesCatalog__r.SubType cannot be empty at the same time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FunctionalKey_formula_validation</fullName>
        <active>true</active>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp; NOT(IF(ExternalPropertiesCatalog__r.RecordType.DeveloperName= &apos;Internal_Activities&apos;, TRUE, FALSE))&amp;&amp; (FunctionalKey__c &lt;&gt; IF( ExternalPropertiesCatalog__r.RecordType.DeveloperName= &apos;BFOProperties&apos;,&apos;BFOProperties&apos;,IF(ExternalPropertiesCatalog__r.RecordType.DeveloperName= &apos;Internal_Activities&apos;,&apos;BFOProperties&apos;,ExternalPropertiesCatalog__r.ExternalPropertiesCatalogType__r.Source__c +ExternalPropertiesCatalog__r.ExternalPropertiesCatalogType__r.Type__c)) + IF(ExternalPropertiesCatalog__r.SubType__c&lt;&gt;null,ExternalPropertiesCatalog__r.SubType__c, ExternalId__c )+ PRMUIMSId__c)
 &amp;&amp; ExternalPropertiesCatalog__r.ExternalPropertiesCatalogType__r.Type__c != &apos;Partner referral&apos;</errorConditionFormula>
        <errorDisplayField>FunctionalKey__c</errorDisplayField>
        <errorMessage>FunctionalKey must be = Source + Type + (Subtype OR ExternalID) + PRMUIMSId</errorMessage>
    </validationRules>
