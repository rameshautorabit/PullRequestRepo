    <webLinks>
        <fullName>CRP_New</fullName>
        <availability>online</availability>
        <description>Custom Button for Console View</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>New Case Related Product</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/support/console/36.0/integration.js&quot;)} 
if (sforce.console.isInConsole()){
	sforce.console.getEnclosingTabId(function(resultgetEnclosingTabId){
		if(resultgetEnclosingTabId.success){
			sforce.console.getEnclosingPrimaryTabId(function(resultgetEnclosingPrimaryTabId){
				if(resultgetEnclosingPrimaryTabId.success){
					sforce.console.openSubtab(resultgetEnclosingPrimaryTabId.id,&apos;/apex/VFP_NewRelatedProductForCase?isdtp=nv&amp;CF00NA000000B0pQc={!Case.CaseNumber}&amp;CF00NA000000B0pQc_lkid={!Case.Id}&apos;,true,&apos;{!Case.CaseNumber}&apos;,resultgetEnclosingTabId.id);
				}
			});
		}
	});
}
else{
	window.open(&apos;/apex/VFP_NewRelatedProductForCase?CF00NA000000B0pQc={!Case.CaseNumber}&amp;CF00NA000000B0pQc_lkid={!Case.Id}&apos;);
}</url>
    </webLinks>
