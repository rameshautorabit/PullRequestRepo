    <validationRules>
        <fullName>LID__Check_CompanyId_Value</fullName>
        <active>true</active>
        <description>Throws an exception message if the company id contains non-numerics</description>
        <errorConditionFormula>AND(
	(LID__LinkedIn_Company_Id__c &lt;&gt; NULL),
	NOT(ISNUMBER(LID__LinkedIn_Company_Id__c))
)</errorConditionFormula>
        <errorDisplayField>LID__LinkedIn_Company_Id__c</errorDisplayField>
        <errorMessage>CompanyId should be a numeric value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP_SLS2017R6_NoCrtnAtStg5ForSoln</fullName>
        <active>true</active>
        <description>Opportunity Simplification - A solutions Opportunity cannot be created at Stage 5</description>
        <errorConditionFormula>AND( NOT( $User.BypassVR__c ) ,AND(ISNEW(), ISPICKVAL(OpptyType__c,&apos;Solutions&apos;), OR(ISPICKVAL(StageName ,&apos;5 - Prepare &amp; Bid&apos;),ISPICKVAL(StageName ,&apos;6 - Negotiate &amp; Win&apos;),ISPICKVAL(StageName ,&apos;7 - Deliver &amp; Validate&apos;))))</errorConditionFormula>
        <errorMessage>A solutions Opportunity cannot be created at Stage 5 and above.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP_SLS2017R6_NoCrtnAtStg6ForSTD</fullName>
        <active>true</active>
        <description>September Release - To prevent  a Solutions Opportunity from creation at Stage 6.</description>
        <errorConditionFormula>AND( NOT( $User.BypassVR__c ) ,AND(ISNEW(), OR(ISPICKVAL(OpptyType__c,&apos;Standard&apos;),ISPICKVAL(OpptyType__c,&apos;	Solutions Fast-Track&apos;)), OR(ISPICKVAL(StageName ,&apos;6 - Negotiate &amp; Win&apos;),ISPICKVAL(StageName ,&apos;7 - Deliver &amp; Validate&apos;))))</errorConditionFormula>
        <errorMessage>A Standard or 	Solutions Fast-Track Opportunity cannot be created at Stage 6 and above.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP_SLS2017R6_SOL3_4RequiredFields</fullName>
        <active>true</active>
        <errorConditionFormula>AND(TECH_TriggerVR__c ,
 NOT(ISCHANGED(TECH_TriggerVR__c)),
